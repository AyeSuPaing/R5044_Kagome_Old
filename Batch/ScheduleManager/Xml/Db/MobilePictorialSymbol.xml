<MobilePictorialSymbol>

  <!-- モバイル絵文字コード取得 -->
  <GetMobilePictorialSymbolCode>
    <Statement>
      <![CDATA[
        SELECT  *
          FROM  w2_MobilePictorialSymbol
         WHERE  dept_id = @dept_id
           AND  symbol_tag IN (@@ symbol_tags @@)
			]]>
    </Statement>
    <Parameter>
      <Input Name="dept_id" Type="nvarchar" Size="30" />
    </Parameter>
  </GetMobilePictorialSymbolCode>

</MobilePictorialSymbol>