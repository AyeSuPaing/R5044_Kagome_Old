<YahooOrderImport>

  <!-- 注文情報取得 -->
  <GetOrder>
    <Statement>
      <![CDATA[
        SELECT  *
          FROM  w2_Order
         WHERE  order_id = @order_id
			]]>
    </Statement>
    <Parameter>
      <Input Name="order_id" Type="nvarchar" Size="30" />
    </Parameter>
  </GetOrder>
  
</YahooOrderImport>