<AdConv>

  <!-- 商品コンバータマスタ取得 -->
  <GetAdConv>
    <Statement>
      <![CDATA[
        SELECT  convertFrom,
                convertTo,
                target
          FROM  w2_MallPrdcnvRule
         WHERE  adto_id = @adto_id
		  ]]>
    </Statement>
    <Parameter>
      <Input Name="adto_id" Type="int" />
    </Parameter>
  </GetAdConv>
  
</AdConv>