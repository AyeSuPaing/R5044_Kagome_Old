<AdColumns>

  <!-- カラム名取得 -->
  <GetColumnName>
    <Statement>
      <![CDATA[
        SELECT  column_name,
                output_format,
                adcolumn_id
          FROM  w2_MallPrdcnvColumns
         WHERE  adto_id = @adto_id
        ORDER BY  column_no
		  ]]>
    </Statement>
    <Parameter>
      <Input Name="adto_id" Type="int" />
    </Parameter>
  </GetColumnName>
  
</AdColumns>