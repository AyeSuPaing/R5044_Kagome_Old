<AdFiles>

  <!-- 商品コンバータファイルマスタ登録 -->
  <InsertItAdFiles>
    <Statement>
      <![CDATA[
        EXEC InsertItAdFiles @adto_id, @path
		  ]]>
    </Statement>
    <Parameter>
      <Input Name="adto_id" Type="int" />
      <Input Name="path" Type="nvarchar" Size="260" />
    </Parameter>
  </InsertItAdFiles>
  
  <!-- 商品コンバータファイルマスタ削除 -->
  <DeleteItAdFiles>
    <Statement>
      <![CDATA[
        DELETE  w2_MallPrdcnvFiles
         WHERE  adto_id = @adto_id
           AND  path = @path
        ]]>
    </Statement>
    <Parameter>
      <Input Name="adto_id" Type="int" />
      <Input Name="path" Type="nvarchar" Size="260" />
    </Parameter>
  </DeleteItAdFiles>

</AdFiles>