<FixedPurchaseItemInsertUpdate>
  <Column name="fixed_purchase_id">
    <name>定期購入ID</name>
    <necessary>1</necessary>
    <length_max>30</length_max>
  </Column>
  <Column name="fixed_purchase_item_no">
    <name>定期購入注文商品枝番</name>
    <necessary>1</necessary>
    <type>HALFWIDTH_NUMBER</type>
  </Column>
  <Column name="fixed_purchase_shipping_no">
    <name>定期購入配送先枝番</name>
    <necessary>1</necessary>
    <type>HALFWIDTH_NUMBER</type>
  </Column>
  <Column name="shop_id">
    <name>店舗ID</name>
    <length_max>10</length_max>
  </Column>
  <Column name="product_id">
    <name>@@ 1 @@商品ID</name>
    <necessary>1</necessary>
    <type>HALFWIDTH_ALPHNUMSYMBOL</type>
    <length_max>30</length_max>
  </Column>
  <Column name="variation_id">
    <name>@@ 1 @@バリエーションID</name>
    <necessary>1</necessary>
    <type>HALFWIDTH_ALPHNUMSYMBOL</type>
    <length_max>60</length_max>
  </Column>
  <Column name="supplier_id">
    <name>サプライヤID</name>
    <length_max>20</length_max>
  </Column>
  <Column name="item_quantity">
    <name>@@ 1 @@数量</name>
    <necessary>1</necessary>
    <type>HALFWIDTH_NUMBER</type>
    <number_min>1</number_min>
  </Column>
  <Column name="item_quantity_single">
    <name>注文数（セット未考慮）</name>
    <type>HALFWIDTH_NUMBER</type>
  </Column>
  <Column name="product_set_id">
    <name>商品セットID</name>
    <length_max>30</length_max>
  </Column>
  <Column name="product_set_no">
    <name>商品セット枝番</name>
    <type>HALFWIDTH_NUMBER</type>
  </Column>
  <Column name="product_set_count">
    <name>商品セット注文数</name>
    <type>HALFWIDTH_NUMBER</type>
  </Column>
  <Column name="item_shipped_count">
    <name>@@ 1 @@商品購入回数(出荷基準)</name>
    <necessary>1</necessary>
    <type>HALFWIDTH_NUMBER</type>
    <number_min>0</number_min>
  </Column>
  <Column name="item_order_count">
    <name>@@ 1 @@商品購入回数(注文基準)</name>
    <necessary>1</necessary>
    <type>HALFWIDTH_NUMBER</type>
    <number_min>0</number_min>
  </Column>
  <Column name="date_created">
    <name>作成日</name>
    <type>DATE</type>
  </Column>
  <Column name="date_changed">
    <name>更新日</name>
    <type>DATE</type>
  </Column>
</FixedPurchaseItemInsertUpdate>