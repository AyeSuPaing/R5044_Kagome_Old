<ProductPriceDelete>
  <Column name="shop_id">
    <name>店舗ID</name>
    <necessary>1</necessary>
    <type></type>
    <length></length>
    <length_max>10</length_max>
    <length_min></length_min>
    <regexp ptn="「0」固定">0</regexp>
  </Column>
  <Column name="product_id">
    <name>商品ID</name>
    <necessary>1</necessary>
    <type></type>
    <length></length>
    <length_max>30</length_max>
    <length_min></length_min>
    <regexp ptn="半角英数字、「_」、「-」、「(」、「)」、「#」">^[a-zA-Z0-9_\-\(\)#]+$</regexp>
  </Column>
  <Column name="variation_id">
    <name>商品バリエーションID</name>
    <necessary></necessary>
    <type></type>
    <length></length>
    <length_max>60</length_max>
    <length_min></length_min>
    <regexp ptn="半角英数字、「_」、「-」、「(」、「)」、「#」">^[a-zA-Z0-9_\-\(\)#]+$</regexp>
  </Column>
  <Column name="member_rank_id">
    <name>会員ランクID</name>
    <necessary>1</necessary>
    <type></type>
    <length></length>
    <length_max>30</length_max>
    <length_min></length_min>
    <regexp ptn="半角英数字、「_」、「-」、「(」、「)」、「#」">^[a-zA-Z0-9_\-\(\)#]+$</regexp>
  </Column>
</ProductPriceDelete>
