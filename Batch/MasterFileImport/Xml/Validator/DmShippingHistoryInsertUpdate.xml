<DmShippingHistoryInsertUpdate>
  <Column name="user_id">
    <name>ユーザーID</name>
    <necessary>1</necessary>
    <type>HALFWIDTH_ALPHNUM</type>
    <length_max>30</length_max>
  </Column>
  <Column name="dm_code">
    <name>DMコード</name>
    <necessary>1</necessary>
    <type>HALFWIDTH_ALPHNUMSYMBOL</type>
    <length_max>30</length_max>
  </Column>
  <Column name="dm_shipping_date">
    <name>DM発送日</name>
    <necessary>1</necessary>
    <type>HALFWIDTH_DATE</type>
  </Column>
  <Column name="dm_name">
    <name>DM名</name>
    <length_max>50</length_max>
  </Column>
  <Column name="valid_date_from">
    <name>有効期間(From)</name>
    <type>HALFWIDTH_DATE</type>
  </Column>
  <Column name="valid_date_to">
    <name>有効期間(To)</name>
    <type>HALFWIDTH_DATE</type>
  </Column>
</DmShippingHistoryInsertUpdate>