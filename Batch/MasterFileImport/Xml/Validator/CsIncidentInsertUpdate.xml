<CsIncidentInsertUpdate>
  <Column name="dept_id">
    <name>識別ID</name>
    <necessary>1</necessary>
    <regexp ptn="「0」固定">0</regexp>
  </Column>
  <Column name="incident_id">
    <name>インシデントID</name>
    <necessary>1</necessary>
    <length_max>30</length_max>
  </Column>
  <Column name="user_id">
    <name>ユーザーID</name>
    <necessary>1</necessary>
    <type>HALFWIDTH_ALPHNUM</type>
    <length_max>30</length_max>
  </Column>
  <Column name="incident_category_id">
    <name>インシデントカテゴリID</name>
    <length_max>30</length_max>
  </Column>
  <Column name="incident_title">
    <name>タイトル</name>
    <necessary>1</necessary>
    <length_max>50</length_max>
  </Column>
  <Column name="status">
    <name>ステータス</name>
    <necessary>1</necessary>
    <length_max>10</length_max>
  </Column>
  <Column name="voc_id">
    <name>VOC</name>
    <length_max>10</length_max>
  </Column>
  <Column name="voc_memo">
    <name>VOCメモ</name>
  </Column>
  <Column name="comment">
    <name>内部メモ</name>
  </Column>
  <Column name="importance">
    <name>重要度</name>
    <necessary>1</necessary>
    <length_max>10</length_max>
  </Column>
  <Column name="user_name">
    <name>問合せ元名称</name>
    <length_max>100</length_max>
  </Column>
  <Column name="user_contact">
    <name>問合せ元連絡先</name>
    <length_max>256</length_max>
  </Column>
  <Column name="cs_group_id">
    <name>担当グループ</name>
    <length_max>10</length_max>
  </Column>
  <Column name="operator_id">
    <name>担当オペレータ</name>
    <length_max>20</length_max>
  </Column>
  <Column name="lock_status">
    <name>ロック状態</name>
    <length_max>30</length_max>
  </Column>
  <Column name="lock_operator_id">
    <name>ロック保持者</name>
    <length_max>20</length_max>
  </Column>
  <Column name="valid_flg">
    <name>有効フラグ</name>
    <length_max>10</length_max>
  </Column>
  <Column name="date_last_received">
    <name>最終受付日時</name>
    <type>DATE</type>
  </Column>
  <Column name="date_message_last_send_received">
    <name>最終送受信日時</name>
    <type>DATE</type>
  </Column>
  <Column name="date_completed">
    <name>対応完了日</name>
    <type>DATE</type>
  </Column>
  <Column name="date_created">
    <name>作成日</name>
    <type>DATE</type>
  </Column>
  <Column name="date_changed">
    <name>更新日</name>
    <type>DATE</type>
  </Column>
</CsIncidentInsertUpdate>