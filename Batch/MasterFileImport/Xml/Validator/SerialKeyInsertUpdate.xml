<SerialKeyInsertUpdate>
  <Column name="serial_key">
    <name>シリアルキー</name>
    <necessary>1</necessary>
    <type>HALFWIDTH_ALPHNUMSYMBOL</type>
    <length></length>
    <length_max>60</length_max>
    <length_min></length_min>
  </Column>
  <Column name="product_id">
    <name>商品ID</name>
    <necessary>1</necessary>
    <type></type>
    <length></length>
    <length_max>30</length_max>
    <length_min></length_min>
    <regexp ptn="半角英数字、「_」、「-」、「(」、「)」、「#」">^[a-zA-Z0-9_\-\(\)#]+$</regexp>
  </Column>
  <Column name="variation_id">
    <name>商品バリエーションID</name>
    <necessary>1</necessary>
    <type></type>
    <length></length>
    <length_max>60</length_max>
    <length_min></length_min>
    <regexp ptn="半角英数字、「_」、「-」、「(」、「)」、「#」">^[a-zA-Z0-9_\-\(\)#]+$</regexp>
  </Column>
  <Column name="order_id">
    <name>注文ID</name>
    <necessary></necessary>
    <type></type>
    <length></length>
    <length_max>60</length_max>
    <length_min></length_min>
    <regexp ptn="半角英数字、「_」、「-」、「(」、「)」、「#」">^[a-zA-Z0-9_\-\(\)#]+$</regexp>
  </Column>
  <Column name="order_item_no">
    <name>注文商品枝番</name>
    <necessary></necessary>
    <type>HALFWIDTH_NUMBER</type>
    <length></length>
    <length_max></length_max>
    <length_min></length_min>
  </Column>
  <Column name="user_id">
    <name>ユーザーID</name>
    <necessary></necessary>
    <type>HALFWIDTH_ALPHNUM</type>
    <length></length>
    <length_max>30</length_max>
    <length_min></length_min>
  </Column>
  <Column name="status">
    <name>状態</name>
    <necessary>1</necessary>
    <type></type>
    <length></length>
    <length_max></length_max>
    <length_min></length_min>
  </Column>
  <Column name="valid_flg">
    <name>有効フラグ</name>
    <necessary></necessary>
    <type>HALFWIDTH_NUMBER</type>
    <length></length>
    <length_max></length_max>
    <length_min></length_min>
  </Column>
  <Column name="date_delivered">
    <name>発行日</name>
    <necessary></necessary>
    <type></type>
    <length></length>
    <length_max></length_max>
    <length_min></length_min>
  </Column>
  <Column name="download_count">
    <name>回数</name>
    <necessary></necessary>
    <type></type>
    <length></length>
    <length_max></length_max>
    <length_min></length_min>
  </Column>
</SerialKeyInsertUpdate>