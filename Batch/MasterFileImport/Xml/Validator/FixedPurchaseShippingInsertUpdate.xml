<FixedPurchaseShippingInsertUpdate>
  <Column name="fixed_purchase_id">
    <name>定期購入ID</name>
    <necessary>1</necessary>
    <length_max>30</length_max>
  </Column>
  <Column name="fixed_purchase_shipping_no">
    <name>定期購入配送先枝番</name>
    <necessary>1</necessary>
    <type>HALFWIDTH_NUMBER</type>
  </Column>
  <Column name="shipping_name">
    <name>配送先氏名</name>
    <length_max>40</length_max>
  </Column>
  <Column name="shipping_name_kana">
    <name>配送先氏名かな</name>
    <length_max>60</length_max>
  </Column>
  <Column name="shipping_zip">
    <name>郵便番号</name>
    <length_max>8</length_max>
  </Column>
  <Column name="shipping_addr1">
    <name>配送先 都道府県</name>
    <necessary>1</necessary>
    <type>@@User.addr1.type@@</type>
  </Column>
  <Column name="shipping_addr2">
    <name>配送先 @@User.addr2.name@@</name>
    <necessary>@@User.addr2.necessary@@</necessary>
    <type>@@User.addr2.type@@</type>
    <length_max>@@User.addr2.length_max@@</length_max>
  </Column>
  <Column name="shipping_addr3">
    <name>配送先 @@User.addr3.name@@</name>
    <necessary>@@User.addr3.necessary@@</necessary>
    <type>@@User.addr3.type@@</type>
    <length_max>@@User.addr3.length_max@@</length_max>
  </Column>
  <Column name="shipping_addr4">
    <name>配送先 @@User.addr4.name@@</name>
    <necessary>@@User.addr4.necessary@@</necessary>
    <type>@@User.addr4.type@@</type>
    <length_max>@@User.addr4.length_max@@</length_max>
  </Column>
  <Column name="shipping_tel1">
    <name>電話番号1</name>
    <length_max>16</length_max>
  </Column>
  <Column name="shipping_tel2">
    <name>電話番号2</name>
    <length_max>16</length_max>
  </Column>
  <Column name="shipping_tel3">
    <name>電話番号3</name>
    <length_max>16</length_max>
  </Column>
  <Column name="shipping_fax">
    <name>ＦＡＸ</name>
    <length_max>16</length_max>
  </Column>
  <Column name="shipping_company">
    <name>配送業者</name>
    <length_max>30</length_max>
  </Column>
  <Column name="shipping_date">
    <name>配送希望日</name>
    <type>DATE</type>
  </Column>
  <Column name="shipping_time">
    <name>配送先 配送希望時間帯</name>
    <type>HALFWIDTH_ALPHNUM</type>
  </Column>
  <Column name="del_flg">
    <name>削除フラグ</name>
    <type>HALFWIDTH_NUMBER</type>
    <length>1</length>
  </Column>
  <Column name="shipping_name1">
    <name>配送先 @@User.name1.name@@</name>
    <type>@@User.name1.type@@</type>
    <length_max>@@User.name1.length_max@@</length_max>
  </Column>
  <Column name="shipping_name2">
    <name>配送先 @@User.name2.name@@</name>
    <necessary>1</necessary>
    <type>@@User.name2.type@@</type>
    <length_max>@@User.name2.length_max@@</length_max>
  </Column>
  <Column name="shipping_name_kana1">
    <name>配送先 @@User.name_kana1.name@@</name>
    <necessary>1</necessary>
    <type>@@User.name_kana1.type@@</type>
    <length_max>@@User.name_kana1.length_max@@</length_max>
  </Column>
  <Column name="shipping_name_kana2">
    <name>配送先 @@User.name_kana2.name@@</name>
    <necessary>1</necessary>
    <type>@@User.name_kana2.type@@</type>
    <length_max>@@User.name_kana2.length_max@@</length_max>
  </Column>
  <Column name="shipping_company_name">
    <name>配送先 @@User.company_name.name@@</name>
    <necessary>@@User.company_name.necessary@@</necessary>
    <type>@@User.company_name.type@@</type>
    <length_max>@@User.company_name.length_max@@</length_max>
  </Column>
  <Column name="shipping_company_post_name">
    <name>配送先 @@User.company_post_name.name@@</name>
    <necessary>@@User.company_post_name.necessary@@</necessary>
    <type>@@User.company_post_name.type@@</type>
    <length_max>@@User.company_post_name.length_max@@</length_max>
  </Column>
  <Column name="shipping_method">
    <name>配送方法</name>
    <length_max>10</length_max>
  </Column>
  <Column name="delivery_company_id">
    <name>配送会社ID</name>
    <length_max>10</length_max>
  </Column>
  <Column name="shipping_country_iso_code">
    <name>住所国ISOコード</name>
    <length_max>3</length_max>
  </Column>
  <Column name="shipping_country_name">
    <name>住所国名</name>
    <length_max>100</length_max>
  </Column>
  <Column name="shipping_addr5">
    <name>住所5</name>
    <length_max>50</length_max>
  </Column>
  <Column name="shipping_receiving_store_flg">
    <name>店舗受取フラグ</name>
    <type>HALFWIDTH_NUMBER</type>
    <length>1</length>
  </Column>
  <Column name="shipping_receiving_store_id">
    <name>店舗受取店舗ID</name>
    <length_max>20</length_max>
  </Column>
  <Column name="shipping_receiving_store_type">
    <name>コンビニ受取：受取方法</name>
    <length_max>5</length_max>
  </Column>
  <Column name="date_created">
    <name>作成日</name>
    <type>DATE</type>
  </Column>
  <Column name="date_changed">
    <name>更新日</name>
    <type>DATE</type>
  </Column>
</FixedPurchaseShippingInsertUpdate>