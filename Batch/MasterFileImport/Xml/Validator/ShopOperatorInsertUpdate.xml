<ShopOperatorInsertUpdate>
  <Column name="shop_id">
    <name>店舗ID</name>
    <necessary>1</necessary>
    <type>HALFWIDTH_ALPHNUM</type>
    <length_max>10</length_max>
  </Column>
  <Column name="operator_id">
    <name>オペレータID</name>
    <necessary>1</necessary>
    <type>HALFWIDTH_NUMBER</type>
    <length_max>20</length_max>
  </Column>
  <Column name="name">
    <name>オペレータ名</name>
    <necessary>1</necessary>
    <length_max>20</length_max>
  </Column>
  <Column name="mail_addr">
    <name>メールアドレス</name>
    <necessary>1</necessary>
    <type>MAILADDRESS</type>
    <length_max>256</length_max>
  </Column>
  <Column name="menu_access_level1">
    <name>メニューアクセスレベル1</name>
    <type>HALFWIDTH_NUMBER</type>
  </Column>
  <Column name="login_id">
    <name>ログインID</name>
    <necessary>1</necessary>
    <type>HALFWIDTH_ALPHNUMSYMBOL</type>
    <length_max>20</length_max>
  </Column>
  <Column name="password">
    <name>パスワード</name>
    <necessary>1</necessary>
    <type>HALFWIDTH_ALPHNUMSYMBOL</type>
    <length_max>20</length_max>
  </Column>
  <Column name="valid_flg">
    <name>有効フラグ</name>
    <necessary>1</necessary>
    <length_max>1</length_max>
  </Column>
  <Column name="date_created">
    <name>作成日</name>
    <necessary>1</necessary>
    <type>DATE</type>
  </Column>
</ShopOperatorInsertUpdate>
