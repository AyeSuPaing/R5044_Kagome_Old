<RealShopInsertUpdate>
  <Column name="real_shop_id">
    <name>リアル店舗ID</name>
    <necessary>1</necessary>
    <type></type>
    <length></length>
    <length_max>30</length_max>
    <length_min></length_min>
  </Column>
  <Column name="name">
    <name>店舗名</name>
    <necessary>1</necessary>
    <type></type>
    <length></length>
    <length_max>100</length_max>
    <length_min></length_min>
  </Column>
  <Column name="name_kana">
    <name>店舗名かな</name>
    <necessary></necessary>
    <type></type>
    <length></length>
    <length_max>200</length_max>
    <length_min></length_min>
  </Column>
  <Column name="zip1">
    <name>郵便番号(前)</name>
    <necessary></necessary>
    <type>HALFWIDTH_NUMBER</type>
    <length>3</length>
    <length_max></length_max>
    <length_min></length_min>
  </Column>
  <Column name="zip2">
    <name>郵便番号(後)</name>
    <necessary></necessary>
    <type>HALFWIDTH_NUMBER</type>
    <length>4</length>
    <length_max></length_max>
    <length_min></length_min>
  </Column>
  <Column name="addr1">
    <name>都道府県</name>
    <necessary></necessary>
    <type>PREFECTURE</type>
    <length></length>
    <length_max></length_max>
    <length_min></length_min>
  </Column>
  <Column name="addr2">
    <name>市区町村</name>
    <necessary></necessary>
    <type></type>
    <length></length>
    <length_max>100</length_max>
    <length_min></length_min>
  </Column>
  <Column name="addr3">
    <name>番地</name>
    <necessary></necessary>
    <type></type>
    <length></length>
    <length_max>100</length_max>
    <length_min></length_min>
  </Column>
  <Column name="addr4">
    <name>ビル・マンション名</name>
    <necessary></necessary>
    <type></type>
    <length></length>
    <length_max>100</length_max>
    <length_min></length_min>
  </Column>
  <Column name="tel_1">
    <name>電話番号(前)</name>
    <necessary></necessary>
    <type>HALFWIDTH_NUMBER</type>
    <length></length>
    <length_max>6</length_max>
    <length_min></length_min>
  </Column>
  <Column name="tel_2">
    <name>電話番号(中)</name>
    <necessary></necessary>
    <type>HALFWIDTH_NUMBER</type>
    <length></length>
    <length_max>4</length_max>
    <length_min></length_min>
  </Column>
  <Column name="tel_3">
    <name>電話番号(後)</name>
    <necessary></necessary>
    <type>HALFWIDTH_NUMBER</type>
    <length></length>
    <length_max>4</length_max>
    <length_min></length_min>
  </Column>
  <Column name="fax_1">
    <name>FAX(前)</name>
    <necessary></necessary>
    <type>HALFWIDTH_NUMBER</type>
    <length></length>
    <length_max>6</length_max>
    <length_min></length_min>
  </Column>
  <Column name="fax_2">
    <name>FAX(中)</name>
    <necessary></necessary>
    <type>HALFWIDTH_NUMBER</type>
    <length></length>
    <length_max>4</length_max>
    <length_min></length_min>
  </Column>
  <Column name="fax_3">
    <name>FAX(後)</name>
    <necessary></necessary>
    <type>HALFWIDTH_NUMBER</type>
    <length></length>
    <length_max>4</length_max>
    <length_min></length_min>
  </Column>
  <Column name="display_order">
    <name>表示順</name>
    <necessary>1</necessary>
    <type>HALFWIDTH_NUMBER</type>
    <length></length>
    <length_max></length_max>
    <length_min></length_min>
  </Column>
  <Column name="mail_addr">
    <name>メールアドレス</name>
    <necessary></necessary>
    <type>MAILADDRESS</type>
    <length></length>
    <length_max>256</length_max>
    <length_min></length_min>
  </Column>
  <Column name="url">
    <name>URL</name>
    <necessary></necessary>
    <type></type>
    <length></length>
    <length_max></length_max>
    <length_min></length_min>
    <regexp ptn=""><![CDATA[^(s?https?://|/)[-_.!~*'()a-zA-Z0-9;/?:@&=+$,%#]+$]]></regexp>
  </Column>
  <Column name="area_id">
    <name>地域</name>
    <necessary>1</necessary>
  </Column>
  <Column name="brand_id">
    <name>ブランド</name>
  </Column>
  <Column name="longitude">
    <name>経度</name>
    <type>HALFWIDTH_DECIMAL</type>
    <regexp ptn=""><![CDATA[^(\d{1,3})\.(\d{1,6})$]]></regexp>
  </Column>
  <Column name="latitude">
    <name>緯度</name>
    <type>HALFWIDTH_DECIMAL</type>
    <regexp ptn=""><![CDATA[^(\d{1,3})\.(\d{1,6})$]]></regexp>
  </Column>
</RealShopInsertUpdate>