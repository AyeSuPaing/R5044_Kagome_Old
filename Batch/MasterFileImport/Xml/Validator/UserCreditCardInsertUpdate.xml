<UserCreditCardInsertUpdate>
  <Column name="user_id">
    <name>ユーザーID</name>
    <necessary>1</necessary>
    <type>HALFWIDTH_ALPHNUM</type>
    <length_max>30</length_max>
  </Column>
  <Column name="branch_no">
    <name>カード枝番</name>
    <necessary>1</necessary>
    <type>HALFWIDTH_NUMBER</type>
  </Column>
  <Column name="cooperation_id">
    <name>連携ID</name>
    <length_max>100</length_max>
  </Column>
  <Column name="card_disp_name">
    <name>カード表示名</name>
    <length_max>100</length_max>
  </Column>
  <Column name="last_four_digit">
    <name>カード番号下４桁</name>
    <length_max>10</length_max>
    <type>HALFWIDTH_NUMBER</type>
  </Column>
  <Column name="expiration_month">
    <name>有効期限（月）</name>
    <length_max>2</length_max>
  </Column>
  <Column name="expiration_year">
    <name>有効期限（年）</name>
    <length_max>4</length_max>
  </Column>
  <Column name="author_name">
    <name>カード名義人</name>
    <length_max>50</length_max>
  </Column>
  <Column name="disp_flg">
    <name>表示フラグ</name>
    <length_max>1</length_max>
  </Column>
  <Column name="last_changed">
    <name>最終更新者</name>
    <length_max>30</length_max>
  </Column>
  <Column name="company_code">
    <name>カード会社コード</name>
    <length_max>20</length_max>
  </Column>
  <Column name="cooperation_id2">
    <name>連携ID2</name>
    <length_max>100</length_max>
  </Column>
  <Column name="register_action_kbn">
    <name>登録アクション区分</name>
    <length_max>10</length_max>
  </Column>
  <Column name="register_status">
    <name>登録ステータス</name>
    <length_max>10</length_max>
  </Column>
  <Column name="register_target_id">
    <name>登録対象ID</name>
    <length_max>30</length_max>
  </Column>
  <Column name="before_order_status">
    <name>更新前ステータス</name>
    <length_max>30</length_max>
  </Column>
  <Column name="cooperation_type">
    <name>連携種別</name>
    <length_max>10</length_max>
  </Column>
  <Column name="date_created">
    <name>作成日</name>
    <type>DATE</type>
  </Column>
  <Column name="date_changed">
    <name>更新日</name>
    <type>DATE</type>
  </Column>
</UserCreditCardInsertUpdate>