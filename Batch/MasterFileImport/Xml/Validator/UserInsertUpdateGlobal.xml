  </Column>
  <Column name="remote_addr">
    <name>Remote address</name>
    <necessary></necessary>
    <type></type>
    <length></length>
    <length_max>15</length_max>
    <length_min></length_min>
  </Column>
  <Column name="member_rank_id">
    <name>Member rank ID</name>
    <necessary></necessary>
    <type>HALFWIDTH_ALPHNUMSYMBOL</type>
    <length></length>
    <length_max>30</length_max>
    <length_min></length_min>
  </Column>
  <Column name="integrated_flg">
    <name>Integrated flag</name>
    <necessary></necessary>
    <type></type>
    <length></length>
    <length_max>1</length_max>
    <length_min></length_min>
    <regexp ptn="存在する区分（0、1）">0|1</regexp>
  </Column>
  <Column name="fixed_purchase_member_flg">
    <name>Fixed purchase member flag</name>
    <necessary>1</necessary>
    <type></type>
    <length></length>
    <length_max>1</length_max>
    <length_min></length_min>
    <regexp ptn="通常会員/定期会員（0、1）">0|1</regexp>
  </Column>
  <Column name="last_birthday_point_add_year">
    <name>Last birthday point add year</name>
    <necessary></necessary>
    <type>HALFWIDTH_NUMBER</type>
    <length></length>
    <length_max></length_max>
    <length_min></length_min>
    <number_min>1900</number_min>
  </Column>
  <Column name="last_birthday_coupon_publish_year">
    <name>Last birthday coupon publish year</name>
    <necessary></necessary>
    <type>HALFWIDTH_NUMBER</type>
    <length></length>
    <length_max></length_max>
    <length_min></length_min>
    <number_min>1900</number_min>
  </Column>
  <Column name="referral_code">
    <name>@@User.referral_code.globalAddress.name@@</name>
    <length_max>@@User.referral_code.length_max@@</length_max>
  </Column>
  <Column name="referred_user_id">
