    <length></length>
    <length_max>@@User.addr5.globalAddress.length_max@@</length_max>
    <length_min></length_min>
  </Column>
  <Column name="company_name">
    <name>@@User.company_name.name@@</name>
    <necessary>@@User.company_name.necessary@@</necessary>
    <type>@@User.company_name.type@@</type>
    <length></length>
    <length_max>@@User.company_name.length_max@@</length_max>
    <length_min></length_min>
  </Column>
  <Column name="company_post_name">
    <name>@@User.company_post_name.name@@</name>
    <necessary>@@User.company_post_name.necessary@@</necessary>
    <type>@@User.company_post_name.type@@</type>
    <length></length>
    <length_max>@@User.company_post_name.length_max@@</length_max>
    <length_min></length_min>
  </Column>
  <Column name="tel1">
    <name>@@User.tel1.globalAddress.name@@</name>
    <necessary>1</necessary>
    <type>@@User.tel1.globalAddress.type@@</type>
    <length></length>
    <length_max>30</length_max>
    <length_min></length_min>
  </Column>
  <Column name="tel2">
    <name>@@User.tel2.globalAddress.name@@</name>
    <necessary></necessary>
    <type>@@User.tel2.globalAddress.type@@</type>
    <length></length>
    <length_max>30</length_max>
    <length_min></length_min>
  </Column>
  <Column name="mail_flg">
    <name>@@User.mail_flg.name@@</name>
    <necessary>1</necessary>
