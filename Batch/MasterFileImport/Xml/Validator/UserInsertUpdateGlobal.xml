<UserInsertUpdateGlobal>
  <Column name="user_id">
    <name>User ID</name>
    <necessary>1</necessary>
    <type>HALFWIDTH_ALPHNUM</type>
    <length></length>
    <length_max>30</length_max>
    <length_min></length_min>
    <regexp ptn="先頭「N」以外">^[^N].*$</regexp><!-- 先頭「N」以外のみ取込可能 -->
  </Column>
  <Column name="user_kbn">
    <name>User Type</name>
    <necessary>1</necessary>
    <type></type>
    <length></length>
    <length_max>10</length_max>
    <length_min></length_min>
    <regexp ptn="存在する区分（PC_USER、MB_USER、SP_USER、OFF_USER）">PC_USER|MB_USER|SP_USER|OFF_USER</regexp><!-- 会員のみ取込可能 -->
  </Column>
  <Column name="name">
    <name>@@User.name.name@@</name>
    <necessary></necessary>
    <type>@@User.name.globalAddress.type@@</type>
    <length></length>
    <length_max>@@User.name.length_max@@</length_max>
    <length_min></length_min>
  </Column>
  <Column name="name1">
    <name>@@User.name1.name@@</name>
    <necessary></necessary>
    <type>@@User.name1.globalAddress.type@@</type>
    <length></length>
    <length_max>@@User.name1.length_max@@</length_max>
    <length_min></length_min>
  </Column>
  <Column name="name2">
    <name>@@User.name2.name@@</name>
    <necessary></necessary>
    <type>@@User.name2.globalAddress.type@@</type>
    <length></length>
    <length_max>@@User.name2.length_max@@</length_max>
    <length_min></length_min>
  </Column>
  <Column name="mail_addr">
    <name>@@User.mail_addr.name@@</name>
    <necessary></necessary>
    <type>MAILADDRESS</type>
    <length></length>
    <length_max>256</length_max>
    <length_min></length_min>
  </Column>
  <Column name="mail_addr2">
    <name>@@User.mail_addr2.name@@</name>
    <necessary></necessary>
    <type>MAILADDRESS</type>
    <length></length>
    <length_max>256</length_max>
    <length_min></length_min>
  </Column>
  <Column name="birth">
    <name>@@User.birth.name@@</name>
    <necessary></necessary>
    <type>DATE_PAST</type>
    <length></length>
    <length_max></length_max>
    <length_min></length_min>
  </Column>
  <Column name="birth_year">
    <name>@@User.birth.name@@(Year)</name>
    <necessary></necessary>
    <type>HALFWIDTH_NUMBER</type>
    <length></length>
    <length_max></length_max>
    <length_min></length_min>
  </Column>
  <Column name="birth_month">
    <name>@@User.birth.name@@(Month)</name>
    <necessary></necessary>
    <type>HALFWIDTH_NUMBER</type>
    <length></length>
    <length_max></length_max>
    <length_min></length_min>
  </Column>
  <Column name="birth_day">
    <name>@@User.birth.name@@(Day)</name>
    <necessary></necessary>
    <type>HALFWIDTH_NUMBER</type>
    <length></length>
    <length_max></length_max>
    <length_min></length_min>
  </Column>
  <Column name="sex">
    <name>@@User.sex.name@@</name>
    <necessary>@@User.sex.necessary@@</necessary>
    <type></type>
    <length></length>
    <length_max></length_max>
    <length_min></length_min>
    <regexp ptn="存在する区分（MALE、FEMALE、UNKNOWN）">MALE|FEMALE|UNKNOWN</regexp><!-- MALE、FEMALE、UNKNOWNのみ取込可能 -->
  </Column>
  <Column name="zip">
    <name>@@User.zip.globalAddress.name@@</name>
    <necessary>@@User.zip.globalAddress.necessary@@</necessary>
    <type>@@User.zip.globalAddress.type@@</type>
    <length></length>
    <length_max>30</length_max>
    <length_min></length_min>
  </Column>
  <Column name="addr2">
