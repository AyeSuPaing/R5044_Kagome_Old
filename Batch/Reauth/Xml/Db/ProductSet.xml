<ProductSet>

  <!-- 商品セット取得 -->
  <GetProductSet>
    <Statement>
      <![CDATA[
        SELECT  w2_ProductSet.*
          FROM  w2_ProductSet
         WHERE  w2_ProductSet.shop_id = @shop_id
           AND  w2_ProductSet.product_set_id = @product_set_id
      ]]>
    </Statement>
    <Parameter>
      <Input Name="shop_id" Type="nvarchar" Size="10" />
      <Input Name="product_set_id" Type="nvarchar" Size="30" />
    </Parameter>
  </GetProductSet>

</ProductSet>