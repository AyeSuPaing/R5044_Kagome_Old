<ProcessAccessLog>

  <!-- 加工開始ステータス更新 -->
  <UpdateProcessBgnStatusAndGetTargetDate>
    <Statement>
      <![CDATA[
        DECLARE @target_date varchar(10)
        EXEC w2_LogProccessBeginSp @target_date OUTPUT
        
        SELECT @target_date
      ]]>
    </Statement>
  </UpdateProcessBgnStatusAndGetTargetDate>

  <!-- 加工終了ステータス更新 -->
  <UpdateProcessEndStatus>
    <Statement>
      <![CDATA[
        EXEC w2_LogProccessEndSp @target_date
      ]]>
    </Statement>
    <Parameter>
      <Input Name="target_date" Type="nvarchar" Size="10" />
    </Parameter>
  </UpdateProcessEndStatus>

  </ProcessAccessLog>