<CsMessageMailData>
  
  <!-- 登録 -->
  <Register>
    <Statement>
      <![CDATA[
        INSERT w2_CsMessageMailData
            (
               dept_id,
               mail_id,
               mail_data
            )
            VALUES
            (
              @dept_id,
              @mail_id,
              @mail_data
            )
      ]]>
    </Statement>
    <Parameter>
      <Input Name="dept_id" Type="nvarchar" Size="30" />
      <Input Name="mail_id" Type="nvarchar" Size="30" />
      <Input Name="mail_data" Type="nvarchar" Size="max" />
    </Parameter>
  </Register>

</CsMessageMailData>
