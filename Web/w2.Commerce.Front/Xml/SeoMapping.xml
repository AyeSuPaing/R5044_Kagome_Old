<!--
  こちらはSEOタグ設定で使用するカラーの設定です。
  SEOタグ設定画面の「@@ product_color @@」で置換される値が下記の「表示されるカラー」で設定されます。
  ■フォーマット
    <Setting before="[一意なID]" after="[表示されるカラー]" />
-->
<SeoMapping>
  <color>
    <Setting before="black" after="黒" />
    <Setting before="gray" after="グレー" />
    <Setting before="white" after="白" />
    <Setting before="red" after="赤" />
    <Setting before="pink" after="ピンク" />
    <Setting before="orange" after="オレンジ" />
    <Setting before="brown" after="茶色" />
    <Setting before="yellow" after="黄色" />
    <Setting before="green" after="緑" />
    <Setting before="blue" after="青" />
    <Setting before="pink" after="紫" />
  </color>
</SeoMapping>