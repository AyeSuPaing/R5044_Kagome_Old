  <!-- ログインID入力チェック(メールアドレスのログインID利用無し) -->
  <Column name="login_id_input_check" control="tbUserLoginId">
    <name>@@User.login_id.name@@</name>
    <necessary>1</necessary>
    <type>@@User.login_id.type@@</type>
    <length></length>
    <length_max>@@User.login_id.length_max@@</length_max>
    <length_min>@@User.login_id.length_min@@</length_min>
  </Column>
  <Column name="login_id">
    <name>ログイン情報</name>
  </Column>
  <Column name="password" control="tbUserPassword">
    <name>@@User.password.name@@</name>
    <necessary>1</necessary>
    <type>@@User.password.type@@</type>
    <length></length>
    <length_max>@@User.password.length_max@@</length_max>
    <length_min>@@User.password.length_min@@</length_min>
    <regexp ptn="@@User.password.regexp_ptn@@">@@User.password.regexp@@</regexp>
    <confirm>password_conf</confirm>
    <equivalence target_name="@@User.password.equivalence_target_name@@">@@User.password.equivalence@@</equivalence>
  </Column>
  <Column name="password_conf" control="tbUserPasswordConf">
    <name>@@User.password.name@@(確認用)</name>
    <necessary>1</necessary>
    <type>@@User.password.type@@</type>
    <length></length>
    <length_max>@@User.password.length_max@@</length_max>
    <length_min>@@User.password.length_min@@</length_min>
  </Column>
  <Column name="zip" control="tbUserZip">
    <name>@@User.zip.name@@</name>
    <necessary>1</necessary>
    <regexp ptn="正しい形式">(^[0-9]{3}(-|ｰ)[0-9]{4}$)|^([0-9]{7}$)</regexp>
  </Column>
  <Column name="tel1" control="tbUserTel1_1">
    <name>@@User.tel1.name@@</name>
    <necessary>1</necessary>
    <length_max>13</length_max>
    <except_regexp ptn="9 桁以下">(^([0-9](-|ｰ)?){1,9}$)</except_regexp>
    <regexp ptn="正しい形式">(^[0-9]{2,5}(-|ｰ)[0-9]{1,4}(-|ｰ)[0-9]{2,4}$)|^([0-9]{10,11}$)</regexp>
  </Column>
  <Column name="tel2" control="tbUserTel1_2">
    <name>@@User.tel2.name@@</name>
    <length_max>13</length_max>
    <except_regexp ptn="9 桁以下">(^([0-9](-|ｰ)?){1,9}$)</except_regexp>
    <regexp ptn="正しい形式">(^[0-9]{2,5}(-|ｰ)[0-9]{1,4}(-|ｰ)[0-9]{2,4}$)|^([0-9]{10,11}$)</regexp>
  </Column>
  <Column name="authentication_code" control="tbAuthenticationCode">
    <name>@@User.authentication_code.name@@</name>
    <length_max>@@User.authentication_code.length_max@@</length_max>
  </Column>
