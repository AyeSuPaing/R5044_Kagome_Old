<PasswordModify>
  <Column name="birth" control="tbBirth">
    <name>@@User.birth.name@@</name>
    <necessary>1</necessary>
    <type>HALFWIDTH_NUMBER</type>
    <length>8</length>
    <length_max></length_max>
    <length_min></length_min>
    <different_value target_name="登録済み@@User.birth.name@@">registed_birth</different_value>
  </Column>
  <Column name="tel1_1" control="tbTel1_1">
    <name>@@User.tel1.name@@(前)</name>
    <necessary>1</necessary>
    <type>HALFWIDTH_NUMBER</type>
    <length_max>6</length_max>
    <different_value target_name="登録済み@@User.tel1.name@@(前)">registed_tel1_1</different_value>
  </Column>
  <Column name="tel1_2" control="tbTel1_2">
   <name>@@User.tel1.name@@(中)</name>
    <necessary>1</necessary>
    <type>HALFWIDTH_NUMBER</type>
    <length></length>
    <length_max>4</length_max>
    <length_min></length_min>
    <different_value target_name="登録済み@@User.tel1.name@@(中)">registed_tel1_2</different_value>
  </Column>
  <Column name="tel1_3" control="tbTel1_3">
   <name>@@User.tel1.name@@(後)</name>
    <necessary>1</necessary>
    <type>HALFWIDTH_NUMBER</type>
    <length></length>
    <length_max>4</length_max>
    <length_min></length_min>
    <different_value target_name="登録済み@@User.tel1.name@@(後)">registed_tel1_3</different_value>
  </Column>
  <Column name="password" control="tbPassword">
    <name>新しい@@User.password.name@@</name>
    <necessary>1</necessary>
    <type>@@User.password.type@@</type>
    <length></length>
    <length_max>@@User.password.length_max@@</length_max>
    <length_min>@@User.password.length_min@@</length_min>
    <regexp ptn="@@User.password.regexp_ptn@@">@@User.password.regexp@@</regexp>
    <confirm>password_conf</confirm>
    <equivalence target_name="@@User.password.equivalence_target_name@@">@@User.password.equivalence@@</equivalence>
  </Column>
  <Column name="password_conf" control="tbPasswordConf">
    <name>新しい@@User.password.name@@（確認用）</name>
    <necessary>1</necessary>
    <type>@@User.password.type@@</type>
    <length></length>
    <length_max>@@User.password.length_max@@</length_max>
    <length_min>@@User.password.length_min@@</length_min>
  </Column>
  <Column name="tel1" control="tbTel1">
    <name>@@User.tel1.name@@</name>
    <necessary>1</necessary>
    <length_max>13</length_max>
    <except_regexp ptn="9 桁以下">(^([0-9](-|ｰ)?){1,9}$)</except_regexp>
    <regexp ptn="正しい形式">(^[0-9]{2,5}(-|ｰ)[0-9]{1,4}(-|ｰ)[0-9]{2,4}$)|^([0-9]{10,11}$)</regexp>
    <different_value target_name="登録済み@@User.tel1.name@@(前)">registed_tel1</different_value>
  </Column>
</PasswordModify>
