<UserBusinessOwner>
  <Column name="owner_name1" control="tbOwnerName1">
    <name>@@User.OwnerName1.name@@</name>
    <necessary>1</necessary>
    <type>@@User.OwnerName1.type@@</type>
    <length></length>
    <length_max>@@User.OwnerName1.length_max@@</length_max>
    <length_min></length_min>
  </Column>
  <Column name="owner_name2" control="tbOwnerName2">
    <name>@@User.OwnerName2.name@@</name>
    <necessary>1</necessary>
    <type>@@User.OwnerName2.type@@</type>
    <length></length>
    <length_max>@@User.OwnerName2.length_max@@</length_max>
    <length_min></length_min>
  </Column>
  <Column name="owner_name_kana1" control="tbOwnerNameKana1">
    <name>@@User.OwnerNameKana1.name@@</name>
    <necessary>1</necessary>
    <type>@@User.OwnerNameKana1.type@@</type>
    <length></length>
    <length_max>@@User.OwnerNameKana1.length_max@@</length_max>
    <length_min></length_min>
  </Column>
  <Column name="owner_name_kana2" control="tbOwnerNameKana2">
    <name>@@User.OwnerNameKana2.name@@</name>
    <necessary>1</necessary>
    <type>@@User.OwnerNameKana2.type@@</type>
    <length></length>
    <length_max>@@User.OwnerNameKana2.length_max@@</length_max>
    <length_min></length_min>
  </Column>

  <Column name="owner_birth_year" control="ddlOwnerBirthYear">
    <name>@@User.birth.name@@(年)</name>
    <necessary>1</necessary>
    <type>HALFWIDTH_NUMBER</type>
    <length></length>
    <length_max></length_max>
    <length_min></length_min>
  </Column>
  <Column name="owner_birth_month" control="ddlOwnerBirthMonth">
    <name>@@User.birth.name@@(月)</name>
    <necessary>1</necessary>
    <type>HALFWIDTH_NUMBER</type>
    <length></length>
    <length_max></length_max>
    <length_min></length_min>
  </Column>
  <Column name="owner_birth_day" control="ddlOwnerBirthDay">
    <name>@@User.birth.name@@(日)</name>
    <necessary>1</necessary>
    <type>HALFWIDTH_NUMBER</type>
    <length></length>
    <length_max></length_max>
    <length_min></length_min>
  </Column>
  <Column name="owner_birth" control="ddlOwnerBirthDay">
    <name>@@User.birth.name@@</name>
    <necessary></necessary>
    <type>DATE_PAST</type>
    <length></length>
    <length_max></length_max>
    <length_min></length_min>
  </Column>
  <Column name="request_budget" control="tbRequestBudget">
    <name>@@User.RequestBudget.name@@</name>
    <necessary>0</necessary>
    <type>@@User.RequestBudget.type@@</type>
    <length></length>
    <length_max>@@User.RequestBudget.length_max@@</length_max>
    <length_min></length_min>
    <price_max>10000000</price_max>
  </Column>
</UserBusinessOwner>