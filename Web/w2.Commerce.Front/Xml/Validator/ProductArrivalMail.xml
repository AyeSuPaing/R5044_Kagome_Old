<ProductArrivalMail>
  <Column name="mail_addr">
    <name>@@User.mail_addr.name@@</name>
    <necessary>1</necessary>
    <type>MAILADDRESS</type>
    <length></length>
    <length_max>256</length_max>
    <length_min></length_min>
  </Column>
  <Column name="mail_addr_check">
    <name>通知先アドレス</name>
    <necessary>1</necessary>
    <type></type>
    <length></length>
    <length_max></length_max>
    <length_min></length_min>
  </Column>
  <Column name="mail_addr_kbn">
    <name>配信先</name>
    <necessary>1</necessary>
    <type></type>
    <length></length>
    <length_max></length_max>
    <length_min></length_min>
  </Column>
  <Column name="date_expired_year_year">
    <name>の通知期限</name>
    <necessary></necessary>
    <type>HALFWIDTH_DATE</type>
    <length></length>
    <length_max></length_max>
    <length_min></length_min>
  </Column>
</ProductArrivalMail>
