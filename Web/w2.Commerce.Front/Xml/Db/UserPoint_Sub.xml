<UserPoint_Sub>

  <!-- ポイント履歴一覧取得用WHERE文 -->
  <GETPOINTHISTORYLIST_SEARCH_WHERE>
    <Statement>
      <![CDATA[
         WHERE  (
                  PointList.point_order_id = ''
                  OR
                  w2_Order.order_status NOT IN ('TMP','TMP_CNSL')	-- 仮注文、仮注文キャンセル以外
                  OR
                  w2_FixedPurchase.fixed_purchase_status IN ('10', '30')
                )
      ]]>
    </Statement>
  </GETPOINTHISTORYLIST_SEARCH_WHERE>

</UserPoint_Sub>