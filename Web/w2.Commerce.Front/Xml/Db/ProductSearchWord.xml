<ProductSearchWord>
  
  <!-- 検索ワード登録 -->
  <RegistSearchWord>
    <Statement>
      <![CDATA[
        INSERT  w2_ProductSearchWordHistory
                (
                  dept_id,
                  access_kbn,
                  search_word,
                  hits,
                  date_created
                )
        VALUES  (
                  @dept_id,
                  @access_kbn,
                  @search_word,
                  @hits,
                  @date_created
                )
        ]]>
      </Statement>
    <Parameter>
      <Input Name="dept_id" Type="nvarchar" Size="30" />
      <Input Name="access_kbn" Type="nvarchar" Size="10" />
      <Input Name="search_word" Type="nvarchar" Size="200" />
      <Input Name="hits" Type="int" />
      <Input Name="date_created" Type="datetime" />
    </Parameter>
  </RegistSearchWord>
  
</ProductSearchWord>