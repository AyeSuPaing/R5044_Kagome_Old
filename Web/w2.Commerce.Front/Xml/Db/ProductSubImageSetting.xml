<ProductSubImageSetting>

  <!-- 商品サブ画像設定取得 -->
  <GetProductSubImageSettingList>
    <Statement>
      <![CDATA[
        SELECT  *
          FROM  w2_ProductSubImageSetting
         WHERE  w2_ProductSubImageSetting.shop_id = @shop_id
           AND  w2_ProductSubImageSetting.product_sub_image_no <= @product_sub_image_no
      ]]>
    </Statement>
    <Parameter>
      <Input Name="shop_id" Type="nvarchar" Size="10" />
      <Input Name="product_sub_image_no" Type="int" />
    </Parameter>
  </GetProductSubImageSettingList>

</ProductSubImageSetting>

