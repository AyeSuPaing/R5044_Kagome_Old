<AffiliateCoopLog>

  <!-- 外部アフィリエイト連携情報インサート -->
  <InsertAffiliateCoopLog>
    <Statement>
      <![CDATA[
        INSERT  w2_AffiliateCoopLog
                (
                  affiliate_kbn,
                  master_id,
                  coop_status,
                  coop_data1,
                  coop_data2,
                  coop_data11,
                  last_changed
                )
        VALUES  (
                  @affiliate_kbn,
                  @master_id,
                  @coop_status,
                  @coop_data1,
                  @coop_data2,
                  @coop_data11,
                  'user'
                )
      ]]>
    </Statement>
    <Parameter>
      <Input Name="affiliate_kbn" Type="nvarchar" Size="50" />
      <Input Name="master_id" Type="nvarchar" Size="50" />
      <Input Name="coop_status" Type="nvarchar" Size="10" />
      <Input Name="coop_data1" Type="nvarchar" Size="max" />
      <Input Name="coop_data2" Type="nvarchar" Size="100" />
      <Input Name="coop_data11" Type="int" />
    </Parameter>
  </InsertAffiliateCoopLog>

</AffiliateCoopLog>