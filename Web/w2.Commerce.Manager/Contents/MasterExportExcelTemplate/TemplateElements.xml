<Settings xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner" xmlns="http://schemas.microsoft.com/sqlserver/reporting/2008/01/reportdefinition">
  <!-- ▼ Field(列名・データタイプ) ▼ -->
  <Field>
    <!-- 列名を付与する -->
    <DataField></DataField>
    <!-- データタイプ(System.String等)を付与する -->
    <rd:TypeName></rd:TypeName>
  </Field>
  <!-- ▲ Field(列名・データタイプ) ▲ -->
  
  <!-- ▼ TablixColumn(列数) ▼ -->
  <TablixColumn>
    <Width>72pt</Width>
  </TablixColumn>
  <!-- ▲ TablixColumn(列数) ▲ -->
  
  <!-- ▼ TablixRow(ヘッダ＆明細) ▼ -->
  <TablixRow>
    <Height>0.25in</Height>
    <TablixCells>
      <!-- TablixCell(要素数)を動的に追加 -->
    </TablixCells>
  </TablixRow>
  <!-- ▲ TablixRow(ヘッダ＆明細) ▲ -->
  
  <!-- ▼ TablixCell(要素数) ▼ -->
  <TablixCell>
    <CellContents>
      <!-- Name属性を付与する -->
      <Textbox>
        <Paragraphs>
          <Paragraph>
            <TextRuns>
              <TextRun>
                <!-- 表示する値を付与する -->
                <Value></Value>
                <Style>
                  <FontFamily>ＭＳ ゴシック</FontFamily>
                </Style>
              </TextRun>
            </TextRuns>
            <Style />
          </Paragraph>
        </Paragraphs>
        <Style>
          <Border>
            <Color>LightGrey</Color>
            <Style>Solid</Style>
          </Border>
        </Style>
      </Textbox>
    </CellContents>
  </TablixCell>
  <!-- ▲ TablixCell(要素数) ▲ -->
  
  <!-- ▼ TablixMember(列数) ▼ -->
  <TablixMember />
  <!-- ▲ TablixMember(列数) ▲ -->
</Settings>