<FixedPurchaseWorkflowSettingModify>
  <Column name="shop_id">
    <name>店舗ID</name>
    <necessary>1</necessary>
    <type>HALFWIDTH_ALPHNUM</type>
    <length></length>
    <length_max>10</length_max>
    <length_min>1</length_min>
  </Column>
  <Column name="workflow_kbn">
    <name>ワークフロー区分</name>
    <necessary>1</necessary>
    <type>HALFWIDTH_NUMBER</type>
    <length></length>
    <length_max>3</length_max>
    <length_min></length_min>
  </Column>
  <Column name="workflow_no">
    <name>枝番</name>
    <necessary>1</necessary>
    <type>HALFWIDTH_NUMBER</type>
    <length></length>
    <length_max></length_max>
    <length_min></length_min>
  </Column>
  <Column name="workflow_name">
    <name>ワークフロー名</name>
    <necessary>1</necessary>
    <type></type>
    <length></length>
    <length_max>50</length_max>
    <length_min></length_min>
  </Column>
  <Column name="desc1">
    <name>説明</name>
    <necessary></necessary>
    <type></type>
    <length></length>
    <length_max>250</length_max>
    <length_min></length_min>
  </Column>
  <Column name="display_order">
    <name>表示順</name>
    <necessary>1</necessary>
    <type>HALFWIDTH_NUMBER</type>
    <number_max>999</number_max>
    <number_min>1</number_min>
  </Column>
  <Column name="valid_flg">
    <name>有効フラグ</name>
    <necessary>1</necessary>
    <type>HALFWIDTH_NUMBER</type>
    <length>1</length>
    <length_max></length_max>
    <length_min></length_min>
  </Column>
  <Column name="fixed_purchase_is_alive_change">
    <name>定期購入状態</name>
    <necessary></necessary>
    <type>HALFWIDTH</type>
    <length></length>
    <length_max></length_max>
    <length_min></length_min>
  </Column>
  <Column name="fixed_purchase_payment_status_change">
    <name>決済ステータス</name>
    <necessary></necessary>
    <type>HALFWIDTH_NUMBER</type>
    <length></length>
    <length_max></length_max>
    <length_min></length_min>
  </Column>
  <Column name="next_shipping_date_change">
    <name>次回予定日更新</name>
    <necessary></necessary>
    <type>HALFWIDTH_NUMBER</type>
    <length></length>
    <length_max></length_max>
    <length_min></length_min>
  </Column>
  <Column name="next_next_shipping_date_change">
    <name>次々回予定日更新</name>
    <necessary></necessary>
    <type>HALFWIDTH_NUMBER</type>
    <length></length>
    <length_max></length_max>
    <length_min></length_min>
  </Column>
  <Column name="product_id">
    <name>商品ID</name>
    <necessary></necessary>
    <type></type>
    <length></length>
    <length_max></length_max>
    <length_min></length_min>
    <regexp ptn="半角英数字、「_」、「-」、「(」、「)」、「#」、「,」">^[a-zA-Z0-9_\-\(\)#\,]+$</regexp>
  </Column>
  <Column name="fixedPurchase_order_count_from">
    <name> 購入回数(注文基準[From])</name>
    <type>HALFWIDTH_NUMBER</type>
    <number_min>0</number_min>
  </Column>
  <Column name="fixedPurchase_order_count_to">
    <name> 購入回数(注文基準[To])</name>
    <type>HALFWIDTH_NUMBER</type>
    <number_min>0</number_min>
  </Column>
  <Column name="fixedPurchase_shipped_count_from">
    <name> 購入回数(出荷基準[From])</name>
    <type>HALFWIDTH_NUMBER</type>
    <number_min>0</number_min>
  </Column>
  <Column name="fixedPurchase_shipped_count_to">
    <name> 購入回数(出荷基準[To])</name>
    <type>HALFWIDTH_NUMBER</type>
    <number_min>0</number_min>
  </Column>
  <Column name="subscriptionBox_order_count_from">
    <name> 頒布会注文回数[From]</name>
    <type>HALFWIDTH_NUMBER</type>
    <number_min>0</number_min>
    <length_max>8</length_max>
  </Column>
  <Column name="subscriptionBox_order_count_to">
    <name> 頒布会注文回数[To]</name>
    <type>HALFWIDTH_NUMBER</type>
    <number_min>0</number_min>
    <length_max>8</length_max>
  </Column>
  <Column name="fixedPurchase_date_created_to">
    <name> 作成日</name>
    <type>HALFWIDTH_NUMBER</type>
    <number_min>0</number_min>
  </Column>
  <Column name="fixedPurchase_date_changed_to">
    <name> 更新日</name>
    <type>HALFWIDTH_NUMBER</type>
    <number_min>0</number_min>
  </Column>
  <Column name="fixedPurchase_last_order_date_to">
    <name> 最終購入日</name>
    <type>HALFWIDTH_NUMBER</type>
    <number_min>0</number_min>
  </Column>
  <Column name="fixedPurchase_date_bgn_to">
    <name> 購入開始日</name>
    <type>HALFWIDTH_NUMBER</type>
    <number_min>0</number_min>
  </Column>
  <Column name="fixedPurchase_next_shipping_date_from">
    <name> 次回配送日[From]</name>
    <type>HALFWIDTH_NUMBER</type>
    <number_min>0</number_min>
  </Column>
  <Column name="fixedPurchase_next_shipping_date_to">
    <name> 次回配送日[To]</name>
    <type>HALFWIDTH_NUMBER</type>
    <number_min>0</number_min>
  </Column>
  <Column name="fixedPurchase_next_next_shipping_date_from">
    <name> 次々回配送日[From]</name>
    <type>HALFWIDTH_NUMBER</type>
    <number_min>0</number_min>
  </Column>
  <Column name="fixedPurchase_next_next_shipping_date_to">
    <name> 次々回配送日[To]</name>
    <type>HALFWIDTH_NUMBER</type>
    <number_min>0</number_min>
  </Column>
</FixedPurchaseWorkflowSettingModify>