<FixedPurchaseProductChangeSettingSearch>
  <Column name="fixed_purchase_product_change_id">
    <name>定期商品変更ID</name>
    <length_max>50</length_max>
    <regexp ptn="半角英数字、「_」、「-」、「(」、「)」、「#」">^[a-zA-Z0-9_\-\(\)#]+$</regexp>
  </Column>
  <Column name="fixed_purchase_product_change_name">
    <name>定期商品変更名</name>
    <length_max>100</length_max>
  </Column>
  <Column name="before_change_product_id">
    <name>変更元商品ID</name>
    <length_max>60</length_max>
  </Column>
  <Column name="after_change_product_id">
    <name>変更後商品ID</name>
    <length_max>60</length_max>
  </Column>
</FixedPurchaseProductChangeSettingSearch>
