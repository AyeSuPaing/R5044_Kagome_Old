<OrderReturnExchangeInput>
  <!-- 注文情報 -->
  <Column name="return_exchange_date">
    <name>返品日・更新日</name>
    <necessary>1</necessary>
    <type>HALFWIDTH_DATE</type>
    <length></length>
    <length_max></length_max>
    <length_min></length_min>
  </Column>  
  <Column name="order_payment_kbn">
    <name>決済種別</name>
    <necessary>1</necessary>
    <type>HALFWIDTH_ALPHNUMSYMBOL</type>
    <length></length>
    <length_max>10</length_max>
    <length_min></length_min>
  </Column>
  <Column name="order_price_regulation">
    <name>調整金額</name>
    <necessary>1</necessary>
    <type>HALFWIDTH_DECIMAL</type>
    <length></length>
    <number_max>9999999</number_max>
    <number_min></number_min>
  </Column>
  <Column name="order_repayment_price">
    <name>返金金額</name>
    <necessary>1</necessary>
    <type>HALFWIDTH_DECIMAL</type>
    <length></length>
    <number_max>9999999</number_max>
    <number_min>0</number_min>
  </Column>
  <Column name="ReducePrice">
    <name>減額請求金額</name>
    <necessary>1</necessary>
    <type>HALFWIDTH_DECIMAL</type>
    <length></length>
    <number_max>9999999</number_max>
    <number_min>0</number_min>
  </Column>
</OrderReturnExchangeInput>