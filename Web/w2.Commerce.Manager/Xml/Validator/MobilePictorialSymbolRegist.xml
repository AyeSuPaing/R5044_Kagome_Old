<MobilePictorialSymbolRegist>

  <Column name="symbol_id">
    <name>絵文字ID</name>
    <necessary>1</necessary>
    <type>HALFWIDTH_ALPHNUMSYMBOL</type>
    <length></length>
    <length_max>10</length_max>
    <length_min></length_min>
    <duplication page="MobilePictorialSymbol" statement="CheckDuplicationInsertSymbolId">1</duplication>
  </Column>
  <Column name="symbol_tag">
    <name>絵文字タグ</name>
    <necessary>1</necessary>
    <type>HALFWIDTH_ALPHNUMSYMBOL</type>
    <length></length>
    <length_max>15</length_max>
    <length_min></length_min>
  </Column>
  <Column name="symbol_name">
    <name>絵文字名</name>
    <necessary>1</necessary>
    <type></type>
    <length></length>
    <length_max>30</length_max>
    <length_min></length_min>
  </Column>
  <Column name="symbol_code1">
    <name>docomo絵文字コード</name>
    <necessary></necessary>
    <type>HALFWIDTH_ALPHNUMSYMBOL</type>
    <length></length>
    <length_max>6</length_max>
    <length_min></length_min>
  </Column>
  <Column name="symbol_code2">
    <name>au絵文字コード</name>
    <necessary></necessary>
    <type>HALFWIDTH_ALPHNUMSYMBOL</type>
    <length></length>
    <length_max>6</length_max>
    <length_min></length_min>
  </Column>  
  <Column name="symbol_code6">
    <name>SoftBank絵文字コード</name>
    <necessary></necessary>
    <type>HALFWIDTH_ALPHNUMSYMBOL</type>
    <length></length>
    <length_max>6</length_max>
    <length_min></length_min>
  </Column>

</MobilePictorialSymbolRegist>