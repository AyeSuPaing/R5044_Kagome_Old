<ProductBrandRegist>
  <Column name="brand_id">
    <name>ブランドID</name>
    <necessary>1</necessary>
    <type>HALFWIDTH_ALPHNUMSYMBOL</type>
    <length></length>
    <length_max>30</length_max>
    <length_min></length_min>
    <prohibit>,</prohibit>
    <duplication page="ProductBrand" statement="CheckDuplicationInsertBrandId">1</duplication>
  </Column>
  <Column name="brand_name">
    <name>ブランド名</name>
    <necessary></necessary>
    <type></type>
    <length></length>
    <length_max>100</length_max>
    <length_min></length_min>
  </Column>
  <Column name="brand_title">
    <name>ブランドタイトル</name>
    <necessary></necessary>
    <type></type>
    <length></length>
    <length_max>100</length_max>
    <length_min></length_min>
  </Column>
  <Column name="seo_keyword">
    <name>SEOキーワード</name>
    <necessary></necessary>
    <type></type>
    <length></length>
    <length_max>200</length_max>
    <length_min></length_min>
  </Column>
</ProductBrandRegist>
