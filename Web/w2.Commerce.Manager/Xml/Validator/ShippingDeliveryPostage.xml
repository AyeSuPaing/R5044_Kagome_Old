<ShippingDeliveryPostage>
  <Column name="shipping_free_price_flg">
    <name>@@ 1 @@の購入金額による送料設定の利用の有無</name>
    <necessary>1</necessary>
    <type>HALFWIDTH_NUMBER</type>
    <length></length>
    <length_max></length_max>
    <length_min></length_min>
  </Column>
  <Column name="shipping_free_price">
    <name>@@ 1 @@の配送料無料購入金額</name>
    <necessary></necessary>
    <type>CURRENCY</type>
    <currency_localeId>@@Global.key_currency.localeId@@</currency_localeId>
    <currency_decimal_digits>@@Global.key_currency.currencyDecimalDigits@@</currency_decimal_digits>
    <length></length>
    <number_max>@@Global.key_currency.max@@</number_max>
    <number_min>0</number_min>
  </Column>
  <Column name="announce_shipping_free_flg">
    <name>@@ 1 @@の配送料無料案内表示の有無</name>
    <necessary></necessary>
    <type></type>
    <length></length>
    <number_max></number_max>
    <number_min></number_min>
  </Column>
  <Column name="calculation_plural_kbn">
    <name>@@ 1 @@の複数商品購入時の送料計算設定の利用の有無</name>
    <necessary>1</necessary>
    <type>HALFWIDTH_NUMBER</type>
    <length></length>
    <length_max></length_max>
    <length_min></length_min>
  </Column>
  <Column name="plural_shipping_price">
    <name>@@ 1 @@の複数商品配送料</name>
    <necessary></necessary>
    <type>CURRENCY</type>
    <currency_localeId>@@Global.key_currency.localeId@@</currency_localeId>
    <currency_decimal_digits>@@Global.key_currency.currencyDecimalDigits@@</currency_decimal_digits>
    <length></length>
    <number_max>@@Global.key_currency.max@@</number_max>
    <number_min>0</number_min>
  </Column>
  <Column name="free_shipping_fee">
    <name>@@ 1 @@の配送料無料時の請求料金</name>
    <necessary>1</necessary>
    <type>CURRENCY</type>
    <currency_localeId>@@Global.key_currency.localeId@@</currency_localeId>
    <currency_decimal_digits>@@Global.key_currency.currencyDecimalDigits@@</currency_decimal_digits>
    <number_max>@@Global.key_currency.max@@</number_max>
    <number_min>0</number_min>
  </Column>
</ShippingDeliveryPostage>
