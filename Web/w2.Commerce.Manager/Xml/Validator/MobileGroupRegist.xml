<MobileGroupRegist>
  <Column name="career_id">
    <name>キャリア</name>
    <necessary>1</necessary>
    <type>HALFWIDTH_ALPHNUM</type>
    <length></length>
    <length_max>30</length_max>
    <length_min></length_min>
  </Column>
  <Column name="mobile_group_id">
    <name>機種グループID</name>
    <necessary>1</necessary>
    <type>HALFWIDTH_ALPHNUM</type>
    <length></length>
    <length_max>5</length_max>
    <length_min></length_min>
    <duplication page="MobileGroup" statement="CheckDuplicationInsertMobileGroupId">1</duplication>
  </Column>
  <Column name="mobile_group_name">
    <name>機種グループ名</name>
    <necessary>1</necessary>
    <type></type>
    <length></length>
    <length_max>30</length_max>
    <length_min></length_min>
  </Column>
</MobileGroupRegist>