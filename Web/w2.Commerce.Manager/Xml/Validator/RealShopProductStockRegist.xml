<RealShopProductStockRegist>
  <Column name="real_shop_id">
    <name>リアル店舗ID</name>
    <necessary>1</necessary>
    <type></type>
    <length></length>
    <length_max>30</length_max>
    <length_min></length_min>
  </Column>
  <Column name="product_id">
    <name>商品ID</name>
    <necessary>1</necessary>
    <type></type>
    <length></length>
    <length_max>30</length_max>
    <length_min></length_min>
    <regexp ptn="半角英数字、「_」、「-」、「(」、「)」、「#」">^[a-zA-Z0-9_\-\(\)#]+$</regexp>
  </Column>
  <Column name="real_shop_stock">
    <name>在庫数</name>
    <necessary>1</necessary>
    <type>HALFWIDTH_NUMBER</type>
    <length></length>
    <number_max>99999</number_max>
    <number_min>0</number_min>
  </Column>
</RealShopProductStockRegist>