<GlobalShippingAreaRegister>
  <Column name="global_shipping_area_id">
    <name>配送エリアID</name>
    <necessary>1</necessary>
    <length_max>10</length_max>
    <type>HALFWIDTH_ALPHNUM</type>
  </Column>
  <Column name="sort_no">
    <name>表示順</name>
    <necessary>1</necessary>
    <length_max>3</length_max>
    <type>HALFWIDTH_NUMBER</type>
  </Column>
  <Column name="global_shipping_area_name">
    <name>配送エリア名</name>
    <necessary>1</necessary>
    <length_max>100</length_max>
  </Column>
</GlobalShippingAreaRegister>