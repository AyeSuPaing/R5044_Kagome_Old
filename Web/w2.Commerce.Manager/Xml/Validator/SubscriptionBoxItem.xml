<SubscriptionBoxItem>
  <Column name="product_id">
    <name>商品ID</name>
    <regexp ptn="半角英数字、「_」、「-」、「(」、「)」、「#」">^[a-zA-Z0-9_\-\(\)#]+$</regexp>
    <length_max>30</length_max>
  </Column>
  <Column name="variation_id">
    <name>バリエーションID</name>
    <regexp ptn="半角英数字、「_」、「-」、「(」、「)」、「#」">^[a-zA-Z0-9_\-\(\)#]+$</regexp>
    <length_max>60</length_max>
  </Column>
  <Column name="selectable_since">
    <name>選択可能期間開始</name>
    <necessary></necessary>
    <type>HALFWIDTH_DATE</type>
    <length></length>
    <length_max></length_max>
    <length_min></length_min>
  </Column>
  <Column name="selectable_until">
    <name>選択可能期間終了</name>
    <necessary></necessary>
    <type>HALFWIDTH_DATE</type>
    <length></length>
    <length_max></length_max>
    <length_min></length_min>
  </Column>
</SubscriptionBoxItem>