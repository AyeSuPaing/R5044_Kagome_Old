<NewsModify>
  <Column name="display_date_from">
    <name>表示日付（From）</name>
    <necessary>1</necessary>
    <type>HALFWIDTH_DATE</type>
    <length></length>
    <length_max></length_max>
    <length_min></length_min>
  </Column>
  <Column name="display_date_to">
    <name>表示日付（To）</name>
    <type>HALFWIDTH_DATE</type>
  </Column>
  <Column name="news_text_kbn">
    <name>本文区分</name>
    <necessary>1</necessary>
    <type></type>
    <length></length>
    <length_max></length_max>
    <length_min></length_min>
  </Column>
  <Column name="news_text">
    <name>本文</name>
    <necessary>1</necessary>
    <type></type>
    <length></length>
    <length_max></length_max>
    <length_min></length_min>
  </Column>
  <Column name="news_text_kbn_mobile">
    <name>モバイル本文区分</name>
    <necessary>1</necessary>
    <type></type>
    <length></length>
    <length_max></length_max>
    <length_min></length_min>
  </Column>
  <Column name="news_text_mobile">
    <name>モバイル本文</name>
    <necessary></necessary>
    <type></type>
    <length></length>
    <length_max></length_max>
    <length_min></length_min>
