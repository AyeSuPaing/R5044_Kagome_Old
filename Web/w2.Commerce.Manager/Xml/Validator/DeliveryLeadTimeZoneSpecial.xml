<DeliveryLeadTimeZoneSpecial>
  <Column name="lead_time_zone_name">
    <name>地帯名</name>
    <necessary>1</necessary>
    <length_max>50</length_max>
  </Column>
  <Column name="shipping_lead_time">
    <name>追加配送リードタイム</name>
    <necessary>1</necessary>
    <type>HALFWIDTH_NUMBER</type>
    <length_max>3</length_max>
    <number_min>0</number_min>
  </Column>
  <Column name="zip">
    <name>@@User.zip.name@@</name>
    <necessary>1</necessary>
    <type>HALFWIDTH_ALPHNUMSYMBOL</type>
    <length_max>4000</length_max>
    <regexp ptn="1234567,8901234,・・">^[0-9]{3}[0-9]{4}(,[0-9]{3}[0-9]{4})*$</regexp>
  </Column>
  <Column name="zip_tw">
    <name>@@User.zip.name@@</name>
    <necessary>1</necessary>
    <type>HALFWIDTH_ALPHNUMSYMBOL</type>
    <length_max>4000</length_max>
    <regexp ptn="123,1234,12345,123456,・・">^[0-9]{3,6}(,[0-9]{3,6})*$</regexp>
  </Column>
</DeliveryLeadTimeZoneSpecial>