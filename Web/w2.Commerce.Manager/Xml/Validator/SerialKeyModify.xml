<SerialKeyModify>
  <Column name="serial_key_chk">
    <name>シリアルキー</name>
    <necessary>1</necessary>
    <type></type>
    <length></length>
    <length_max>30</length_max>
    <length_min></length_min>
    <regexp ptn="半角英数字、「_」、「-」、「(」、「)」、「#」">^[a-zA-Z0-9_\-\(\)#]+$</regexp>
  </Column>
  <Column name="product_id">
    <name>商品ID</name>
    <necessary>1</necessary>
    <type></type>
    <length></length>
    <length_max>30</length_max>
    <length_min></length_min>
    <regexp ptn="半角英数字、「_」、「-」、「(」、「)」、「#」">^[a-zA-Z0-9_\-\(\)#]+$</regexp>
  </Column>
  <Column name="v_id">
    <name>バリエーションID</name>
    <necessary></necessary>
    <type></type>
    <length></length>
    <length_max>60</length_max>
    <length_min></length_min>
    <regexp ptn="半角英数字、「_」、「-」、「(」、「)」、「#」">^[a-zA-Z0-9_\-\(\)#]+$</regexp>
  </Column>
  <Column name="order_id">
    <name>注文ID</name>
    <necessary></necessary>
    <type></type>
    <length></length>
    <length_max>30</length_max>
    <length_min></length_min>
    <regexp ptn="半角英数字、「_」、「-」、「(」、「)」、「#」">^[a-zA-Z0-9_\-\(\)#]+$</regexp>
  </Column>
  <Column name="order_item_no">
    <name>注文商品枝番</name>
    <necessary></necessary>
    <type>HALFWIDTH_NUMBER</type>
    <length></length>
    <length_max></length_max>
    <length_min>0</length_min>
  </Column>
  <Column name="user_id">
    <name>ユーザID</name>
    <necessary></necessary>
    <type></type>
    <length></length>
    <length_max>30</length_max>
    <length_min></length_min>
    <regexp ptn="半角英数字、「_」、「-」、「(」、「)」、「#」">^[a-zA-Z0-9_\-\(\)#]+$</regexp>
  </Column>
  <Column name="date_delivered">
    <name>引渡日</name>
    <necessary></necessary>
    <type>HALFWIDTH_DATE</type>
    <length></length>
    <length_max></length_max>
    <length_min></length_min>
  </Column>
</SerialKeyModify>
