<ProductStockMessageRegist>
  <Column name="stock_message_id">
    <name>在庫文言ID</name>
    <necessary>1</necessary>
    <type>HALFWIDTH_NUMBER</type>
    <length>3</length>
    <duplication page="ProductStockMessage" statement="CheckDuplicationProductStockMessageId">1</duplication>
  </Column>
  <Column name="stock_message_name">
    <name>在庫文言名</name>
    <necessary>1</necessary>
    <type></type>
    <length></length>
    <length_max>30</length_max>
    <length_min></length_min>
  </Column>
  <Column name="stock_message_def">
    <name>標準在庫文言</name>
    <necessary></necessary>
    <type></type>
    <length></length>
    <length_max>30</length_max>
    <length_min></length_min>
  </Column>
  <Column name="stock_message_def_mobile">
    <name>モバイル用標準在庫文言</name>
    <necessary></necessary>
    <type></type>
    <length></length>
    <length_max>30</length_max>
    <length_min></length_min>
  </Column>
  <Column name="stock_datum">
    <name>在庫基準</name>
    <necessary>1</necessary>
    <type>HALFWIDTH_DECIMAL</type>
    <length></length>
    <length_max></length_max>
    <length_min></length_min>
    <regexp ptn="整数">^[\-]?[0-9]*$</regexp>
  </Column>
  <Column name="stock_message">
    <name>在庫文言</name>
    <necessary></necessary>
    <type></type>
    <length></length>
    <length_max>30</length_max>
    <length_min></length_min>
  </Column>
  <Column name="stock_message_mobile">
    <name>モバイル用在庫文言</name>
    <necessary></necessary>
    <type></type>
    <length></length>
    <length_max>30</length_max>
    <length_min></length_min>
  </Column>
</ProductStockMessageRegist>