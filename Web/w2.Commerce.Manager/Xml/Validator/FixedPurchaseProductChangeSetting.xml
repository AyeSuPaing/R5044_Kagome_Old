<FixedPurchaseProductChangeSetting>
  <Column name="fixed_purchase_product_change_id">
    <name>定期変更商品ID</name>
    <necessary>1</necessary>
    <length_max>50</length_max>
    <regexp ptn="半角英数字、「_」、「-」、「(」、「)」、「#」">^[a-zA-Z0-9_\-\(\)#]+$</regexp>
  </Column>
  <Column name="fixed_purchase_product_change_name">
    <name>定期変更商品名</name>
    <necessary>1</necessary>
    <length_max>100</length_max>
  </Column>
  <Column name="priority">
    <name>適用優先順</name>
    <necessary>1</necessary>
    <type>HALFWIDTH_NUMBER</type>
    <number_max>9999999</number_max>
    <number_min>1</number_min>
  </Column>
</FixedPurchaseProductChangeSetting>
