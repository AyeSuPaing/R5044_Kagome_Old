<ShopShippingZoneModify>
  <Column name="shipping_id">
    <name>配送種別</name>
    <necessary>1</necessary>
    <type>HALFWIDTH_ALPHNUM</type>
    <length></length>
    <length_max>4</length_max>
    <length_min></length_min>
  </Column>
  <Column name="shipping_zone_no">
    <name>配送料地帯区分</name>
    <necessary>1</necessary>
    <type>HALFWIDTH_NUMBER</type>
    <number_min>48</number_min>
    <length></length>
    <length_max>9</length_max>
    <duplication page="ShopShippingZone" statement="CheckDuplicationModifyShippingZoneNo">1</duplication>
  </Column>
  <Column name="shipping_zone_no_tw">
    <name>配送料地帯区分</name>
    <necessary>1</necessary>
    <type>HALFWIDTH_NUMBER</type>
    <number_min>24</number_min>
    <length_max>9</length_max>
    <duplication page="ShopShippingZone" statement="CheckDuplicationModifyShippingZoneNo">1</duplication>
  </Column>
  <Column name="shipping_zone_name">
    <name>地帯名</name>
    <necessary>1</necessary>
    <type></type>
    <length></length>
    <length_max>30</length_max>
  </Column>
  <Column name="zip">
    <name>@@User.zip.name@@</name>
    <necessary>1</necessary>
    <type>HALFWIDTH_ALPHNUMSYMBOL</type>
    <length></length>
    <length_max></length_max>
    <regexp ptn="1234567,8901234,・・">^[0-9]{3}[0-9]{4}(,[0-9]{3}[0-9]{4})*$</regexp>
  </Column>
  <Column name="zip_tw">
    <name>@@User.zip.name@@</name>
    <necessary>1</necessary>
    <type>HALFWIDTH_ALPHNUMSYMBOL</type>
    <regexp ptn="556001,701,70103,・・">^[0-9]{3,6}(,[0-9]{3,6})*$</regexp>
  </Column>
  <Column name="size_xxs_shipping_price">
    <name>XXSサイズ商品配送料</name>
    <necessary></necessary>
    <type>CURRENCY</type>
    <currency_localeId>@@Global.key_currency.localeId@@</currency_localeId>
    <currency_decimal_digits>@@Global.key_currency.currencyDecimalDigits@@</currency_decimal_digits>
    <length></length>
    <number_max>@@Global.key_currency.max@@</number_max>
    <number_min>0</number_min>
  </Column>
  <Column name="size_xs_shipping_price">
    <name>XSサイズ商品配送料</name>
    <necessary></necessary>
    <type>CURRENCY</type>
    <currency_localeId>@@Global.key_currency.localeId@@</currency_localeId>
    <currency_decimal_digits>@@Global.key_currency.currencyDecimalDigits@@</currency_decimal_digits>
    <length></length>
    <number_max>@@Global.key_currency.max@@</number_max>
    <number_min>0</number_min>
  </Column>
  <Column name="size_s_shipping_price">
    <name>Sサイズ商品配送料</name>
    <necessary></necessary>
    <type>CURRENCY</type>
    <currency_localeId>@@Global.key_currency.localeId@@</currency_localeId>
    <currency_decimal_digits>@@Global.key_currency.currencyDecimalDigits@@</currency_decimal_digits>
    <length></length>
    <number_max>@@Global.key_currency.max@@</number_max>
    <number_min>0</number_min>
  </Column>
  <Column name="size_m_shipping_price">
    <name>Mサイズ商品配送料</name>
    <necessary></necessary>
    <type>CURRENCY</type>
    <currency_localeId>@@Global.key_currency.localeId@@</currency_localeId>
    <currency_decimal_digits>@@Global.key_currency.currencyDecimalDigits@@</currency_decimal_digits>
    <length></length>
    <number_max>@@Global.key_currency.max@@</number_max>
    <number_min>0</number_min>
  </Column>
  <Column name="size_l_shipping_price">
    <name>Lサイズ商品配送料</name>
    <necessary></necessary>
    <type>CURRENCY</type>
    <currency_localeId>@@Global.key_currency.localeId@@</currency_localeId>
    <currency_decimal_digits>@@Global.key_currency.currencyDecimalDigits@@</currency_decimal_digits>
    <length></length>
    <number_max>@@Global.key_currency.max@@</number_max>
    <number_min>0</number_min>
  </Column>
  <Column name="size_xl_shipping_price">
    <name>XLサイズ商品配送料</name>
    <necessary></necessary>
    <type>CURRENCY</type>
    <currency_localeId>@@Global.key_currency.localeId@@</currency_localeId>
    <currency_decimal_digits>@@Global.key_currency.currencyDecimalDigits@@</currency_decimal_digits>
    <length></length>
    <number_max>@@Global.key_currency.max@@</number_max>
    <number_min>0</number_min>
  </Column>
  <Column name="size_xxl_shipping_price">
    <name>XXLサイズ商品配送料</name>
    <necessary></necessary>
    <type>CURRENCY</type>
    <currency_localeId>@@Global.key_currency.localeId@@</currency_localeId>
    <currency_decimal_digits>@@Global.key_currency.currencyDecimalDigits@@</currency_decimal_digits>
    <length></length>
    <number_max>@@Global.key_currency.max@@</number_max>
    <number_min>0</number_min>
  </Column>
</ShopShippingZoneModify>