    <length></length>
    <length_max>50</length_max>
    <length_min></length_min>
    <regexp ptn=" カンマ区切り形式（1,2,.. ,(99)）">(^\([1-9][0-9]*\)|^[1-9][0-9]*)((,\([1-9][0-9]*\))|(,[1-9][0-9]*))*$</regexp>
    <!-- 1以上999以下 -->
