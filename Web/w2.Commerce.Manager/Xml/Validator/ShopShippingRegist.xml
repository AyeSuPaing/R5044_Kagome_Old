    <necessary></necessary>
    <type>HALFWIDTH_ALPHNUMSYMBOL</type>
    <length></length>
    <length_max></length_max>
    <length_min></length_min>
    <regexp ptn=" カンマ区切り形式「5,10,..,999」また「(5),10,..,999」">(^\([1-9][0-9]*\)|^[1-9][0-9]*)((,\([1-9][0-9]*\))|(,[1-9][0-9]*))*$</regexp><!-- 1以上999以下 -->
  </Column>
  <Column name="fixed_purchase_kbn4_setting1">
