  <Column name="fixed_purchase_kbn1_setting">
    <name>月間隔選択肢</name>
    <necessary></necessary>
    <type>HALFWIDTH_ALPHNUMSYMBOL</type>
    <length></length>
    <length_max></length_max>
    <length_min></length_min>
    <regexp ptn=" カンマ区切り形式（1,2,.. ,99） ">^[1-9][0-9]{0,1}(,[1-9][0-9]{0,1})*$</regexp><!-- 1以上99以下 -->
  </Column>
  <Column name="fixed_purchase_kbn3_setting">
