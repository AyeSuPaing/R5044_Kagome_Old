<SubscriptionBoxDefaultItem>
  <Column name="product_id">
    <name>商品ID</name>
    <regexp ptn="半角英数字、「_」、「-」、「(」、「)」、「#」">^[a-zA-Z0-9_\-\(\)#]+$</regexp>
    <length_max>30</length_max>
  </Column>
  <Column name="variation_id">
    <name>バリエーションID</name>
    <regexp ptn="半角英数字、「_」、「-」、「(」、「)」、「#」">^[a-zA-Z0-9_\-\(\)#]+$</regexp>
    <length_max>60</length_max>
  </Column>
  <Column name="term_since">
    <name>期間開始</name>
    <necessary></necessary>
    <type>HALFWIDTH_DATE</type>
    <length></length>
    <length_max></length_max>
    <length_min></length_min>
  </Column>
  <Column name="term_until">
    <name>期間終了</name>
    <necessary></necessary>
    <type>HALFWIDTH_DATE</type>
    <length></length>
    <length_max></length_max>
    <length_min></length_min>
  </Column>
  <Column name="item_quantity">
    <name>数量</name>
    <necessary>1</necessary>
    <type>HALFWIDTH_NUMBER</type>
    <length></length>
    <number_max>99</number_max>
    <number_min>1</number_min>
  </Column>
  <Column name="necessary_product_flg">
    <name>必須商品フラグ</name>
    <necessary>1</necessary>
    <type>HALFWIDTH_NUMBER</type>
    <length>1</length>
    <number_max></number_max>
    <number_min></number_min>
  </Column>
</SubscriptionBoxDefaultItem>