<ProductSaleModify>
  <Column name="productsale_kbn">
    <name>商品セール区分</name>
    <necessary>1</necessary>
    <type></type>
    <length></length>
    <length_max></length_max>
    <length_min></length_min>
  </Column>
  <Column name="productsale_id">
    <name>商品セールID</name>
    <necessary></necessary>
    <type></type>
    <length></length>
    <length_max></length_max>
    <length_min></length_min>
  </Column>
  <Column name="productsale_name">
    <name>商品セール名</name>
    <necessary>1</necessary>
    <type></type>
    <length></length>
    <length_max>30</length_max>
    <length_min></length_min>
  </Column>
  <Column name="closedmarket_password_chk">
    <name>闇市パスワード</name>
    <necessary>1</necessary>
    <type></type>
    <length></length>
    <length_max></length_max>
    <length_min></length_min>
  </Column>
  <Column name="closedmarket_password">
    <name>闇市パスワード</name>
    <necessary></necessary>
    <type></type>
    <length></length>
    <length_max>30</length_max>
    <length_min></length_min>
    <!--duplication page="ProductSale" statement="CheckDuplicationModifyClosedmarketPassword">1</duplication-->
  </Column>
  <Column name="date_bgn">
    <name>開始日時</name>
    <necessary>1</necessary>
    <type>HALFWIDTH_DATE</type>
    <length></length>
    <length_max></length_max>
    <length_min></length_min>
  </Column>
  <Column name="date_end">
    <name>終了日時</name>
    <necessary>1</necessary>
    <type>HALFWIDTH_DATE</type>
    <length></length>
    <length_max></length_max>
    <length_min></length_min>
  </Column>
  <Column name="holding_period">
    <name>タイムセール開催期間</name>
    <necessary></necessary>
    <type></type>
    <length></length>
    <length_max></length_max>
    <length_min></length_min>
    <duplication page="ProductSale" statement="CheckDuplicationModifyTimeSalesDate">1</duplication>
  </Column>
  <Column name="valid_flg">
    <name>有効フラグ</name>
    <necessary>1</necessary>
    <type></type>
    <length></length>
    <length_max></length_max>
    <length_min></length_min>
  </Column>
</ProductSaleModify>
