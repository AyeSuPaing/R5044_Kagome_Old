<MallConfig>
  <Column name="mall_id">
    <name>モールID</name>
    <necessary>1</necessary>
    <length_max>14</length_max>
  </Column>
  <Column name="ddl_mall_kbn">
    <name>モール区分</name>
    <necessary>1</necessary>
  </Column>
  <Column name="mall_name">
    <name>モール名</name>
    <necessary>1</necessary>
    <length_max>50</length_max>
  </Column>
  <Column name="tgt_mail_addr">
    <name>対象メールアドレス</name>
    <type>MAILADDRESS</type>
    <length_max>256</length_max>
  </Column>
  <Column name="pop_server">
    <name>受信ＰＯＰサーバ</name>
    <type>HALFWIDTH_ALPHNUMSYMBOL</type>
    <length_max>100</length_max>
  </Column>
  <Column name="pop_port">
    <name>受信ＰＯＰポート</name>
    <type>HALFWIDTH_NUMBER</type>
    <length_max>5</length_max>
    <number_min>1</number_min>
    <number_max>65535</number_max>
  </Column>
  <Column name="pop_user_name">
    <name>受信ＰＯＰユーザ名</name>
    <type>HALFWIDTH_ALPHNUMSYMBOL</type>
    <length_max>50</length_max>
  </Column>
  <Column name="pop_password">
    <name>受信ＰＯＰパスワード</name>
    <type>HALFWIDTH_ALPHNUMSYMBOL</type>
    <length_max>50</length_max>
  </Column>
  <Column name="ftp_host">
    <name>ＦＴＰホスト名</name>
    <type>HALFWIDTH_ALPHNUMSYMBOL</type>
    <length_max>256</length_max>
  </Column>
  <Column name="ftp_user_name">
    <name>ＦＴＰユーザ名</name>
    <type>HALFWIDTH_ALPHNUMSYMBOL</type>
    <length_max>50</length_max>
  </Column>
  <Column name="ftp_password">
    <name>ＦＴＰパスワード</name>
    <type>HALFWIDTH_ALPHNUMSYMBOL</type>
    <length_max>50</length_max>
  </Column>
  <Column name="ftp_upload_dir">
    <name>ＦＴＰアップロード先ディレクトリ</name>
    <type>HALFWIDTH_ALPHNUMSYMBOL</type>
    <length_max>256</length_max>
  </Column>
  <Column name="maintenance_date_from">
    <name>メンテナンス開始日</name>
    <type>DATE</type>
    <length_max></length_max>
  </Column>
  <Column name="maintenance_date_to">
    <name>メンテナンス終了日</name>
    <type>DATE</type>
    <length_max></length_max>
  </Column>
  <Column name="rtn_mail_addr">
    <name>店舗連絡先メールアドレス</name>
    <type>MAILADDRESS</type>
    <length_max>256</length_max>
  </Column>
  <Column name="rakuten_api_user_name">
    <name>ユーザ－名</name>
    <necessary>1</necessary>
  </Column>
  <Column name="rakuten_api_shop_url">
    <name>店舗URL</name>
    <necessary>1</necessary>
  </Column>
  <Column name="rakuten_api_service_secret">
    <name>サービスシークレット</name>
    <necessary>1</necessary>
  </Column>
  <Column name="rakuten_api_license_key">
    <name>ライセンスキー</name>
    <necessary>1</necessary>
  </Column>
  <Column name="amazon_merchant_id">
    <name>Amazon出品者ID</name>
    <type>HALFWIDTH_ALPHNUMSYMBOL</type>
    <necessary>30</necessary>
  </Column>
  <Column name="amazon_marketplace_id">
    <name>AmazonマーケットプレイスID</name>
    <type>HALFWIDTH_ALPHNUMSYMBOL</type>
    <necessary>30</necessary>
  </Column>
  <Column name="amazon_aws_accesskey_id">
    <name>AmazonAWSアクセスキーID</name>
    <type>HALFWIDTH_ALPHNUMSYMBOL</type>
    <necessary>50</necessary>
  </Column>
  <Column name="amazon_secret_key">
    <name>Amazon秘密キー</name>
    <type>HALFWIDTH_ALPHNUMSYMBOL</type>
    <necessary>50</necessary>
  </Column>
  <Column name="amazon_mws_authtoken">
    <name>AmazonMWS認証トークン</name>
    <type>HALFWIDTH_ALPHNUMSYMBOL</type>
    <necessary>100</necessary>
  </Column>
  <Column name="stock_update_use_flg">
    <name>在庫連携利用フラグ</name>
    <necessary>1</necessary>
  </Column>
  <Column name="lohaco_private_key">
    <name>Lohaco秘密鍵</name>
    <necessary>1</necessary>
  </Column>
  <Colunm name="sftp_host">
    <name>SFTP サーバ接続先</name>
    <type>HALFWIDTH_ALPHNUMSYMBOL</type>
    <length_max>256</length_max>
  </Colunm>
  <Colunm name="sftp_user_name">
    <name>SFTP ユーザ名</name>
    <type>HALFWIDTH_ALPHNUMSYMBOL</type>
    <length_max>50</length_max>
  </Colunm>
  <Colunm name="sftp_pass_phrase">
    <name>SFTP パスフレーズ</name>
    <type>HALFWIDTH_ALPHNUMSYMBOL</type>
    <length_max>50</length_max>
  </Colunm>
  <Column name="sftp_port">
    <name>SFTP ポート</name>
    <type>HALFWIDTH_NUMBER</type>
  </Column>
  <Column name="andmall_tenant_code">
    <name>テナントコード設定</name>
    <necessary>1</necessary>
    <length_max>5</length_max>
  </Column>
  <Column name="andmall_base_store_code">
    <name>ショップID設定</name>
    <necessary>1</necessary>
    <length_max>8</length_max>
  </Column>
  <Column name="andmall_shop_no">
    <name>店番設定</name>
    <necessary>1</necessary>
    <length_max>32</length_max>
  </Column>
  <Colunm name="andmall_site_code">
    <name>サイトコード</name>
    <type>HALFWIDTH_ALPHNUMSYMBOL</type>
    <length_max>30</length_max>
  </Colunm>
  <Colunm name="andmall_signature_key">
    <name>サイト認証キー</name>
    <type>HALFWIDTH_ALPHNUMSYMBOL</type>
    <length_max>30</length_max>
  </Colunm>
  <Column name="andmall_cooperation">
    <name>＆mallの商品連携カラム</name>
    <necessary>1</necessary>
  </Column>
  <Column name="andmall_variation_cooperation">
    <name>＆mallの商品バリエーション連携カラム</name>
    <necessary>1</necessary>
  </Column>
</MallConfig>