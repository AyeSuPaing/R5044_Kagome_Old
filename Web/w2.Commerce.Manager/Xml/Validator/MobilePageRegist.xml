<MobilePageRegist>
  <Column name="page_id">
    <name>ページID</name>
    <necessary>1</necessary>
    <type>HALFWIDTH_ALPHNUMSYMBOL</type>
    <length></length>
    <length_max>30</length_max>
    <length_min></length_min>
    <prohibit>?&amp;/+#</prohibit>
    <duplication page="MobilePage" statement="CheckDuplicationInsertPageId">1</duplication>
  </Column>
  <Column name="page_name">
    <name>ページ名</name>
    <necessary>1</necessary>
    <type></type>
    <length></length>
    <length_max>30</length_max>
    <length_min></length_min>
  </Column>
  <Column name="html">
    <name>HTML</name>
    <necessary>1</necessary>
    <type></type>
    <length></length>
    <length_max></length_max>
    <length_min></length_min>
  </Column>
  <Column name="brand_id">
    <name>ブランドID</name>
    <necessary></necessary>
    <type>HALFWIDTH_ALPHNUMSYMBOL</type>
    <length></length>
    <length_max>30</length_max>
    <length_min></length_min>
  </Column>
</MobilePageRegist>