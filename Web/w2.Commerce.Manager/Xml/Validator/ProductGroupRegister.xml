<ProductGroupRegister>
  <Column name="product_group_id">
    <name>商品特集グループID</name>
    <necessary>1</necessary>
    <type></type>
    <length></length>
    <length_max>30</length_max>
    <length_min></length_min>
    <regexp ptn="半角英数字、「_」、「-」、「(」、「)」、「#」">^[a-zA-Z0-9_\-\(\)#]+$</regexp>
  </Column>
  <Column name="product_group_name">
    <name>商品特集グループ名</name>
    <necessary>1</necessary>
    <type></type>
    <length></length>
    <length_max>100</length_max>
    <length_min></length_min>
  </Column>
  <Column name="begin_date">
    <name>開始日時</name>
    <necessary>1</necessary>
    <type>DATE</type>
    <length></length>
    <length_max></length_max>
    <length_min></length_min>
  </Column>
  <Column name="end_date">
    <name>終了日時</name>
    <necessary></necessary>
    <type>DATE</type>
    <length></length>
    <length_max></length_max>
    <length_min></length_min>
  </Column>
</ProductGroupRegister>