  <!-- メールアドレスのログインID利用に対応するため、暫定的に半角英数記号用のチェックを設定 -->
  <Column name="login_id_chk">
    <name>@@User.login_id.name@@</name>
    <necessary>1</necessary>
    <type>HALFWIDTH_ALPHNUMSYMBOL</type>
    <length></length>
    <length_max>256</length_max>
    <length_min></length_min>
  </Column>
