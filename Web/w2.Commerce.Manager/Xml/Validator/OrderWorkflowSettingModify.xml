<OrderWorkflowSettingModify>
  <Column name="shop_id">
    <name>店舗ID</name>
    <necessary>1</necessary>
    <type>HALFWIDTH_ALPHNUM</type>
    <length></length>
    <length_max>10</length_max>
    <length_min>1</length_min>
  </Column>
  <Column name="workflow_kbn">
    <name>ワークフロー区分</name>
    <necessary>1</necessary>
    <type>HALFWIDTH_NUMBER</type>
    <length></length>
    <length_max>3</length_max>
    <length_min></length_min>
  </Column>
  <Column name="workflow_no">
    <name>枝番</name>
    <necessary>1</necessary>
    <type>HALFWIDTH_NUMBER</type>
    <length></length>
    <length_max></length_max>
    <length_min></length_min>
  </Column>
  <Column name="workflow_name">
    <name>ワークフロー名</name>
    <necessary>1</necessary>
    <type></type>
    <length></length>
    <length_max>50</length_max>
    <length_min></length_min>
  </Column>
  <Column name="desc1">
    <name>説明</name>
    <necessary></necessary>
    <type></type>
    <length></length>
    <length_max>250</length_max>
    <length_min></length_min>
  </Column>
  <Column name="display_order">
    <name>表示順</name>
    <necessary>1</necessary>
    <type>HALFWIDTH_NUMBER</type>
    <number_max>999</number_max>
    <number_min>1</number_min>
  </Column>
  <Column name="valid_flg">
    <name>有効フラグ</name>
    <necessary>1</necessary>
    <type>HALFWIDTH_NUMBER</type>
    <length>1</length>
    <length_max></length_max>
    <length_min></length_min>
  </Column>
  <Column name="order_status_change">
    <name>注文ステータス</name>
    <necessary></necessary>
    <type>HALFWIDTH</type>
    <length></length>
    <length_max></length_max>
    <length_min></length_min>
  </Column>
  <Column name="payment_status_change">
    <name>入金ステータス</name>
    <necessary></necessary>
    <type>HALFWIDTH_NUMBER</type>
    <length></length>
    <length_max>1</length_max>
    <length_min></length_min>
  </Column>
  <Column name="demand_status_change">
    <name>督促ステータス</name>
    <necessary></necessary>
    <type>HALFWIDTH_NUMBER</type>
    <length></length>
    <length_max>1</length_max>
    <length_min></length_min>
  </Column>
  <Column name="return_exchange_status_change">
    <name>返品交換ステータス</name>
    <necessary></necessary>
    <type>HALFWIDTH_NUMBER</type>
    <length></length>
    <length_max>2</length_max>
    <length_min></length_min>
  </Column>
  <Column name="repayment_status_change">
    <name>返金ステータス</name>
    <necessary></necessary>
    <type>HALFWIDTH_NUMBER</type>
    <length></length>
    <length_max>2</length_max>
    <length_min></length_min>
  </Column>
  <Column name="mail_id">
    <name>メール送信</name>
    <necessary></necessary>
    <type>HALFWIDTH_ALPHNUM</type>
    <length></length>
    <length_max>10</length_max>
    <length_min></length_min>
  </Column>
  <Column name="order_price_total">
    <name>合計金額</name>
    <necessary></necessary>
    <type>CURRENCY</type>
    <currency_localeId>@@Global.key_currency.localeId@@</currency_localeId>
    <currency_decimal_digits>@@Global.key_currency.currencyDecimalDigits@@</currency_decimal_digits>
    <length></length>
    <number_max>@@Global.key_currency.max@@</number_max>
    <number_min></number_min>
  </Column>
  <Column name="product_id">
    <name>商品ID</name>
    <necessary></necessary>
    <type></type>
    <length></length>
    <length_max></length_max>
    <length_min></length_min>
    <regexp ptn="半角英数字、「_」、「-」、「(」、「)」、「#」、「,」">^[a-zA-Z0-9_\-\(\)#\,]+$</regexp>
  </Column>
  <Column name="shipping_date_from">
    <name>配送希望日[From]</name>
    <necessary></necessary>
    <type></type>
    <length></length>
    <length_max></length_max>
    <length_min></length_min>
    <number_min>0</number_min>
    <regexp ptn="整数">^[\-|\+]?[0-9]*$</regexp>
  </Column>
  <Column name="shipping_date_to">
    <name>配送希望日[To]</name>
    <necessary></necessary>
    <type></type>
    <length></length>
    <length_max></length_max>
    <length_min></length_min>
    <number_min>0</number_min>
    <regexp ptn="整数">^[\-|\+]?[0-9]*$</regexp>
  </Column>
  <Column name="scheduled_shipping_date_from">
    <name>出荷予定日[From]</name>
    <type>HALFWIDTH_NUMBER</type>
    <number_min>0</number_min>
  </Column>
  <Column name="scheduled_shipping_date_to">
    <name>出荷予定日[To]</name>
    <type>HALFWIDTH_NUMBER</type>
    <number_min>0</number_min>
  </Column>
  <Column name="novelty_id">
    <name>ノベルティID</name>
    <necessary></necessary>
    <type></type>
    <length></length>
    <length_max></length_max>
    <length_min></length_min>
    <regexp ptn="半角英数字、「_」、「-」、「(」、「)」、「#」、「,」">^[a-zA-Z0-9_\-\(\)#\,]+$</regexp>
  </Column>
  <Column name="recommend_id">
    <name>レコメンドID</name>
    <necessary></necessary>
    <type></type>
    <length></length>
    <length_max></length_max>
    <length_min></length_min>
    <regexp ptn="半角英数字、「_」、「-」、「(」、「)」、「#」、「,」">^[a-zA-Z0-9_\-\(\)#\,]+$</regexp>
  </Column>
  <Column name="setpromotion_id">
    <name>セットプロモーションID</name>
    <necessary></necessary>
    <type></type>
    <length></length>
    <length_max></length_max>
    <length_min></length_min>
    <regexp ptn="半角英数字、「_」、「-」、「(」、「)」、「#」">^[a-zA-Z0-9_\-\(\)#]+$</regexp>
  </Column>
  <Column name="subscriptionBox_order_count_from">
    <name>頒布会注文回数[From]</name>
    <necessary></necessary>
    <type>HALFWIDTH_NUMBER</type>
    <length_max>8</length_max>
    <length_min></length_min>
  </Column>
  <Column name="subscriptionBox_order_count_to">
    <name>頒布会注文回数[To]</name>
    <necessary></necessary>
    <type>HALFWIDTH_NUMBER</type>
    <length_max>8</length_max>
    <length_min></length_min>
  </Column>
</OrderWorkflowSettingModify>