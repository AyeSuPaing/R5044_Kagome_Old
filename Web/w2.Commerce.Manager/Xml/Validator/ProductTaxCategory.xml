<ProductTaxCategory>
  <Column name="tax_category_id">
    <name>[ @@ 1 @@ ]の商品税率カテゴリID</name>
    <necessary>1</necessary>
    <type></type>
    <length></length>
    <length_max>30</length_max>
    <length_min></length_min>
    <regexp ptn="半角英数字、「_」">^[a-zA-Z0-9_]+$</regexp>
  </Column>
  <Column name="tax_category_name">
    <name>[ @@ 1 @@ ]の税率カテゴリ名</name>
    <necessary>1</necessary>
    <length_max>40</length_max>
  </Column>
  <Column name="tax_rate">
    <name>[ @@ 1 @@ ]の税率</name>
    <necessary>1</necessary>
    <type>TAX_RATE</type>
  </Column>
  <Column name="display_order">
    <name>[ @@ 1 @@ ]の表示順</name>
    <necessary>1</necessary>
    <type>HALFWIDTH_NUMBER</type>
    <number_max>999</number_max>
    <number_min>1</number_min>
  </Column>
</ProductTaxCategory>
