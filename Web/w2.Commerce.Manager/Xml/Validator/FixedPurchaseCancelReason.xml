<FixedPurchaseCancelReason>
  <Column name="cancel_reason_id">
    <name>[ No.@@ 1 @@ ] の解約理由区分ID</name>
    <necessary>1</necessary>
    <length_max>30</length_max>
    <regexp ptn="半角英数字、「_」、「-」、「(」、「)」、「#」">^[a-zA-Z0-9_\-\(\)#]+$</regexp>
  </Column>
  <Column name="cancel_reason_name">
    <name>[ No.@@ 1 @@ ] の解約理由区分名</name>
    <necessary>1</necessary>
    <length_max>100</length_max>
  </Column>
  <Column name="display_order">
    <name>[ No.@@ 1 @@ ] の表示順</name>
    <necessary>1</necessary>
    <type>HALFWIDTH_NUMBER</type>
    <number_max>9999999</number_max>
    <number_min>1</number_min>
  </Column>
</FixedPurchaseCancelReason>