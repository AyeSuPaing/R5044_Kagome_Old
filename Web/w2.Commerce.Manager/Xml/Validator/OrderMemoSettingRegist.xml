<OrderMemoSettingRegist>
  <Column name="order_memo_id">
    <name>注文メモID</name>
    <necessary>1</necessary>
    <type>HALFWIDTH_ALPHNUM</type>
    <length></length>
    <length_max>10</length_max>
    <length_min></length_min>
    <duplication page="OrderMemoSetting" statement="CheckDuplicationRegistOrderMemoId">1</duplication>
  </Column>
  <Column name="order_memo_name">
    <name>注文メモ名称</name>
    <necessary>1</necessary>
    <type></type>
    <length></length>
    <length_max>100</length_max>
  </Column>
  <Column name="height">
    <name>入力欄縦幅</name>
    <necessary></necessary>
    <type>HALFWIDTH_NUMBER</type>
    <length></length>
    <number_min>0</number_min>
    <number_max>999999999</number_max>
  </Column>
  <Column name="width">
    <name>入力欄横幅</name>
    <necessary></necessary>
    <type>HALFWIDTH_NUMBER</type>
    <length></length>
    <number_min>0</number_min>
    <number_max>999999999</number_max>
  </Column>
  <Column name="css_class">
    <name>CSS Class</name>
    <necessary></necessary>
    <type>HALFWIDTH_ALPHNUMSYMBOL</type>
    <length></length>
    <length_max>100</length_max>
  </Column>
  <Column name="max_length">
    <name>入力可能最大文字数</name>
    <necessary>1</necessary>
    <type>HALFWIDTH_NUMBER</type>
    <length></length>
    <number_min>1</number_min>
    <number_max>999999999</number_max>
  </Column>
  <Column name="default_text">
    <name>デフォルト文字列</name>
    <necessary></necessary>
    <type></type>
    <length></length>
    <length_max></length_max>
  </Column>
  <Column name="display_kbn">
    <name>表示区分</name>
    <necessary>1</necessary>
    <type>HALFWIDTH_ALPHNUM</type>
    <length></length>
    <length_max></length_max>
  </Column>
  <Column name="display_order">
    <name>表示順</name>
    <necessary>1</necessary>
    <type>HALFWIDTH_NUMBER</type>
    <length></length>
    <length_max>2</length_max>
    <number_min>1</number_min>
    <number_max>99</number_max>
  </Column>
  <Column name="term_bgn">
    <name>有効期間（開始）</name>
    <necessary>1</necessary>
    <type>DATE</type>
    <length></length>
    <length_max></length_max>
  </Column>
  <Column name="term_end">
    <name>有効期間（終了）</name>
    <necessary>1</necessary>
    <type>DATE</type>
    <length></length>
    <length_max></length_max>
  </Column>
  <Column name="valid_flg">
    <name>有効フラグ</name>
    <necessary>1</necessary>
    <type>HALFWIDTH_DECIMAL</type>
    <length></length>
    <length_max></length_max>
  </Column>
</OrderMemoSettingRegist>