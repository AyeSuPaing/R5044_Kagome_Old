<!--
  Uploadバッチ内でDirectory名をそのままTable名として使用するため
  Directory名とTable名は同一のものとして設定する
  ※w2_UserNotValidatorで実際の処理対象テーブルはw2_Userです
-->
<MasterUploadSetting>
<!--<Master>
    <Name>ターゲットリストデータ</Name>
    <Directory>w2_TargetListData</Directory>
  </Master>-->
  <Master>
    <Name>商品マスタ</Name>
    <Directory>w2_Product</Directory>
  </Master>
  <Master>
    <Name>商品バリエーションマスタ</Name>
    <Directory>w2_ProductVariation</Directory>
  </Master>
  <Master>
    <Name>会員価格マスタ</Name>
    <Directory>w2_ProductPrice</Directory>
  </Master>
  <Master>
    <Name>商品タグマスタ</Name>
    <Directory>w2_ProductTag</Directory>
  </Master>
  <Master>
    <Name>商品拡張項目マスタ</Name>
    <Directory>w2_ProductExtend</Directory>
  </Master>  
  <Master>
    <Name>商品カテゴリマスタ</Name>
    <Directory>w2_ProductCategory</Directory>
  </Master>
  <Master>
    <Name>在庫マスタ</Name>
    <Directory>w2_ProductStock</Directory>
  </Master>
  <Master>
    <Name>シリアルキーマスタ</Name>
    <Directory>w2_SerialKey</Directory>
  </Master>
  <Master>
    <Name>ユーザーマスタ（非入力チェック）</Name>
    <Directory>w2_UserNotValidator</Directory>
  </Master>
  <!-- ▽Web.config「ユーザーデータ移行利用有無」設定にて表示切替可▽ -->
  <Master>
    <Name>ユーザーマスタ（初期移行用）</Name>
    <Directory>w2_User</Directory>
  </Master>
  <Master>
    <Name>ユーザー拡張項目マスタ（初期移行用）</Name>
    <Directory>w2_UserExtend</Directory>
  </Master>
  <Master>
    <Name>ユーザー配送先マスタ</Name>
    <Directory>w2_UserShipping</Directory>
  </Master>
  <!-- △Web.config「ユーザーデータ移行利用有無」設定にて表示切替可△ -->
  <Master>
    <Name>商品レビュー</Name>
    <Directory>w2_ProductReview</Directory>
  </Master>
  <Master>
    <Name>商品セール価格マスタ</Name>
    <Directory>w2_ProductSalePrice</Directory>
  </Master>
  <!--<Master>
    <Name>ショートURL</Name>
    <Directory>w2_ShortUrl</Directory>
  </Master>-->
  <Master>
    <Name>商品画像削除</Name>
    <Directory>DeleteProductImage</Directory>
  </Master>
  <Master>
    <Name>リアル店舗マスタ</Name>
    <Directory>w2_RealShop</Directory>
  </Master>
  <Master>
    <Name>リアル店舗商品在庫マスタ</Name>
    <Directory>w2_RealShopProductStock</Directory>
  </Master>
  <Master>
    <Name>名称翻訳設定マスタ</Name>
    <Directory>w2_NameTranslationSetting</Directory>
  </Master>
  <Master>
    <Name>オペレータマスタ</Name>
    <Directory>w2_ShopOperator</Directory>
  </Master>
</MasterUploadSetting>
