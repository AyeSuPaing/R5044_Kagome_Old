<!--
  DataMigrationバッチ内でDirectory名をそのままTable名として使用するため
  Directory名とTable名は同一のものとして設定する
-->
<DataMigrationSetting>

  <Master>
    <Name>ユーザークレジットカードマスタ</Name>
    <Directory>w2_UserCreditCard</Directory>
  </Master>
  <Master>
    <Name>注文関連マスタ（削除のみ）</Name>
    <Directory>w2_Order</Directory>
  </Master>
  <Master>
    <Name>定期購入マスタ</Name>
    <Directory>w2_FixedPurchase</Directory>
  </Master>
  <Master>
    <Name>定期購入商品マスタ</Name>
    <Directory>w2_FixedPurchaseItem</Directory>
  </Master>
  <Master>
    <Name>定期購入配送先マスタ</Name>
    <Directory>w2_FixedPurchaseShipping</Directory>
  </Master>
  <Master>
    <Name>定期購入履歴マスタ</Name>
    <Directory>w2_FixedPurchaseHistory</Directory>
  </Master>

</DataMigrationSetting>
