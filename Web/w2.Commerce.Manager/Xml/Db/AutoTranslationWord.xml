<AutoTranslationWord>

  <!-- 自動翻訳ワード重複チェック -->
  <CheckDuplicationAutoTranslationWordRegister>
    <Statement>
      <![CDATA[
        SELECT  COUNT(word_hash_key)
          FROM  w2_AutoTranslationWord
         WHERE  w2_AutoTranslationWord.word_hash_key = @word_hash_key
           AND  w2_AutoTranslationWord.language_code = @language_code
      ]]>
    </Statement>
    <Parameter>
      <Input Name="word_hash_key" Type="nvarchar" />
      <Input Name="language_code" Type="nvarchar" />
    </Parameter>
  </CheckDuplicationAutoTranslationWordRegister>

</AutoTranslationWord>