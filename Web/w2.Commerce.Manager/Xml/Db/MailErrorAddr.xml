<MailErrorAddr>
  <!-- エラーポイントクリア -->
  <DeleteMailErrorAddr>
    <Statement>
      <![CDATA[
      DELETE  w2_MailErrorAddr
      WHERE
          w2_MailErrorAddr.mail_addr = @mail_addr
      ]]>
    </Statement>
    <Parameter>
      <Input Name="mail_addr" Type="nvarchar" Size="256" />
    </Parameter>
  </DeleteMailErrorAddr>
</MailErrorAddr>