<ShopShipping_Sub>

  <!-- 店舗配送料情報一覧取得用WHERE文 -->
  <SHOPSHIPPING_SEARCH_WHERE>
    <Statement>
      <![CDATA[
         WHERE  w2_ShopShipping.shop_id = @shop_id
      ]]>
    </Statement>
  </SHOPSHIPPING_SEARCH_WHERE>

  <!-- 店舗配送料情報一覧取得用ORDER_BY -->
  <SHOPSHIPPING_SEARCH_ORDER_BY>
    <Statement>
      <![CDATA[
        ORDER BY
          -- 常に一定の結果セットを得るため、主キー（昇順）ソート
          w2_ShopShipping.shop_id ASC, w2_ShopShipping.shipping_id ASC
      ]]>
    </Statement>
  </SHOPSHIPPING_SEARCH_ORDER_BY>

</ShopShipping_Sub>