<CoordinatePageModify>
  <Column name="coordinate_title">
    <name>フロント/管理用ページ名</name>
    <necessary>1</necessary>
    <type></type>
    <length></length>
    <length_max>200</length_max>
    <length_min></length_min>
  </Column>
  <Column name="staff_id">
    <name>スタッフ</name>
    <type></type>
    <length></length>
    <length_max>30</length_max>
    <length_min></length_min>
  </Column>
  <Column name="real_shop_id">
    <name>店舗名</name>
    <type></type>
    <length></length>
    <length_max>30</length_max>
    <length_min></length_min>
  </Column>
  <Column name="display_date1">
    <name>公開日時(日付)</name>
    <necessary>1</necessary>
    <type>DATE</type>
    <length></length>
    <length_max></length_max>
    <length_min></length_min>
  </Column>
  <Column name="display_date2">
    <name>公開日時(時間)</name>
    <necessary>1</necessary>
    <type></type>
    <regexp ptn="正しい時間(00:00～23:59)">([0-1][0-9]|2[0-3]):[0-5][0-9]</regexp>
    <length></length>
    <length_max></length_max>
    <length_min></length_min>
  </Column>
</CoordinatePageModify>