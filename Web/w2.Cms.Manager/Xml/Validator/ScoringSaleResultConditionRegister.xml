  <Column name="score_axis_axis_value_to">
    <name>スコア軸加算値(To)</name>
    <necessary>1</necessary>
    <type>HALFWIDTH_INT</type>
    <number_min>-999</number_min>
    <number_max>999</number_max>
  </Column>
