<ProductRankingModity>
  <Column name="ranking_id">
    <name>商品ランキングID</name>
    <necessary>1</necessary>
    <type>HALFWIDTH_ALPHNUM</type>
    <length></length>
    <length_max>10</length_max>
    <length_min></length_min>
  </Column>
  <Column name="total_type">
    <name>集計タイプ</name>
    <necessary>1</necessary>
    <type>HALFWIDTH_ALPHNUM</type>
    <length></length>
    <length_max></length_max>
    <length_min></length_min>
  </Column>
  <Column name="total_kbn">
    <name>集計期間区分</name>
    <necessary>1</necessary>
    <type>HALFWIDTH_ALPHNUM</type>
    <length></length>
    <length_max>30</length_max>
    <length_min></length_min>
  </Column>
  <Column name="total_from">
    <name>集計期間(FROM)</name>
    <necessary>1</necessary>
    <type>HALFWIDTH_DATE</type>
    <length></length>
    <length_max></length_max>
    <length_min></length_min>
  </Column>
  <Column name="total_to">
    <name>集計期間(TO)</name>
    <necessary>1</necessary>
    <type>HALFWIDTH_DATE</type>
    <length></length>
    <length_max>30</length_max>
    <length_min></length_min>
  </Column>
  <Column name="total_days">
    <name>集計日数</name>
    <necessary>1</necessary>
    <type>HALFWIDTH_NUMBER</type>
    <length></length>
    <length_max></length_max>
    <length_min></length_min>
    <number_max>999</number_max>
    <number_min>1</number_min>
  </Column>
  <Column name="category_id">
    <name>カテゴリID</name>
    <necessary></necessary>
    <type></type>
    <length></length>
    <length_max>30</length_max>
    <length_min>3</length_min>
  </Column>
  <Column name="desc1">
    <name>説明文</name>
    <necessary></necessary>
    <type></type>
    <length></length>
    <length_max>250</length_max>
    <length_min></length_min>
  </Column>
  <Column name="stock_kbn">
    <name>在庫切れ商品表示区分</name>
    <necessary>1</necessary>
    <type>HALFWIDTH_NUMBER</type>
    <length></length>
    <length_max></length_max>
    <length_min></length_min>
  </Column>
  <Column name="exclude_category_ids">
    <name>カテゴリ除外ID</name>
    <necessary></necessary>
    <type></type>
    <length></length>
    <length_max></length_max>
    <length_min>3</length_min>
  </Column>
</ProductRankingModity>
