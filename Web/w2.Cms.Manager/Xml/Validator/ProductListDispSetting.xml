<ProductListDispSetting>
  <Column name="setting_name">
    <name>「@@ 1 @@」の表示名</name>
    <necessary>1</necessary>
    <length_max>50</length_max>
  </Column>
  <Column name="disp_no">
    <name>「@@ 1 @@」の表示順</name>
    <necessary>1</necessary>
    <type>HALFWIDTH_NUMBER</type>
    <length></length>
    <length_max>2</length_max>
    <length_min></length_min>
  </Column>
  <Column name="disp_count">
    <name>「@@ 1 @@」の件数</name>
    <type>HALFWIDTH_NUMBER</type>
    <necessary>1</necessary>
    <number_max>99</number_max>
    <length_max>3</length_max>
  </Column>
</ProductListDispSetting>