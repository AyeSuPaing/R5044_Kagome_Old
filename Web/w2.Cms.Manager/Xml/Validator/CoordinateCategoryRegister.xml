<CoordinateCategoryRegister>
  <Column name="coordinate_category_name">
    <name>カテゴリ名</name>
    <necessary>1</necessary>
    <type></type>
    <length></length>
    <length_max>40</length_max>
    <length_min></length_min>
    <duplication page="CoordinateCategory" statement="CheckDuplicationCategoryName">1</duplication>
  </Column>
  <Column name="coordinate_category_id">
    <name>カテゴリID</name>
    <necessary></necessary>
    <type></type>
    <length></length>
    <length_max>30</length_max>
    <length_min></length_min>
    <duplication page="CoordinateCategory" statement="CheckDuplicationCategoryId">1</duplication>
  </Column>
  <Column name="formatted_coordinate_category_id">
    <name>カテゴリID</name>
    <necessary>1</necessary>
    <type>HALFWIDTH_ALPHNUM</type>
    <length>3</length>
    <length_max></length_max>
    <length_min></length_min>
  </Column>
  <Column name="seo_keywords">
    <name>SEOキーワード</name>
    <necessary></necessary>
    <type></type>
    <length></length>
    <length_max>200</length_max>
    <length_min></length_min>
  </Column>
  <Column name="display_order">
    <name>表示順</name>
    <necessary>1</necessary>
    <type>HALFWIDTH_NUMBER</type>
    <length></length>
    <length_max>8</length_max>
    <length_min></length_min>
  </Column>
</CoordinateCategoryRegister>