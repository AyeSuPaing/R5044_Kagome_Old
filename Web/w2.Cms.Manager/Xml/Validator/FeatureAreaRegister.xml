<FeatureAreaRegister>
  <Column name="area_id">
    <name>特集エリアID</name>
    <necessary>1</necessary>
    <type>HALFWIDTH_ALPHNUM</type>
    <length></length>
    <length_max>30</length_max>
    <length_min></length_min>
    <duplication page="FeatureArea" statement="CheckDuplicationRegist">1</duplication>
  </Column>
  <Column name="area_name">
    <name>特集エリア名</name>
    <necessary>1</necessary>
    <type></type>
    <length></length>
    <length_max>30</length_max>
    <length_min></length_min>
  </Column>
  <Column name="area_type_id">
    <name>特集エリアタイプ</name>
    <necessary>1</necessary>
    <type></type>
    <length></length>
    <length_max>100</length_max>
    <length_min></length_min>
  </Column>
  <Column name="internal_memo">
    <name>内部用メモ</name>
    <necessary></necessary>
    <type></type>
    <length></length>
    <length_max>100</length_max>
    <length_min></length_min>
  </Column>
  <Column name="use_type">
    <name>サイト</name>
    <necessary>1</necessary>
    <type></type>
    <length></length>
    <length_max>20</length_max>
    <length_min></length_min>
  </Column>
</FeatureAreaRegister>