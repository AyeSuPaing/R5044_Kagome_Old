<FeatureArea>

  <!-- 重複チェック（特集エリア登録時） -->
  <CheckDuplicationRegist>
    <Statement>
      <![CDATA[
        SELECT  COUNT(*)
          FROM  w2_FeatureArea
         WHERE  area_id = @area_id
      ]]>
    </Statement>
    <Parameter>
      <Input Name="area_id" Type="nvarchar" Size="30" />
    </Parameter>
  </CheckDuplicationRegist>

  <!-- 重複チェック（特集エリアタイプ登録時） -->
  <CheckDuplicationTypeRegist>
    <Statement>
      <![CDATA[
        SELECT  COUNT(*)
          FROM  w2_FeatureAreaType
         WHERE  area_type_id = @area_type_id
      ]]>
    </Statement>
    <Parameter>
      <Input Name="area_type_id" Type="nvarchar" Size="30" />
    </Parameter>
  </CheckDuplicationTypeRegist>

</FeatureArea>