<!--
  Uploadバッチ内でDirectory名をそのままTable名として使用するため
  Directory名とTable名は同一のものとして設定する
  ※w2_UserNotValidatorで実際の処理対象テーブルはw2_Userです
-->
<MasterUploadSetting>
  <Master>
    <Name>ショートURL</Name>
    <Directory>w2_ShortUrl</Directory>
  </Master>
  <Master>
    <Name>スタッフ</Name>
    <Directory>w2_Staff</Directory>
  </Master>
  <Master>
    <Name>コーディネートカテゴリ</Name>
    <Directory>w2_CoordinateCategory</Directory>
  </Master>
  <Master>
    <Name>コーディネート</Name>
    <Directory>w2_Coordinate</Directory>
  </Master>
  <Master>
    <Name>コーディネートアイテム</Name>
    <Directory>w2_CoordinateItem</Directory>
  </Master>
</MasterUploadSetting>
