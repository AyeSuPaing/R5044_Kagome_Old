<!-- 【注意:金額を保持するカラムには必ず[type="price"]を設定してください。】 -->
<MasterExportSetting>

  <ShortUrl>
    <!--Field jname="ショートURL NO" name="surl_no" field="w2_ShortUrl.surl_no" /-->
    <Field jname="店舗ID" name="shop_id" field="w2_ShortUrl.shop_id" />
    <Field jname="ショートURL" name="short_url" field="w2_ShortUrl.short_url" />
    <Field jname="ロングURL" name="long_url" field="w2_ShortUrl.long_url" />
    <!--Field jname="作成日" name="date_created" field="w2_ShortUrl.date_created" /-->
    <!--Field jname="更新日" name="date_changed" field="w2_ShortUrl.date_changed" /-->
    <!--Field jname="最終更新者" name="last_changed" field="w2_ShortUrl.last_changed" /-->
  </ShortUrl>

  <Coordinate>
    <Field jname="コーディネートID" name="coordinate_id" field="w2_Coordinate.coordinate_id" />
    <Field jname="コーディネートタイトル" name="coordinate_title" field="w2_Coordinate.coordinate_title" />
    <Field jname="コーディネートURL" name="coordinate_url" field="w2_Coordinate.coordinate_url" />
    <Field jname="コーディネート概要" name="coordinate_summary" field="w2_Coordinate.coordinate_summary" />
    <Field jname="内部用メモ" name="internal_memo" field="w2_Coordinate.internal_memo" />
    <Field jname="スタッフID" name="staff_id" field="w2_Coordinate.staff_id" />
    <Field jname="リアル店舗ID" name="real_shop_id" field="w2_Coordinate.real_shop_id" />
    <Field jname="タイトル" name="html_title" field="w2_Coordinate.html_title" />
    <Field jname="ディスクリプション" name="metadata_desc" field="w2_Coordinate.metadata_desc" />
    <Field jname="表示区分" name="display_kbn" field="w2_Coordinate.display_kbn" />
    <Field jname="表示用更新日" name="display_date" field="w2_Coordinate.display_date" type="datetime" />
  </Coordinate>

  <CoordinateItem>
    <Field jname="コーディネートID" name="coordinate_id" field="w2_CoordinateItem.coordinate_id" />
    <Field jname="アイテムNO" name="item_no" field="w2_CoordinateItem.item_no" />
    <Field jname="アイテム区分" name="item_kbn" field="w2_CoordinateItem.item_kbn" />
    <Field jname="アイテムID" name="item_id" field="w2_CoordinateItem.item_id" />
    <Field jname="アイテムID2" name="item_id2" field="w2_CoordinateItem.item_id2" />
    <Field jname="アイテム名" name="item_name" field="w2_CoordinateItem.item_name" />
  </CoordinateItem>

  <CoordinateCategory>
    <Field jname="カテゴリID" name="coordinate_category_id" field="w2_CoordinateCategory.coordinate_category_id" />
    <Field jname="親カテゴリID" name="coordinate_parent_category_id" field="w2_CoordinateCategory.coordinate_parent_category_id" />
    <Field jname="カテゴリ名" name="coordinate_category_name" field="w2_CoordinateCategory.coordinate_category_name" />
    <Field jname="SEOキーワード" name="seo_keywords" field="w2_CoordinateCategory.seo_keywords" />
    <Field jname="表示順" name="display_order" field="w2_CoordinateCategory.display_order" />
    <Field jname="有効フラグ" name="valid_flg" field="w2_CoordinateCategory.valid_flg" />
  </CoordinateCategory>

  <Staff>
    <Field jname="スタッフID" name="staff_id" field="w2_Staff.staff_id" />
    <Field jname="氏名" name="staff_name" field="w2_Staff.staff_name" />
    <Field jname="プロフィールテキスト" name="staff_profile" field="w2_Staff.staff_profile" />
    <Field jname="身長" name="staff_height" field="w2_Staff.staff_height" />
    <Field jname="インスタグラムID" name="staff_instagram_id" field="w2_Staff.staff_instagram_id" />
    <Field jname="性別" name="staff_sex" field="w2_Staff.staff_sex" />
    <Field jname="モデルフラグ" name="model_flg" field="w2_Staff.model_flg" />
    <Field jname="オペレータID" name="operator_id" field="w2_Staff.operator_id" />
    <Field jname="リアル店舗ID" name="real_shop_id" field="w2_Staff.real_shop_id" />
    <Field jname="有効フラグ" name="valid_flg" field="w2_Staff.valid_flg" />
  </Staff>

</MasterExportSetting>
