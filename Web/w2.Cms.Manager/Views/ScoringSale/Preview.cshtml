@using w2.App.Common.Web.Page
@using w2.Cms.Manager.Codes.Common
@{
	ViewBag.Title = CommonPage.ReplaceTag("@@ScoringSale.scoring_sales_information_title.name@@");
}

@model w2.Cms.Manager.ViewModels.ScoringSale.PreviewViewModel
@WebMessages.PreviewIsLoadingMessage
<script type="text/javascript">
	setTimeout(redirect_preview, 2000);

	function redirect_preview() {
		var url = '@Model.PreviewUrl';
		location.href = url.replace(/&amp;/g, '&') + '&reset=1';
	}
</script>
