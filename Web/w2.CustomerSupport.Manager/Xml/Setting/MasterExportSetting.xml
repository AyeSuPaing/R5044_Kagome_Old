<MasterExportSetting>
  <CsMessage>
    <!-- メッセージ -->
    <Field jname="識別ID" name="dept_id" field="w2_CsMessage.dept_id" />
    <Field jname="インシデントID" name="incident_id" field="w2_CsMessage.incident_id" />
    <Field jname="メッセージ番号" name="message_no" field="w2_CsMessage.message_no" />
    <Field jname="問合せ媒体" name="media_kbn" field="w2_CsMessage.media_kbn" />
    <Field jname="受発信区分" name="direction_kbn" field="w2_CsMessage.direction_kbn"  />
    <Field jname="作成オペレータID" name="operator_id" field="w2_CsMessage.operator_id" />
    <Field jname="問合せ・回答日時" name="inquiry_date" field="w2_CsMessage.inquiry_reply_date" />
    <Field jname="メッセージステータス" name="message_status" field="w2_CsMessage.message_status" />
    <Field jname="顧客氏名1" name="user_name1" field="w2_CsMessage.user_name1" />
    <Field jname="顧客氏名2" name="user_name2" field="w2_CsMessage.user_name2" />
    <Field jname="顧客氏名かな1" name="user_name_kana1" field="w2_CsMessage.user_name_kana1" />
    <Field jname="顧客氏名かな2" name="user_name_kana2" field="w2_CsMessage.user_name_kana2" />
    <Field jname="顧客メールアドレス" name="user_mail_addr" field="w2_CsMessage.user_mail_addr" />
    <Field jname="顧客電話番号1" name="user_tel1" field="w2_CsMessage.user_tel1" />
    <Field jname="問合せ件名" name="inquiry_title" field="w2_CsMessage.inquiry_title" />
    <Field jname="問合せ内容" name="inquiry_text" field="w2_CsMessage.inquiry_text" />
    <Field jname="回答内容" name="reply_text" field="w2_CsMessage.reply_text" />
    <Field jname="回答オペレータID" name="reply_operator_id" field="w2_CsMessage.reply_operator_id" />
    <Field jname="受信メール識別ID" name="receive_mail_id" field="w2_CsMessage.receive_mail_id" />
    <Field jname="送信メール識別ID" name="send_mail_id" field="w2_CsMessage.send_mail_id" />
    <Field jname="有効フラグ" name="valid_flg" field="w2_CsMessage.valid_flg" />
    <Field jname="作成日" name="date_created" field="w2_CsMessage.date_created" />
    <Field jname="更新日" name="date_changed" field="w2_CsMessage.date_changed" />
    <Field jname="最終更新者" name="last_changed" field="w2_CsMessage.last_changed" />
    <!-- インシデント -->
    <Field jname="識別ID" name="incident_dept_id" field="w2_CsIncident.dept_id" Type="nvarchar" />
    <Field jname="インシデントID" name="incident_incident_id" field="w2_CsIncident.incident_id" Type="nvarchar" />
    <Field jname="ユーザーID" name="incident_user_id" field="w2_CsIncident.user_id" Type="nvarchar" />
    <Field jname="集計区分" name="incident_summary_value" field="STUFF((SELECT ',' + w2_CsIncidentSummaryValue.value FROM w2_CsIncidentSummaryValue WHERE w2_CsIncidentSummaryValue.dept_id = w2_CsIncident.dept_id AND w2_CsIncidentSummaryValue.incident_id = w2_CsIncident.incident_id ORDER BY w2_CsIncidentSummaryValue.summary_no FOR XML PATH (''), TYPE).value('.', 'NVARCHAR(MAX)'), 1, 1, '') AS incident_summary_value" />
    <Field jname="インシデントカテゴリID" name="incident_incident_category_id" field="w2_CsIncident.incident_category_id" Type="nvarchar" />
    <Field jname="インシデント件名" name="incident_incident_title" field="w2_CsIncident.incident_title" Type="nvarchar" />
    <Field jname="ステータス" name="incident_status" field="w2_CsIncident.status" Type="nvarchar" />
    <Field jname="VOCID" name="incident_voc_id" field="w2_CsIncident.voc_id" Type="nvarchar" />
    <Field jname="VOCメモ" name="incident_voc_memo" field="w2_CsIncident.voc_memo" Type="nvarchar" />
    <Field jname="コメント" name="incident_comment" field="w2_CsIncident.comment" Type="nvarchar" />
    <Field jname="重要度" name="incident_importance" field="w2_CsIncident.importance" Type="nvarchar" />
    <Field jname="問合せ元名称" name="incident_user_name" field="w2_CsIncident.user_name" Type="nvarchar" />
    <Field jname="問合せ元連絡先" name="incident_user_contact" field="w2_CsIncident.user_contact" Type="nvarchar" />
    <Field jname="担当グループ" name="incident_cs_group_id" field="w2_CsIncident.cs_group_id" Type="nvarchar" />
    <Field jname="担当オペレータ" name="incident_operator_id" field="w2_CsIncident.operator_id" Type="nvarchar" />
    <Field jname="ロック状態" name="incident_lock_status" field="w2_CsIncident.lock_status" Type="nvarchar" />
    <Field jname="ロック保持者" name="incident_lock_operator_id" field="w2_CsIncident.lock_operator_id" Type="nvarchar" />
    <Field jname="有効フラグ" name="incident_valid_flg" field="w2_CsIncident.valid_flg" Type="nvarchar" />
    <Field jname="対応完了日" name="incident_date_completed" field="w2_CsIncident.date_completed" Type="datetime" />
    <Field jname="作成日" name="incident_date_created" field="w2_CsIncident.date_created" />
    <Field jname="更新日" name="incident_date_changed" field="w2_CsIncident.date_changed" />
    <Field jname="最終更新者" name="incident_last_changed" field="w2_CsIncident.last_changed" Type="nvarchar" />
    <!-- カテゴリ -->
    <Field jname="インシデントカテゴリ名" name="incident_category_name" field="w2_CsIncidentCategory.category_name" Type="nvarchar" />
  </CsMessage>
</MasterExportSetting>
