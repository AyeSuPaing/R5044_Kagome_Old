<MailErrorAddr>
  <!-- 取得 -->
  <Get>
    <Statement>
      <![CDATA[
        SELECT  w2_MailErrorAddr.* 
          FROM  w2_MailErrorAddr
         WHERE  w2_MailErrorAddr.mail_addr = @mail_addr
      ]]>
    </Statement>
    <Parameter>
      <Input Name="mail_addr" Type="nvarchar" Size="256" />
    </Parameter>
  </Get>
</MailErrorAddr>
