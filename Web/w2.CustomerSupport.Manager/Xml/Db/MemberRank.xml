<MemberRank>

  <!-- 会員ランクリスト取得 -->
  <GetMemberRankList>
    <Statement>
      <![CDATA[
        SELECT  w2_MemberRank.*
          FROM  w2_MemberRank
         WHERE  valid_flg = 'ON'  -- 有効
        ORDER BY w2_MemberRank.member_rank_order
      ]]>
    </Statement>
    <Parameter>
    </Parameter>
  </GetMemberRankList>

  <!-- 会員ランク詳細情報取得 -->
  <GetMemberRankDetail>
    <Statement>
      <![CDATA[
        SELECT  w2_MemberRank.*
          FROM  w2_MemberRank
         WHERE  w2_MemberRank.member_rank_id = @member_rank_id
      ]]>
    </Statement>
    <Parameter>
      <Input Name="member_rank_id" Type="nvarchar" Size="30" />
    </Parameter>
  </GetMemberRankDetail>
  
</MemberRank>