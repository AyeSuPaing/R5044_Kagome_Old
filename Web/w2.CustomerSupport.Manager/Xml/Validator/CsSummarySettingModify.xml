<CsSummarySettingModify>
  <Column name="dept_id">
    <name>識別ID</name>
    <necessary>1</necessary>
    <type>HALFWIDTH_ALPHNUM</type>
    <length></length>
    <length_max>30</length_max>
    <length_min>1</length_min>
  </Column>
  <Column name="summary_setting_no">
    <name>集計区分No</name>
    <necessary>1</necessary>
    <type>HALFWIDTH_NUMBER</type>
    <length></length>
    <length_max>3</length_max>
    <length_min></length_min>
  </Column>
  <Column name="summary_setting_title">
    <name>集計区分名</name>
    <necessary>1</necessary>
    <type></type>
    <length></length>
    <length_max>30</length_max>
    <length_min></length_min>
  </Column>
  <Column name="summary_setting_type">
    <name>入力タイプ</name>
    <necessary>1</necessary>
    <type></type>
    <length></length>
    <length_max></length_max>
    <length_min></length_min>
  </Column>
  <Column name="display_order">
    <name>表示順</name>
    <necessary>1</necessary>
    <type>HALFWIDTH_NUMBER</type>
    <length></length>
    <length_max></length_max>
  </Column>	
  <Column name="valid_flg">
    <name>有効フラグ</name>
    <necessary>1</necessary>
    <type>HALFWIDTH_NUMBER</type>
    <length>1</length>
    <length_max></length_max>
  </Column>
</CsSummarySettingModify>