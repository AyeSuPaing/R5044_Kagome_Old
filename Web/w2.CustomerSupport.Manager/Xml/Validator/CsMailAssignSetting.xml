<CsMailAssignSetting>
  <Column name="matching_value">
    <name>振分け条件</name>
    <necessary>1</necessary>
    <type></type>
    <length></length>
    <length_max>100</length_max>
    <length_min></length_min>
  </Column>
  <Column name="assign_priority">
    <name>優先順</name>
    <necessary>1</necessary>
    <type>HALFWIDTH_NUMBER</type>
    <length></length>
    <length_max>5</length_max>
    <number_max>999</number_max>
    <number_min>1</number_min>
  </Column>
  <Column name="auto_response_from">
    <name>差出人From</name>
    <necessary>1</necessary>
    <type>MAILADDRESS</type>
    <length></length>
    <length_max>256</length_max>
    <length_min></length_min>
  </Column>
  <Column name="auto_response_cc">
    <name>送信先Cc</name>
    <necessary></necessary>
    <type></type>
    <length></length>
    <length_max></length_max>
    <length_min></length_min>
  </Column>
  <Column name="auto_response_bcc">
    <name>送信先Bcc</name>
    <necessary></necessary>
    <type></type>
    <length></length>
    <length_max></length_max>
    <length_min></length_min>
  </Column>
  <Column name="auto_response_subject">
    <name>件名</name>
    <necessary>1</necessary>
    <type></type>
    <length></length>
    <length_max>256</length_max>
    <length_min></length_min>
  </Column>
  <Column name="auto_response_body">
    <name>本文</name>
    <necessary>1</necessary>
    <type></type>
    <length></length>
    <length_max></length_max>
    <length_min></length_min>
  </Column>
  <Column name="mail_assign_name">
    <name>振分設定名</name>
    <necessary>1</necessary>
    <length_max>256</length_max>
  </Column>
</CsMailAssignSetting>