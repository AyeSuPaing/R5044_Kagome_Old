    <type>HALFWIDTH_ALPHNUMSYMBOL</type>
    <length></length>
    <length_max>30</length_max>
  </Column>
  <Column name="summary_setting_item_text">
    <name>@@ 1 @@表示文言</name>
    <necessary>1</necessary>
    <type></type>
    <length></length>
    <length_max>50</length_max>
  </Column>	
</CsSummarySettingItemRegister>