<CsExternalLinkRegister>
	<Column name="dept_id">
		<name>識別ID</name>
		<necessary>1</necessary>
		<type>HALFWIDTH_ALPHNUM</type>
		<length></length>
		<length_max>30</length_max>
		<length_min>1</length_min>
	</Column>
  <Column name="link_id">
    <name>リンクID</name>
    <necessary></necessary>
    <type>HALFWIDTH_ALPHNUM</type>
    <length></length>
    <length_max></length_max>
    <length_min>1</length_min>
  </Column>
  <Column name="link_title">
		<name>リンクタイトル</name>
		<necessary>1</necessary>
		<type></type>
		<length></length>
		<length_max>20</length_max>
		<length_min>1</length_min>
	</Column>
  <Column name="link_url">
    <name>リンクURL</name>
    <necessary>1</necessary>
    <type>HALFWIDTH</type>
    <length></length>
    <length_max></length_max>
    <length_min>1</length_min>
	</Column>
  <Column name="link_memo">
    <name>管理メモー</name>
    <necessary>0</necessary>
    <type></type>
    <length></length>
    <length_max></length_max>
  </Column>
	<Column name="display_order">
		<name>表示順</name>
		<necessary>1</necessary>
		<type>HALFWIDTH_NUMBER</type>
		<length></length>
		<length_max></length_max>
	</Column>	
	<Column name="valid_flg">
		<name>有効フラグ</name>
		<necessary>1</necessary>
		<type>HALFWIDTH_NUMBER</type>
		<length>1</length>
		<length_max></length_max>
	</Column>
</CsExternalLinkRegister>
