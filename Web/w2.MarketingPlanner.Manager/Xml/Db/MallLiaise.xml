<MallLiaise>

  <!-- モール連携設定一覧取得（ショップIDより） -->
  <GetMallCooperationSettingListFromShopId>
    <Statement>
      <![CDATA[
        SELECT  w2_MallCooperationSetting.*
          FROM  w2_MallCooperationSetting
         WHERE  shop_id = @shop_id
      ]]>
    </Statement>
    <Parameter>
      <Input Name="shop_id" Type="nvarchar" Size="10" />
    </Parameter>
  </GetMallCooperationSettingListFromShopId>

</MallLiaise>
