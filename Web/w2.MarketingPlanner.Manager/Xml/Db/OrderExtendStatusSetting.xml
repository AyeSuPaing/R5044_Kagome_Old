<OrderExtendStatusSetting>
  
  <!-- 注文拡張ステータス設定取得 -->
  <GetOrderExtendStatusSetting>
    <Statement>
      <![CDATA[
        -- 利用可能枝番までを取得。
        SELECT  w2_OrderExtendStatusSetting.*
          FROM  w2_OrderExtendStatusSetting
         WHERE  w2_OrderExtendStatusSetting.shop_id = @shop_id
                AND
                w2_OrderExtendStatusSetting.extend_status_no <= @extend_status_max
      ]]>
    </Statement>
    <Parameter>
      <Input Name="shop_id" Type="nvarchar" Size="10" />
      <Input Name="extend_status_max" Type="int" />
    </Parameter>
  </GetOrderExtendStatusSetting>
  
</OrderExtendStatusSetting>