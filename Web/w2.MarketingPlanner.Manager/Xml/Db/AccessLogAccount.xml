<AccessLogAccount>
  <GetAccountId>
    <Statement>
      <![CDATA[
      SELECT  w2_AccessLogAccount.account_id
        FROM  w2_AccessLogAccount
       WHERE  w2_AccessLogAccount.dept_id = @dept_id
      ]]>
    </Statement>
    <Parameter>
      <Input Name="dept_id" Type="nvarchar" Size="30" />
    </Parameter>
  </GetAccountId>
</AccessLogAccount>
