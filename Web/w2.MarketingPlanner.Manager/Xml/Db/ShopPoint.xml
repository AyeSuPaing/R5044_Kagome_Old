<ShopPoint>
  <!-- ポイントマスタ取得 -->
  <GetShopPoint>
    <Statement>
      <![CDATA[
        SELECT  * 
          FROM  w2_Point 
         WHERE  dept_id = @dept_id 
           AND  point_kbn = @point_kbn 
      ]]>
    </Statement>
    <Parameter>
      <Input Name="dept_id" Type="nvarchar" Size="30" />
      <Input Name="point_kbn" Type="nvarchar" Size="10" />
    </Parameter>
  </GetShopPoint>
</ShopPoint>