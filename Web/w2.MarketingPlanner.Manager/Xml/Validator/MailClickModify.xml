<MailClickModify>

  <Column name="mailclick_name">
    <name>メールクリック名</name>
    <necessary>1</necessary>
    <type>FULLWIDTH</type>
    <length></length>
    <length_max>30</length_max>
    <length_min></length_min>
  </Column>
  <Column name="mailclick_key">
    <name>メールクリック識別キー</name>
    <necessary>1</necessary>
    <type></type>
    <length></length>
    <length_max>10</length_max>
    <length_min></length_min>
    <duplication page="MailClick" statement="CheckDuplicationModifyMailClickKey">1</duplication>
  </Column>
  <Column name="pc_redirect_url">
    <name>PC遷移先URL</name>
    <necessary></necessary>
    <type>HALFWIDTH_ALPHNUMSYMBOL</type>
    <length></length>
    <length_max></length_max>
    <length_min></length_min>
  </Column>
  <Column name="mobile_redirect_url">
    <name>モバイル遷移先URL</name>
    <necessary></necessary>
    <type>HALFWIDTH_ALPHNUMSYMBOL</type>
    <length></length>
    <length_max></length_max>
    <length_min></length_min>
  </Column>
  <Column name="redirect_url">
    <name>遷移先URL</name>
    <necessary>1</necessary>
    <type>HALFWIDTH_ALPHNUMSYMBOL</type>
    <length></length>
    <length_max></length_max>
    <length_min></length_min>
  </Column>

</MailClickModify>