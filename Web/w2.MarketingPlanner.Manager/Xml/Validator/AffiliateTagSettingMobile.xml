<AffiliateTagSettingMobile>
  <Column name="affiliate_name">
    <name>アフィリエイト名</name>
    <necessary>1</necessary>
    <type></type>
    <length></length>
    <length_max>100</length_max>
    <length_min></length_min>
  </Column>
  <Column name="display_order">
    <name>表示順</name>
    <necessary>1</necessary>
    <type>HALFWIDTH_NUMBER</type>
    <length></length>
    <length_max>2</length_max>
    <length_min></length_min>
    <number_max>99</number_max>
    <number_min>1</number_min>
  </Column>
  <Column name="session_name1">
    <name>セッション変数名1</name>
    <necessary>1</necessary>
    <type>HALFWIDTH_ALPHNUMSYMBOL</type>
    <length></length>
    <length_max>50</length_max>
    <length_min></length_min>
  </Column>
  <Column name="session_name2">
    <name>セッション変数名2</name>
    <necessary></necessary>
    <type>HALFWIDTH_ALPHNUMSYMBOL</type>
    <length></length>
    <length_max>50</length_max>
    <length_min></length_min>
    <equivalence target_name="セッション変数名1">session_name1</equivalence>
  </Column>
  <Column name="user_agent_coop_kbn">
    <name>ユーザーエージェント指定</name>
    <necessary>1</necessary>
    <type></type>
    <length></length>
    <length_max></length_max>
    <length_min></length_min>
  </Column>
</AffiliateTagSettingMobile>