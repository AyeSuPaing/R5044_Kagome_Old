<MailDistSettingRegist>
  <Column name="maildist_name">
    <name>メール配信設定名</name>
    <necessary>1</necessary>
    <type></type>
    <length></length>
    <length_max>30</length_max>
    <length_min></length_min>
  </Column>
  <Column name="target_ids">
    <name>ターゲット</name>
    <necessary>1</necessary>
    <type></type>
    <length></length>
    <length_max></length_max>
    <length_min></length_min>
  </Column>
  <Column name="mailtext_id">
    <name>メール文章</name>
    <necessary>1</necessary>
    <type></type>
    <length></length>
    <length_max></length_max>
    <length_min></length_min>
  </Column>
  <Column name="except_error_point">
    <name>メール配信エラー除外設定</name>
    <necessary>1</necessary>
    <type>HALFWIDTH_NUMBER</type>
    <length></length>
    <length_max></length_max>
    <length_min></length_min>
  </Column>
  <Column name="except_list">
    <name>排除リスト</name>
    <necessary></necessary>
    <type></type>
    <length></length>
    <length_max></length_max>
    <length_min></length_min>
    <prohibit>&lt;&gt;</prohibit>
  </Column>
  <Column name="exec_timing">
    <name>実行タイミング</name>
    <necessary>1</necessary>
    <type></type>
    <length></length>
    <length_max></length_max>
    <length_min></length_min>
  </Column>
  <Column name="schedule_date">
    <name>スケジュール日付</name>
    <necessary></necessary>
    <type>HALFWIDTH_DATE</type>
    <length></length>
    <length_max></length_max>
    <length_min></length_min>
  </Column>
  <Column name="valid_flg">
    <name>有効フラグ</name>
    <necessary>1</necessary>
    <type></type>
    <length></length>
    <length_max></length_max>
    <length_min></length_min>
  </Column>
</MailDistSettingRegist>