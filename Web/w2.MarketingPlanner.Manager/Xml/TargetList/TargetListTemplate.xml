    <Name>1年以内に定期購入を解約したお客様を抽出</Name>
    <Category>掘り起こし施策</Category>
    <Conditions type="AND">
      <Condition>
        <DataKbn>UserInfo</DataKbn>
        <DataField>w2_User.fixed_purchase_member_flg</DataField>
        <DataType>select</DataType>
        <Value>0</Value>
        <EqualSign>EqualSelect</EqualSign>
        <OrderExist />
      </Condition>
    </Conditions>
    <Conditions type="AND">
      <Condition>
        <DataKbn>FixedPurchaseInfo</DataKbn>
        <DataField>w2_FixedPurchase.shipped_count</DataField>
        <DataType>number</DataType>
        <Value>0</Value>
        <EqualSign>BiggerThanNumber</EqualSign>
        <OrderExist />
      </Condition>
    </Conditions>
    <Conditions type="AND">
      <Condition>
        <DataKbn>FixedPurchaseInfo</DataKbn>
        <DataField>w2_FixedPurchase.last_order_date</DataField>
        <DataType>datetime</DataType>
        <Value>1</Value>
        <EqualSign>LessThanYearDate</EqualSign>
        <OrderExist />
      </Condition>
    </Conditions>
  </Template>
