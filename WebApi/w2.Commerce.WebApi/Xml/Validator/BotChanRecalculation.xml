<BotChanRecalculation>
  <Column name="cart_id">
    <name>カートID</name>
    <type>HALFWIDTH_NUMBER</type>
    <length_max>12</length_max>
  </Column>
  <Column name="user_id">
    <name>ユーザーID</name>
    <necessary></necessary>
    <type></type>
    <length></length>
  </Column>
  <Column name="order_division">
    <name>注文種類</name>
    <necessary>1</necessary>
    <type></type>
    <length></length>
    <length_max>1</length_max>
    <regexp ptn="1または2">[1-2]</regexp>
  </Column>
  <Column name="order_kbn">
    <name>注文区分</name>
    <necessary>1</necessary>
    <type></type>
    <length></length>
    <regexp ptn="PCまたはSP">^(PC|SP)$</regexp>
  </Column>
  <Column name="add_novelty_flag">
    <name>ノベルティ追加購入フラグ</name>
    <necessary></necessary>
    <type></type>
    <length></length>
    <length_max>1</length_max>
    <regexp ptn="0または1">[0-1]</regexp>
  </Column>
  <Column name="recommend_flag">
    <name>レコメンド実施フラグ</name>
    <necessary></necessary>
    <type>HALFWIDTH_NUMBER</type>
    <regexp ptn="0または1">^[0-1]$</regexp>
  </Column>
  <Column name="name1">
    <name>@@User.name1.name@@</name>
    <necessary>@@User.name1.necessary@@</necessary>
    <type>@@User.name1.type@@</type>
    <length_max>@@User.name1.length_max@@</length_max>
  </Column>
  <Column name="name2">
    <name>@@User.name2.name@@</name>
    <necessary>@@User.name2.necessary@@</necessary>
    <type>@@User.name2.type@@</type>
    <length_max>@@User.name2.length_max@@</length_max>
  </Column>
  <Column name="name_kana1">
    <name>@@User.name_kana1.name@@</name>
    <necessary>@@User.name_kana1.necessary@@</necessary>
    <type>@@User.name_kana1.type@@</type>
    <length_max>@@User.name_kana1.length_max@@</length_max>
  </Column>
  <Column name="name_kana2">
    <name>@@User.name_kana2.name@@</name>
    <necessary>@@User.name_kana2.necessary@@</necessary>
    <type>@@User.name_kana2.type@@</type>
    <length_max>@@User.name_kana2.length_max@@</length_max>
  </Column>
  <Column name="birth">
    <name>@@User.birth.name@@</name>
    <necessary>1</necessary>
    <regexp ptn="正しい日付">^[0-9]{4}(0[1-9]|1[0-2])(0[1-9]|[12][0-9]|3[01])$</regexp>
  </Column>
  <Column name="sex">
    <name>@@User.sex.name@@</name>
    <necessary>1</necessary>
    <regexp ptn="MALE、FEMALE、UNKNOWNのいずれか">^(MALE|FEMALE|UNKNOWN)$</regexp>
  </Column>
  <Column name="mail_addr">
    <name>@@User.mail_addr.name@@</name>
    <necessary>1</necessary>
    <type>@@User.mail_addr.type@@</type>
    <length_max>256</length_max>
  </Column>
  <Column name="zip">
    <name>@@User.zip.name@@</name>
    <necessary>1</necessary>
    <regexp ptn="正しい郵便番号">^[0-9]{3}-[0-9]{4}$</regexp>
  </Column>
  <Column name="addr1">
    <name>@@User.addr1.name@@</name>
    <necessary>1</necessary>
    <type>@@User.addr1.type@@</type>
    <length></length>
    <length_max>50</length_max>
    <length_min></length_min>
  </Column>
  <Column name="addr2">
    <name>@@User.addr2.name@@</name>
    <necessary>@@User.addr2.necessary@@</necessary>
    <type>@@User.addr2.type@@</type>
    <length></length>
    <length_max>@@User.addr2.length_max@@</length_max>
    <length_min></length_min>
  </Column>
  <Column name="addr3">
    <name>@@User.addr3.name@@</name>
    <necessary>@@User.addr3.necessary@@</necessary>
    <type>@@User.addr3.type@@</type>
    <length></length>
    <length_max>@@User.addr3.length_max@@</length_max>
    <length_min></length_min>
  </Column>
  <Column name="addr4">
    <name>@@User.addr4.name@@</name>
    <necessary>@@User.addr4.necessary@@</necessary>
    <type>@@User.addr4.type@@</type>
    <length></length>
    <length_max>@@User.addr4.length_max@@</length_max>
    <length_min></length_min>
  </Column>
  <Column name="tel1">
    <name>@@User.tel1.name@@</name>
    <necessary>1</necessary>
    <regexp ptn="正しい電話番号">^[0-9]{1,6}-[0-9]{1,4}-[0-9]{1,4}$</regexp>
  </Column>
  <Column name="shipping_method">
    <name>配送方法</name>
    <necessary></necessary>
    <regexp ptn="EXPRESSまたはMAIL">^(EXPRESS|MAIL)$</regexp>
  </Column>
  <Column name="name">
    <name>配送先名</name>
    <necessary></necessary>
  </Column>
  <Column name="shipping_name">
    <name>@@User.name.name@@</name>
    <necessary>1</necessary>
    <type>@@User.name.type@@</type>
    <length></length>
    <length_max>@@User.name.length_max@@</length_max>
    <length_min></length_min>
  </Column>
  <Column name="shipping_name1">
    <name>@@User.name1.name@@</name>
    <necessary></necessary>
    <type>@@User.name1.type@@</type>
    <length></length>
    <length_max>@@User.name1.length_max@@</length_max>
    <length_min></length_min>
  </Column>
  <Column name="shipping_name2">
    <name>@@User.name2.name@@</name>
    <necessary></necessary>
    <type>@@User.name2.type@@</type>
    <length></length>
    <length_max>@@User.name2.length_max@@</length_max>
    <length_min></length_min>
  </Column>
  <Column name="shipping_name_kana">
    <name>@@User.name_kana.name@@</name>
    <necessary>1</necessary>
    <type>@@User.name_kana.type@@</type>
    <length></length>
    <length_max>@@User.name_kana.length_max@@</length_max>
    <length_min></length_min>
  </Column>
  <Column name="shipping_name_kana1">
    <name>@@User.name_kana1.name@@</name>
    <necessary></necessary>
    <type>@@User.name_kana1.type@@</type>
    <length></length>
    <length_max>@@User.name_kana1.length_max@@</length_max>
    <length_min></length_min>
  </Column>
  <Column name="shipping_name_kana2">
    <name>@@User.name_kana2.name@@</name>
    <necessary></necessary>
    <type>@@User.name_kana2.type@@</type>
    <length></length>
    <length_max>@@User.name_kana2.length_max@@</length_max>
    <length_min></length_min>
  </Column>
  <Column name="shipping_zip">
    <name>@@User.zip.name@@</name>
    <necessary>1</necessary>
    <regexp ptn="正しい郵便番号">^[0-9]{3}-[0-9]{4}$</regexp>
  </Column>
  <Column name="shipping_addr1">
    <name>@@User.addr1.name@@</name>
    <necessary>1</necessary>
    <type>@@User.addr1.type@@</type>
    <length></length>
    <length_max>50</length_max>
    <length_min></length_min>
  </Column>
  <Column name="shipping_addr2">
    <name>@@User.addr2.name@@</name>
    <necessary>@@User.addr2.necessary@@</necessary>
    <type>@@User.addr2.type@@</type>
    <length></length>
    <length_max>@@User.addr2.length_max@@</length_max>
    <length_min></length_min>
  </Column>
  <Column name="shipping_addr3">
    <name>@@User.addr3.name@@</name>
    <necessary>@@User.addr3.necessary@@</necessary>
    <type>@@User.addr3.type@@</type>
    <length></length>
    <length_max>@@User.addr3.length_max@@</length_max>
    <length_min></length_min>
  </Column>
  <Column name="shipping_addr4">
    <name>@@User.addr4.name@@</name>
    <necessary>@@User.addr4.necessary@@</necessary>
    <type>@@User.addr4.type@@</type>
    <length></length>
    <length_max>@@User.addr4.length_max@@</length_max>
    <length_min></length_min>
  </Column>
  <Column name="shipping_tel1">
    <name>@@User.tel1.name@@</name>
    <necessary>1</necessary>
    <regexp ptn="正しい電話番号">^[0-9]{1,6}-[0-9]{1,4}-[0-9]{1,4}$</regexp>
  </Column>
  <Column name="shipping_company_name">
    <name>@@User.company_name.name@@</name>
    <necessary>@@User.company_name.necessary@@</necessary>
    <type>@@User.company_name.type@@</type>
    <length></length>
    <length_max>@@User.company_name.length_max@@</length_max>
    <length_min></length_min>
  </Column>
  <Column name="shipping_company_post_name">
    <name>@@User.company_post_name.name@@</name>
    <necessary>@@User.company_post_name.necessary@@</necessary>
    <type>@@User.company_post_name.type@@</type>
    <length></length>
    <length_max>@@User.company_post_name.length_max@@</length_max>
    <length_min></length_min>
  </Column>
  <Column name="fixed_purchase_kbn">
    <name>定期購入区分</name>
    <necessary>1</necessary>
    <type>HALFWIDTH_NUMBER</type>
    <regexp ptn="01～03">^0[1-3]$</regexp>
  </Column>
  <Column name="course_buy_setting">
    <name>定期購入設定</name>
    <necessary>1</necessary>
    <type></type>
    <regexp ptn="月,日もしくは月,週,曜日もしくは日">^(([1-9]|1[0-2]),([1-9]|1[0-9]|2[0-8]|-1)|([1-9]|1[0-2]),[1-4],[0-6]|([1-9]|[1-9][0-9]))$</regexp>
  </Column>
  <Column name="payment_id">
    <name>お支払い方法</name>
    <necessary>1</necessary>
    <type></type>
    <length></length>
    <length_max></length_max>
    <length_min></length_min>
    <regexp ptn="クレジット、代金引換、コンビニ（後払い）、NP後払い、銀行振込（前払い）、銀行振込（後払い）のいずれか">K(10|20|32|36|41|42)</regexp>
  </Column>
  <Column name="credit_card_name">
    <name>クレジットカード登録名</name>
    <necessary>1</necessary>
    <type></type>
    <length></length>
    <length_max>100</length_max>
    <length_min></length_min>
  </Column>
  <Column name="credit_token">
    <name>クレジットカードトークン情報</name>
    <necessary>1</necessary>
  </Column>
  <Column name="credit_card_no">
    <name>カード番号</name>
    <necessary>1</necessary>
    <type>HALFWIDTH_NUMBER</type>
    <length_max>16</length_max>
  </Column>
  <Column name="expiration_month">
    <name>有効期限(月)</name>
    <necessary>1</necessary>
    <type>HALFWIDTH_NUMBER</type>
    <length></length>
    <length_max>2</length_max>
    <length_min></length_min>
  </Column>
  <Column name="expiration_year">
    <name>有効期限(年)</name>
    <necessary>1</necessary>
    <type>HALFWIDTH_NUMBER</type>
    <length></length>
    <length_max>2</length_max>
    <length_min></length_min>
  </Column>
  <Column name="author_name">
    <name>カード名義人</name>
    <necessary>1</necessary>
    <type></type>
    <length></length>
    <length_max>50</length_max>
    <length_min></length_min>
    <regexp ptn="半角英字">^[a-zA-Z ]+$</regexp>
  </Column>
  <Column name="credit_security_code">
    <name>セキュリティコード</name>
    <necessary>1</necessary>
    <type>HALFWIDTH_NUMBER</type>
    <length></length>
    <length_max>4</length_max>
    <length_min>3</length_min>
  </Column>
  <Column name="credit_installments">
    <name>支払い回数</name>
    <necessary>1</necessary>
    <type>HALFWIDTH_NUMBER</type>
  </Column>
  <Column name="credit_regist_flag">
    <name>クレジットカード登録フラグ</name>
    <necessary></necessary>
    <type></type>
    <length></length>
    <length_max>1</length_max>
    <length_min></length_min>
    <regexp ptn="0 or 1">[01]</regexp>
  </Column>
  <Column name="order_point_use">
    <name>利用ポイント数</name>
    <necessary></necessary>
    <type>HALFWIDTH_NUMBER</type>
  </Column>
  <Column name="coupon_code">
    <name>クーポンコード</name>
    <necessary></necessary>
    <type></type>
    <length></length>
    <length_max>30</length_max>
    <length_min></length_min>
  </Column>
  <Column name="product_id">
    <name>商品ID</name>
    <necessary>1</necessary>
    <type></type>
    <length></length>
    <length_max>30</length_max>
    <length_min></length_min>
    <regexp ptn="半角英数字、「_」、「-」、「(」、「)」、「#」">^[a-zA-Z0-9_\-\(\)#]+$</regexp>
  </Column>
  <Column name="variation_id">
    <name>商品バリエーションID</name>
    <necessary>1</necessary>
    <type></type>
    <length></length>
    <length_max>60</length_max>
    <length_min></length_min>
    <regexp ptn="半角英数字、「_」、「-」、「(」、「)」、「#」">^[a-zA-Z0-9_\-\(\)#]+$</regexp>
  </Column>
  <Column name="product_count">
    <name>注文数</name>
    <necessary>1</necessary>
    <type>HALFWIDTH_NUMBER</type>
    <length_max>3</length_max>
    <length_min>1</length_min>
    <number_min>1</number_min>
  </Column>
</BotChanRecalculation>