<CsExternalLink_Sub>
  
  <!-- 検索WHERE -->
  <Search_Where_Link_Title>
    <Statement>
      <![CDATA[
         WHERE  w2_CsExternalLink.dept_id = @dept_id
                <@@hasval:link_title@@>
                AND
                  (w2_CsExternalLink.link_title like '%' + @link_title + '%' ESCAPE '#')
                </@@hasval:link_title@@>
                <@@hasval:valid_flg@@>
                AND
                  (w2_CsExternalLink.valid_flg = @valid_flg)
                </@@hasval:valid_flg@@>
      ]]>
    </Statement>
  </Search_Where_Link_Title>
</CsExternalLink_Sub>