<CountryIpv4>

  <!-- IP(数値)より取得 -->
  <GetByIpNumeric>
    <Statement>
      <![CDATA[
        SET TRANSACTION ISOLATION LEVEL READ UNCOMMITTED
        
        SELECT  *
          FROM  w2_CountryIpv4
         WHERE  @ip_numeric
       BETWEEN  ip_numeric
           AND  ip_broadcast_numeric
      ]]>
    </Statement>
    <Parameter>
      <Input Name="ip_numeric" Type="int" />
    </Parameter>
  </GetByIpNumeric>

</CountryIpv4>
