<OrderExtendStatusSetting>

  <!-- 注文拡張ステータス設定取得 -->
  <GetOrderExtendStatusSettingList>
    <Statement>
      <![CDATA[
        -- 利用可能枝番までを取得。
        SELECT  *
          FROM  w2_OrderExtendStatusSetting
         WHERE  w2_OrderExtendStatusSetting.extend_status_no <= @extend_status_max
      ]]>
    </Statement>
    <Parameter>
      <Input Name="extend_status_max" Type="int" />
    </Parameter>
  </GetOrderExtendStatusSettingList>

</OrderExtendStatusSetting>