<GlobalZipcode>

<!-- Get -->
  <Get>
    <Statement>
      <![CDATA[
        SELECT  TOP 1
                w2_GlobalZipcode.*
          FROM  w2_GlobalZipcode
         WHERE  w2_GlobalZipcode.country_iso_code = @country_iso_code
           AND  w2_GlobalZipcode.zipcode = @zipcode
      ]]>
    </Statement>
    <Parameter>
      <Input Name="country_iso_code" Type="nvarchar" Size="3" />
      <Input Name="zipcode" Type="nvarchar" Size="20" />
    </Parameter>
  </Get>

</GlobalZipcode>
