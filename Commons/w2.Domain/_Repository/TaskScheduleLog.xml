<TaskScheduleLog>

  <!-- 取得 -->
  <GetLogEachMessagingAppKbn>
    <Statement>
      <![CDATA[
        SELECT  *
          FROM  w2_TaskScheduleLog
         WHERE  dept_id = @dept_id
           AND  action_kbn = @action_kbn
           AND  action_master_id = @action_master_id
           AND  action_no = @action_no
           AND  messaging_app_kbn = @messaging_app_kbn
      ]]>
    </Statement>
    <Parameter>
      <Input Name="dept_id" Type="nvarchar" Size="30" />
      <Input Name="action_kbn" Type="nvarchar" Size="30" />
      <Input Name="action_master_id" Type="nvarchar" Size="30" />
      <Input Name="action_no" Type="int" />
      <Input Name="messaging_app_kbn" Type="nvarchar" Size="20" />
    </Parameter>
  </GetLogEachMessagingAppKbn>

  <!-- 登録 -->
  <InsertLog>
    <Statement>
      <![CDATA[
        INSERT  w2_TaskScheduleLog
                (
                  dept_id
                  ,action_kbn
                  ,action_master_id
                  ,action_no
                  ,messaging_app_kbn
                  ,result
                )
        VALUES  (
                  @dept_id
                  ,@action_kbn
                  ,@action_master_id
                  ,@action_no
                  ,@messaging_app_kbn
                  ,@result
                )
      ]]>
    </Statement>
    <Parameter>
      <Input Name="dept_id" Type="nvarchar" Size="30" />
      <Input Name="action_kbn" Type="nvarchar" Size="30" />
      <Input Name="action_master_id" Type="nvarchar" Size="30" />
      <Input Name="action_no" Type="int" />
      <Input Name="messaging_app_kbn" Type="nvarchar" Size="20" />
      <Input Name="result" Type="nvarchar" Size="1000" />
    </Parameter>
  </InsertLog>
</TaskScheduleLog>
