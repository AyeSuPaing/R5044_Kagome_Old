<UserMemberRankHistory>

  <GetByUserId>
    <Statement>
      <![CDATA[
        SELECT  TOP 1 w2_UserMemberRankHistory.*
          FROM  w2_UserMemberRankHistory
         WHERE  w2_UserMemberRankHistory.user_id = @user_id
         ORDER BY w2_UserMemberRankHistory.date_created DESC
      ]]>
    </Statement>
    <Parameter>
      <Input Name="user_id" Type="nvarchar" Size="30" />
    </Parameter>
  </GetByUserId>

</UserMemberRankHistory>