<AmazonMwsMapping>

  <!-- デフォルト変換設定 Defaultは削除不可 ProjectAmazonMwsMapping.xmlに設定が存在する場合はProjectAmazonMwsMapping.xmlが優先されます。-->
  <Default>
    <!-- 支払区分 -->
    <PaymentMethod>
      <Payment>
        <Bef>COD</Bef>
        <Aft>M70</Aft>
      </Payment>
      <Payment>
        <Bef>CVS</Bef>
        <Aft>M20</Aft>
      </Payment>
      <Payment>
        <Bef>Other</Bef>
        <Aft>M80</Aft>
      </Payment>
    </PaymentMethod>
    <!-- 配送希望時間帯 -->
    <ShippingTimeSetting>
      <ShippingTime>
        <Bef>08:00-12:00</Bef>
        <Aft>101</Aft>
      </ShippingTime>
      <ShippingTime>
        <Bef>12:00-14:00</Bef>
        <Aft>103</Aft>
      </ShippingTime>
      <ShippingTime>
        <Bef>14:00-18:00</Bef>
        <Aft>104</Aft>
      </ShippingTime>
      <ShippingTime>
        <Bef>18:00-22:00</Bef>
        <Aft>105</Aft>
      </ShippingTime>
    </ShippingTimeSetting>
  </Default>

</AmazonMwsMapping>