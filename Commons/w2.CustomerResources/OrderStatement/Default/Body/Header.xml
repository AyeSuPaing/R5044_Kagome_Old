    <Textbox Name="Textbox104">
      <CanGrow>true</CanGrow>
      <KeepTogether>true</KeepTogether>
      <Paragraphs>
        <Paragraph>
          <TextRuns>
            <TextRun>
              <Value xml:space="preserve">　</Value>
              <Style>
                <FontFamily>ＭＳ ゴシック</FontFamily>
                <FontSize>9pt</FontSize>
              </Style>
            </TextRun>
            <TextRun>
              <Label>now_page_number</Label>
              <Value>1</Value>
              <Style>
                <FontFamily>ＭＳ ゴシック</FontFamily>
                <FontSize>9pt</FontSize>
              </Style>
            </TextRun>
            <TextRun>
              <Value>/</Value>
              <Style>
                <FontFamily>ＭＳ ゴシック</FontFamily>
                <FontSize>9pt</FontSize>
              </Style>
            </TextRun>
            <TextRun>
              <Label>total_page_number</Label>
              <Value>=Int((
   CountRows("order_id_group")
   +
   (
     Code.m_iMaxRows
	 -
	 Code.m_iMaxFirstPageRows
   )
   +
   (
     Code.m_iMaxFirstPageRows
	 -
     Code.GetMinRowCountUntilFirstChangingPage()
   )
   -
   1
 )/Code.m_iMaxRows) + 1

' ページ数 = (データ行数+(最大行数-最初のページの最大行数)+合計行-1)/20 + 1</Value>
              <Style>
                <FontFamily>ＭＳ ゴシック</FontFamily>
                <FontSize>9pt</FontSize>
              </Style>
            </TextRun>
          </TextRuns>
          <Style>
            <TextAlign>Left</TextAlign>
          </Style>
        </Paragraph>
      </Paragraphs>
      <rd:DefaultName>Textbox101</rd:DefaultName>
      <Top>0.61008cm</Top>
      <Left>0.065cm</Left>
      <Height>0.52062cm</Height>
      <Width>9.59083cm</Width>
      <ZIndex>2</ZIndex>
      <Visibility>
        <Hidden>=((Int((
   RowNumber("order_id_group")
   +
   (
     Code.m_iMaxRows
	 -
	 Code.m_iMaxFirstPageRows
   )
   +
   (
     Code.m_iMaxFirstPageRows
	 -
     Code.GetMinRowCountUntilFirstChangingPage()
   )
   -
   1
 )/Code.m_iMaxRows)) = 0)</Hidden>
      </Visibility>
      <Style>
        <Border>
          <Style>None</Style>
        </Border>
        <VerticalAlign>Top</VerticalAlign>
        <PaddingLeft>2pt</PaddingLeft>
        <PaddingRight>2pt</PaddingRight>
        <PaddingTop>2pt</PaddingTop>
        <PaddingBottom>2pt</PaddingBottom>
      </Style>
    </Textbox>
