<!--*******************************************************************************-->
<!-- 環境依存の値を置換タグとして提供します -->
<!--*******************************************************************************-->
<Environment>
  <!-- ===================== -->
  <!--  基本設定             -->
  <!-- ===================== -->
  <!-- サイトドメイン設定（本番:ドメイン名 Test/Trial:ttX.w2solution.com） -->
  <EnvTag tag="@@SITE_DOMAIN@@" value="localhost" />
  <!-- 案件ごとのプラン名（W2 Unified、W2 Repeat ONE、W2 Repeat STD、W2 Repeat PRO、W2 Repeat FOOD、EP、その他） -->
  <EnvTag tag="@@PLAN_NAME@@" value="その他" />
  <!-- プロジェクトNo（テスト/トライアル/本番で同じ設定） -->
  <EnvTag tag="@@PROJECT_NO@@" value="V5" />
  <!-- 環境名文字列（本番:空値 テスト:_Test トライアル:_Trial） ※プロジェクトNoの後ろにつける環境名 -->
  <EnvTag tag="@@ENVIRONMENT_NAME@@" value="" />
  <!-- SMTPサーバIP（開発:192.168.100.241(w2server01) 運用:SMTPサーバIP） -->
  <EnvTag tag="@@SMTP_SERVER_IP@@" value="192.168.100.241" />
  <!-- ===================== -->
  <!-- w2Database共通設定(_Plugins、_Project配下は対象外) -->
  <!-- ===================== -->
  <!-- w2Database設定 -->
  <EnvTag tag="@@CONNECTION_STRING@@" value="server=AYESUPAING-PC\SQLEXPRESS;database=R5044_Kagome;uid=sa;pwd=root" />
  <!-- 転送先w2Database設定 -->
  <EnvTag tag="@@CONNECTION_STRING_TRANSFER@@" value="server=w2DB1;database=R5044_Kagome.Develop_kkpw.Backup;uid=sa;pwd=w2Sa" />
  <!-- ============================ -->
  <!-- バッチ系実行結果の送信アドレス -->
  <!-- ============================ -->
  <!-- ※複数指定する場合はカンマ区切り。 この置換タグはFromに指定しないでください。 -->
  <!-- 顧客向けメール -->
  <EnvTag tag="@@CLIENT_MAIL_ADDRESS_LIST@@" value="" />
  <!-- 社内PJ向けメール（PJメールアドレス or モニタリングメールアドレスを必ず指定してください） -->
  <EnvTag tag="@@PROJECT_MAIL_ADDRESS_LIST@@" value="bh@w2s.xyz" />
  <!--EnvTag tag="@@PROJECT_MAIL_ADDRESS_LIST@@" value="monitoring@w2commerce-saas.net" /-->
  <!-- システム管理者向けメール（モニタリングメールアドレス） -->
  <EnvTag tag="@@ADMINISTRATOR_MAIL_ADDRESS_LIST@@" value="bh@w2s.xyz" />
  <!--EnvTag tag="@@ADMINISTRATOR_MAIL_ADDRESS_LIST@@" value="monitoring@w2commerce-saas.net" /-->
  <!-- ===================== -->
  <!--  ディレクトリ物理パス -->
  <!-- ===================== -->
  <EnvTag tag="@@DIR_PCSITE@@" value="C:\Inetpub\wwwroot\20240705_Kagome.Develop\Web\w2.Commerce.Front\" />
  <EnvTag tag="@@DIR_MOBILESITE@@" value="C:\Inetpub\wwwroot\20240705_Kagome.Develop\Web\w2.Commerce.MobileFront\" />
  <EnvTag tag="@@DIR_W2CMANAGERSITE@@" value="C:\Inetpub\wwwroot\20240705_Kagome.Develop\Web\w2.Commerce.Manager\" />
  <EnvTag tag="@@DIR_W2MPMANAGERSITE@@" value="C:\Inetpub\wwwroot\20240705_Kagome.Develop\Web\w2.MarketingPlanner.Manager\" />
  <EnvTag tag="@@DIR_W2CSMANAGERSITE@@" value="C:\Inetpub\wwwroot\20240705_Kagome.Develop\Web\w2.CustomerSupport.Manager\" />
  <EnvTag tag="@@DIR_W2CMSMANAGERSITE@@" value="C:\Inetpub\wwwroot\20240705_Kagome.Develop\Web\w2.Cms.Manager\" />
  <EnvTag tag="@@DIR_BATCHROOT@@" value="C:\Inetpub\wwwroot\20240705_Kagome.Develop\Batch\" />
  <EnvTag tag="@@DIR_BATCHROOT_DB@@" value="C:\Inetpub\wwwroot\20240705_Kagome.Develop\Batch\" />
  <!-- カスタマーリソース -->
  <EnvTag tag="@@DIR_CUSTOMERRESOURCES@@" value="C:\Inetpub\wwwroot\20240705_Kagome.Develop\Commons\w2.CustomerResources\" />
  <!-- カスタマーリソース(ネットワークパス)(ネットワーク越しにカスタマーリソースを参照する) -->
  <EnvTag tag="@@DIR_CUSTOMERRESOURCES_UNC@@" value="C:\Inetpub\wwwroot\20240705_Kagome.Develop\Commons\w2.CustomerResources\" />
  <!-- ログファイル出力先パス -->
  <EnvTag tag="@@DIR_APPLOG@@" value="C:\Logs\20240705_Kagome.Develop\" />
  <!-- ログファイルパス(DBサーバからWEBサーバへアクセスする際利用) -->
  <EnvTag tag="@@DIR_DBLOG@@" value="C:\Logs\20240705_Kagome.Develop\" />
  <!-- オペレータ操作ログ出力先ディレクトリ -->
  <EnvTag tag="@@DIR_OPERATIONLOG@@" value="w2.OperationLog\" />
  <!-- オペレータ操作ログ送信できない出力先ディレクトリ -->
  <EnvTag tag="@@DIR_OPERATIONLOGNOTSEND@@" value="w2.OperationLogNotSent\" />
  <!-- History Backup Path [@@DIR_DBLOG@@]配下に出力するDIRを指定  -->
  <EnvTag tag="@@DIR_OPERATIONUPDATEHISTORYLOG@@" value="w2.Commerce.Manager\" />
  <!-- ベリトランス連携設定ファイルディレクトリ -->
  <EnvTag tag="@@DIR_VERITRANS_CONFIG@@" value="C:\inetpub\wwwroot\20240705_Kagome.Develop\Commons\w2.CustomerResources\VeriTrans\" />

  <!-- 台湾FLAPS連携API実行ログ出力先ディレクトリ -->
  <EnvTag tag="@@DIR_FLAPS_API_LOG@@" value="ApiLog\" />
  <!-- ===================== -->
  <!--  サイトURLパス        -->
  <!-- ===================== -->
  <EnvTag tag="@@PATH_PCSITE@@" value="/20240705_Kagome.Develop/Web/w2.Commerce.Front/" />
  <EnvTag tag="@@PATH_MOBILESITE@@" value="/20240705_Kagome.Develop/Web/w2.Commerce.MobileFront/" />
  <EnvTag tag="@@PATH_W2CMANAGERSITE@@" value="/20240705_Kagome.Develop/Web/w2.Commerce.Manager/" />
  <EnvTag tag="@@PATH_W2MPMANAGERSITE@@" value="/20240705_Kagome.Develop/Web/w2.MarketingPlanner.Manager/" />
  <EnvTag tag="@@PATH_W2CSMANAGERSITE@@" value="/20240705_Kagome.Develop/Web/w2.CustomerSupport.Manager/" />
  <EnvTag tag="@@PATH_W2CMSMANAGERSITE@@" value="/20240705_Kagome.Develop/Web/w2.Cms.Manager/" />
  <!-- ===================== -->
  <!--  管理画面デザイン     -->
  <!-- ===================== -->
  <!--管理画面デザイン設定
        W2Unified の場合「」（空文字）
        W2Repeat の場合「Repeat」
        W2RepeatFood の場合「RepeatFood」
        その他 の場合「OEM」-->
  <EnvTag tag="@@MANAGER_DESIGN_SETTING@@" value="" />
  <!-- 管理画面デザイン 装飾アイコンファイル名（""：表記なし（本番など）、"icon_site_test.svg"：テストサイト表記） -->
  <EnvTag tag="@@MANAGER_DESIGN_DECORATE_ICON_FILENAME@@" value="" />
  <!-- 管理画面デザイン 装飾文字列（""：表記なし（本番など）、"テスト環境"：テストサイト表記） -->
  <EnvTag tag="@@MANAGER_DESIGN_DECORATE_MESSAGE@@" value="" />
  <!-- ===================== -->
  <!--  その他               -->
  <!-- ===================== -->
  <!-- 開発用：\bin\ リリース用：\ -->
  <EnvTag tag="@@BATCHBIN@@" value="\bin\" />
</Environment>
