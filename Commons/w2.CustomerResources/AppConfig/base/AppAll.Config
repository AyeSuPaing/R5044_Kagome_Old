    <!-- Lohaco連携：デフォルト配送種別 -->
    <Setting key="LiaiseLohacoMall_DefaultShippingId" value="101" />
    <!-- Lohaco連携：予約注文フラグの拡張ステタース番号 -->
    <Setting key="LiaiseLohacoMall_ReserveOrderExtendStatusNo" value="" />
  </C200_LiaiseLohacoMall>
  <!--===============================================================================-->
  <!-- 為替レート取得バッチ -->
  <!--===============================================================================-->
  <C200_GetExchangeRate>
    <!-- 為替レート取得API区分(CurrencyLayer、ExchangeRate、CurrencyConverter) -->
    <Setting key="GetExchangeRate_Kbn" value="CurrencyLayer" />
    <!-- 為替レート取得データ保存先 -->
    <Setting key="ExchangeRate_SaveDirectoryPath" value="@@DIR_BATCHROOT@@GetExchangeRate\ExchangeRateData" />
    <!-- 為替レート取得データ保存ファイル名 -->
    <Setting key="ExchangeRate_SaveFileName" value="exchange_rates_@@DATE_FORMAT@@.json" />
    <!-- 為替レート取得データ保存ファイル名の日付形式 -->
    <Setting key="ExchangeRate_SaveFileName_DateFormat" value="yyyyMMddHHmmss" />
  </C200_GetExchangeRate>
  <!--===============================================================================-->
  <!-- トークン削除バッチ -->
  <!--===============================================================================-->
  <C200_TokenDeleteBatch>
    <!-- トークン削除対象：注文ステータスが出荷完了（配送完了含む）後の何日以降を指定する -->
    <Setting key="Payment_Paidy_TokenDeleteLimitDay" value="60" />
  </C200_TokenDeleteBatch>
  <C200_ShippingReceivingStoreStatusImportBatch>
    <!-- Shipping Receiving Store Status Import Batch Temp Dir Path -->
    <Setting key="ShippingReceivingStoreStatusImportBatch_Temp_Dir_Path" value="@@DIR_BATCHROOT@@ShippingReceivingStoreStatusImportBatch\ShippingReceivingStoreStatusImportBatch\Temp" />
  </C200_ShippingReceivingStoreStatusImportBatch>
  <!--===============================================================================-->
  <!-- シルバーエッグアイジェント商品データ連携バッチ -->
  <!--===============================================================================-->
  <C200_SilvereggAigentCsvFileUploader>
    <!-- PCサイトパスルート -->
    <Setting key="Site_RootPath_PCFront" value="@@PATH_PCSITE@@" />
    <!-- WEBサイトHTTPSプロトコル -->
    <Setting key="Site_ProtocolHttps" value="https://" />
  </C200_SilvereggAigentCsvFileUploader>
  <!--===============================================================================-->
  <!-- Awoo商品連携バッチ -->
  <!--===============================================================================-->
  <C200_AwooProductSync>
    <!-- awoo連携ファイル出力パス -->
    <Setting key="Product_Master_File_Path" value="@@DIR_PCSITE@@awoo\data\productList.csv" />
    <Setting key="Site_ProtocolHttps" value="https://" />
  </C200_AwooProductSync>
</configuration>
