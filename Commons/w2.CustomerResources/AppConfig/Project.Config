    <Setting key="MultiPurpose_AffiliateOption_TaxExcludedFractionRounding" value="ROUNDDOWN" />
    <!-- %割引端数処理(切り上げ：ROUNDUP、切り捨て：ROUNDDOWN、四捨五入：ROUNDOFF) -->
    <Setting key="PercentOffFractionRounding" value="ROUNDDOWN" />
    <!-- 割引後商品価格按分時端数処理(切り上げ：ROUNDUP、切り捨て：ROUNDDOWN、四捨五入：ROUNDOFF) -->
    <Setting key="DiscountedPriceFractionRounding" value="ROUNDDOWN" />
