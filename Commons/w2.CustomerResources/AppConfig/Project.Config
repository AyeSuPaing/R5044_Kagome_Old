    <!-- 端数処理方法(切り上げ：ROUNDUP、切り捨て：ROUNDDOWN、四捨五入：ROUNDOFF) -->
    <!-- ===================== -->
    <!-- 税抜金額端数処理： -->
    <Setting key="TaxExcludedFractionRounding" value="ROUNDDOWN" />
    <!-- 税抜金額端数処理：ポイントオプション -->
    <Setting key="PointOption_TaxExcludedFractionRounding" value="ROUNDDOWN" />
