    <!-- 外部レコメンド：サイトコード -->
    <Setting key="Recommend_Reconize_site_Code" value="1000" />
    <!-- 外部レコメンド：レコメンドコードヘッダ -->
    <Setting key="Recommend_Reconize_Recommend_Code_Head" value="w2c1_s000_" />
