    <!-- シルバーエッグ：商品連携用FTPアカウント -->
    <Setting key="Recommend_Silveregg_Ftp_Id" value="w2solution" />
    <!-- シルバーエッグ：商品連携用FTPパスワード -->
    <Setting key="Recommend_Silveregg_Ftp_Pw" value="JidaYU732JKx" />
    <!-- シルバーエッグ：FTP送信先ファイルパス -->
    <Setting key="Recommend_Silveregg_Destination_File_Path" value="w2s0000" />
