    <!-- シルバーエッグ：レポートAPIアクセストークン -->
    <!--   テスト環境はこちら「b0d189f0d8fd40bdb8417839457ec09e」 -->
    <Setting key="Recommend_Silveregg_report_Api_Token" value="" />
    <!-- シルバーエッグ：商品連携用FTPホスト -->
