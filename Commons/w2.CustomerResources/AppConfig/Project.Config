    <!-- オペレータ：未対応滞留警告日数 -->
    <Setting key="WarningNoActionLimitDays" value="3" />
    <!-- 管理者：担当未設定滞留警告 -->
    <Setting key="WarningNoAssign" value="TRUE" />
    <!-- 管理者：担当未設定滞留警告日数 -->
    <Setting key="WarningNoAssignLimitDays" value="1" />
  </C200_CsWarningMailSender>
  <!--*******************************************************************************-->
  <!-- モール：メール注文取込 -->
  <!--*******************************************************************************-->
  <C300_MailOrderGetter>
    <!-- 決済：商品IDか外部連携ID1か -->
    <Setting key="MailOrderGetter_ProductKey" value="0" />
    <!-- 決済：切り上げタイプ（切り上げ：ROUNDUP、切り捨て：ROUNDDOWN、四捨五入：ROUNDOFF） -->
    <Setting key="MailOrderGetter_Tax_RoundType" value="ROUNDUP" />
    <!-- 決済：楽天：取り込み保留対象ステータス(必須 カンマ区切りで複数指定) -->
    <Setting key="MailOrderGetter_Rakuten_RetryImport_OrderProgress" value="100,200,800" />
    <!-- 決済：楽天：取り込み保留対象のリトライ期間(時間) -->
    <Setting key="MailOrderGetter_Rakuten_RetryImport_Period" value="24" />
    <!-- 決済：楽天：取り込み対象ステータス(必須 カンマ区切りで複数指定) ※取り込み保留が優先 -->
    <Setting key="MailOrderGetter_Rakuten_Import_OrderProgress" value="300,400,500,600,700" />
    <!-- 決済：楽天：取り込み対象キャンセルメールステータス(必須 カンマ区切りで複数指定) ※取り込み保留が優先 -->
    <Setting key="MailOrderGetter_Rakuten_Cancel_OrderProgress" value="900" />
    <!-- 決済：楽天：予約購入注文内容ご確認メール取込設定（予約注文が本注文に確定した時点で送信されるメールの取込設定） -->
    <Setting key="MailOrderGetter_Rakuten_ReservationOrderMail_Enabled" value="TRUE" />
    <!-- 決済：楽天：注文メール内の受注番号を含む行の正規表現 -->
    <Setting key="MailOrderGetter_Rakuten_Regular_Expression_For_Line" value="\[(受注|注文)番号\] .*" />
    <!-- 決済：楽天：注文メール内の受注番号の正規表現 -->
    <Setting key="MailOrderGetter_Rakuten_Regular_Expression_For_Order_No" value="[0-9]+-[0-9]+-[0-9]+" />
    <!-- モール：楽天：配送希望時間帯を指定しない場合の文言 カンマ区切り複数指定可能 -->
    <Setting key="MailOrderGetter_Rakuten_Api_Shipping_Term_None_Comment" value="" />
  </C300_MailOrderGetter>
