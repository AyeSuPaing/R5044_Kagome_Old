    <!-- 運用地：州(運用地：国がUSの場合に設定する) -->
    <Setting key="Operational_Base_Province" value="" />
    <!--  管理側の言語ロケールコード  -->
    <Setting key="Operational_Language_Locale_Code" value="ja-JP" />
