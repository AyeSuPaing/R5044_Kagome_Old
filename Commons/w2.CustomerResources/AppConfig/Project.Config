    <Setting key="Recommend_Silveregg_Merchant_Id" value="w2s0000" />
    <!-- シルバーエッグ：レコメンドAPI -->
    <!--   本番環境はこちら「w2s0000.silveregg.net」 -->
    <!--   テスト環境はこちら「w2s0000-test.silveregg.net」 -->
    <Setting key="Recommend_Silveregg_Api_Domain" value="w2s0000.silveregg.net" />
