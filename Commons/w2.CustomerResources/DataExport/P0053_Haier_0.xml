<ExportData>
  <!-- Export SQL -->
  <export>
    <statement>
      <![CDATA[
            -- Export
            SELECT  '' AS send_id,  -- 送信ID
                    '1' AS shipping_instruction,  -- 出荷指示処理区分
                    '' AS shipper_code, -- 荷主コード
                    '10' AS shipping_classification,  -- 出荷種別区分
                    '' AS delivery_code,  -- 運送会社コード
                    '' AS invoice_customer_code,  -- 運賃請求先顧客コード
                    '' AS invoice_customer_branch_code, -- 運賃請求先顧客コード枝番
                    '' AS tracking_number,  -- 運賃管理番号
                    '' AS warehouse_code, -- 倉庫コード
                    w2_OrderShipping.shipping_date, -- 配送指定日
                    '' AS delivery_weekofdate,  -- 配送指定曜日区分
                    w2_OrderShipping.shipping_time, -- 配送指定時間帯区分
                    '' AS check_kbn,  -- 伝票区分
                    '' AS amount_of_money,  -- 金額
                    '' AS cooperation_consumption_tax,  -- B2連携用消費税額
                    '' AS payment_classification, -- 支払方法区分
                    '' AS division_number,  -- 分割回数
                    '' AS split_amount, -- 分割金額
                    '' AS adjust_amount,   -- 調整金額
                    '' AS amount_item1, -- 金額項目1
                    '' AS amount_item2, -- 金額項目2
                    '' AS amount_item3, -- 金額項目3
                    '' AS amount_item4, -- 金額項目4
                    '' AS amount_item5, -- 金額項目5
                    '' AS initial_payment_deadline, -- 初回支払期限
                    '' AS postage,  -- 送料
                    '' AS num_field_1,  -- 数値項目1
                    '' AS num_field_2,  -- 数値項目2
                    '' AS num_field_3,  -- 数値項目3
                    '' AS num_field_4,  -- 数値項目4
                    '' AS num_field_5,  -- 数値項目5
                    '' AS customer_code,  -- 取引先コード
                    '' AS shop_name,  -- 取引先名称
                    '' AS customer_part_signature,  -- 取引先部署名
                    '' AS contact_person, -- 取引先担当者
                    '' AS shop_zip_code,  -- 取引先郵便番号
                    '' AS shop_address, -- 取引先住所
                    '' AS shop_tel, -- 取引先電話番号
                    '' AS shop_fax, -- 取引先ＦＡＸ番号
                    '' AS destination_code, -- 届先コード
                    w2_OrderShipping.shipping_company_name + w2_OrderShipping.shipping_name AS shipping_name, -- 届先名称
                    '' AS address_signature,  -- 届先部署名
                    '' AS address_authority_person, -- 届先担当者
                    REPLACE(w2_OrderShipping.shipping_zip, '-', '') AS shipping_zip,  -- 届先郵便番号
                    w2_OrderShipping.shipping_addr1 + w2_OrderShipping.shipping_addr2 + w2_OrderShipping.shipping_addr3 + w2_OrderShipping.shipping_addr4 AS shipping_addr, -- 届先住所
                    w2_OrderShipping.shipping_tel1, -- 届先電話番号
                    '' AS shipping_fax, -- 届先ＦＡＸ番号
                    CASE
                      WHEN DATEADD(DAY, -7, ISNULL(w2_OrderShipping.shipping_date, GETDATE())) < GETDATE() THEN GETDATE()
                      ELSE DATEADD(DAY, -7, ISNULL(w2_OrderShipping.shipping_date, GETDATE()))
                    END AS export_shipping_time,  -- 出荷予定日
                    GETDATE() AS export_shipping_date,  -- 出荷指示日
                    w2_Order.order_id AS purchase_order_no, -- 注文書番号
                    w2_Order.order_date,  -- 注文書日付
                    '' AS order_no, -- 注文書注釈
                    '' AS original_department_code, -- 発生元部署コード
                    '' AS original_part_name, -- 発生元部署名
                    w2_Order.order_id AS original_order_no, -- 発生元番号
                    GETDATE() AS export_order_date, -- 発生日付
                    w2_Order.order_id AS remark,  -- 備考
                    '' AS header_area1, -- ヘッダーフリーエリア１
                    '' AS header_area2, -- ヘッダーフリーエリア２
                    '' AS header_area3, -- ヘッダーフリーエリア３
                    '' AS header_area4, -- ヘッダーフリーエリア４
                    '' AS header_area5, -- ヘッダーフリーエリア５
                    '' AS invoice_input_item1,  -- 納品書出力項目１
                    '' AS invoice_input_item2,  -- 納品書出力項目２
                    '' AS invoice_input_item3,  -- 納品書出力項目３
                    '' AS shipping_shedule_email_addr,  -- 出荷予定eメールアドレス
                    '' AS input_model_type, -- 入力機種種別
                    '' AS shipping_shedule_email_message, -- 出荷予定eメールメッセージ
                    '' AS convenience_store_division, -- コンビニ区分
                    '' AS company_name, -- 事業者名
                    '' AS business_postal_code, -- 事業者郵便番号
                    '' AS operator_addr_1,  -- 事業者住所１
                    '' AS operator_addr_2,  -- 事業者住所２
                    '' AS initial_message_1,  -- 初回請求メッセージ１
                    '' AS initial_message_2,  -- 初回請求メッセージ２
                    '' AS invoice_no, -- 請求No
                    '' AS ocr_1,  -- OCR情報１行目
                    '' AS ocr_2,  -- OCR情報２行目
                    '' AS bar_code_infor, -- バーコード情報
                    '' AS disp_barcode_1, -- 表示用バーコード1行目
                    '' AS disp_barcode_2, -- 表示用バーコード2行目
                    '' AS convenience_store_payment_date, -- コンビニ支払期限日
                    '' AS mail_account_office_name, -- 郵便口座所轄庁名
                    '' AS mail_account_office_code, -- 郵便口座所轄庁コード
                    '' AS postal_check_digit, -- 郵便チェックデジット
                    '' AS postal_account_no,  -- 郵便口座番号
                    '' AS approve_no, -- 私製承認番号
                    '' AS approve_district_name,  -- 私製承認地区名
                    '' AS noshi_address,  -- 熨斗表書き
                    '' AS noshi_package_division, -- 熨斗包装区分
                    '' AS wrapping_paper_kind,  -- 熨斗紙種類
                    '' AS new_name1,  -- 名入れ１
                    '' AS new_name2,  -- 名入れ２
                    '' AS new_name3,  -- 名入れ３
                    '1' AS screen_correctable_flg,  -- 画面訂正可能フラグ
                    '1' AS invoice_inspection_target_flg, -- 納品書検品対象フラグ
                    '1' AS shipping_specification_class,  -- 出荷指示明細処理区分
                    w2_OrderItem.order_item_no, -- 出荷指示明細番号
                    w2_OrderItem.variation_id,  -- 商品コード
                    '00' AS package_type, -- 荷姿区分
                    w2_OrderItem.cooperation_id3, -- 取引先商品コード
                    '' AS location_code,  -- ロケーションコード
                    '' AS status_code,  -- 状態コード
                    '' AS lot_number, -- ロット番号
                    '' AS serial_number,  -- シリアル番号
                    '' AS due_date, -- 賞味期限
                    '' AS stock_class_code, -- 在庫切分けコード
                    '' AS delivery_specific_code, -- 納品先指定コード
                    '' AS inventory_manage_1, -- 在庫管理項目１
                    '' AS inventory_manage_2, -- 在庫管理項目２
                    '' AS inventory_manage_3, -- 在庫管理項目３
                    '' AS inventory_manage_4, -- 在庫管理項目４
                    '' AS inventory_manage_5, -- 在庫管理項目５
                    '' AS inventory_manage_6, -- 在庫管理項目６
                    '' AS inventory_manage_7, -- 在庫管理項目７
                    '' AS inventory_date_1, -- 在庫管理日付１
                    '' AS inventory_date_2, -- 在庫管理日付２
                    '' AS inventory_date_3, -- 在庫管理日付３
                    w2_OrderItem.item_quantity, -- 出荷指示数量
                    w2_OrderItem.product_price, -- 本体金額(単価）
                    '' AS consumption_tax_amount, -- 消費税金額
                    w2_OrderItem.order_item_no, -- 備考
                    w2_OrderItem.product_option_texts,  -- 明細フリーエリア１
                    w2_OrderItem.novelty_id,  -- 明細フリーエリア２
                    '' AS free_area_3,  -- 明細フリーエリア３
                    '' AS free_area_4,  -- 明細フリーエリア４
                    '' AS free_area_5,  -- 明細フリーエリア５
                    w2_Order.mall_id  -- Mall ID
              FROM  w2_Order
        INNER JOIN  ##order_id ON (w2_Order.order_id = ##order_id.order_id)
        INNER JOIN  w2_OrderOwner ON (w2_Order.order_id = w2_OrderOwner.order_id)
        INNER JOIN  w2_OrderShipping ON (w2_Order.order_id = w2_OrderShipping.order_id)
        INNER JOIN  w2_OrderItem ON (w2_Order.order_id = w2_OrderItem.order_id )
          ORDER BY  w2_Order.order_id
      ]]>
    </statement>
    <Parameter>
    </Parameter>
  </export>
