<GlobalSettings>

  <!--
  =========================================================================================================
  グローバル対応 対応状況設定
  =========================================================================================================
  -->
  <!-- 対応国ISOコード -->
  <!-- 対応させる国ISOコードを追加する場合は<Code></Code>を追加してください -->
  <Countries>
    <Code>JP</Code>
    <Code>US</Code>
    <Code>TW</Code>
    <Code>VN</Code>
  </Countries>

  <!-- 対応言語コード・通貨ロケールID -->
  <!-- 
    対応させる対応言語コード・通貨ロケールIDを追加する場合は<Language/>を追加してください 
    Code → 対応言語コード
    LocaleId →　対応言語コードに紐づく対応言語ロケールID ※日付などの表示フォーマットに利用されます。
    必ず、言語コードと言語ロケールIDは1対1で設定が可能となります。
    DefaultUrl → 対応言語コードに紐づくトップページのURL。
    ※自動翻訳サービスを利用する時、「DefaultUrl=""」と設定してください。
    ※複数言語サイトがある場合、「DefaultUrl="＜該当のフロントサイトのURL（lc、ccのパラメータを付く）＞"」
      のように設定してください。
      lc→言語コード
      cc→通貨コード
    ■運用時の例）
        <Languages>
          <Language Code="ja" LocaleId="ja-JP" DefaultUrl="(日本語サイトURL)?lc=ja&amp;cc=JPY"/>
          <Language Code="en" LocaleId="en-US" DefaultUrl="(英語サイトURL)?lc=en&amp;cc=USD"/>
        </Languages>

      ●１サーバーに複数言語サイトのURL例（クッキーが共用できないようにそれぞれディレクトリを作成したい）
        日本語サイト：http://example.jp/jp/
        英語サイト  ：http://example.jp/us/

      ●別ドメインのURL例
        日本語サイト：http://jp.example.jp/
        英語サイト  ：http://us.example.jp/
  -->
  <Languages>
    <Language Code="ja" LocaleId="ja-JP" DefaultUrl=""/>
    <Language Code="en" LocaleId="en-US" DefaultUrl=""/>
    <Language Code="fr" LocaleId="fr-FR" DefaultUrl=""/>
    <Language Code="zh" LocaleId="zh-TW" DefaultUrl=""/>
    <Language Code="vi" LocaleId="vi-VN" DefaultUrl=""/>
  </Languages>

  <!-- 対応通貨コード・通貨ロケールID -->
  <!-- 
    対応させる対応通貨コード・通貨ロケールIDを追加する場合は<Currency></Currency>を追加してください 
    Code → 対応通貨コード
    LocaleId →　対応通貨コードに紐づく通貨ロケールID ※通貨記号の表示に利用されます。
    LocaleFormat → 通貨フォーマット
    通貨コードと通貨ロケールID,通貨フォーマットは1対多で設定可能となります。
    通貨コードに紐づく通貨ロケールID,通貨フォーマットを追加する場合は<CurrencyLocale></CurrencyLocale>を追加してください。
  -->
  <Currencies>
    <Currency Code="JPY">
      <CurrencyLocales>
        <CurrencyLocale LocaleId="ja-JP" LocaleFormat="{0:#,0}円" LocaleFormatJapanese="{0:C}" LocaleFormatWithoutSymbol="{0:#,0}"/>
      </CurrencyLocales>
    </Currency>
    <Currency Code="USD">
      <CurrencyLocales>
        <CurrencyLocale LocaleId="en-US" LocaleFormat="{0:C}" LocaleFormatWithoutSymbol="{0:#.##,0}"/>
      </CurrencyLocales>
    </Currency>
    <Currency Code="EUR">
      <CurrencyLocales>
        <CurrencyLocale LocaleId="fr-FR" LocaleFormat="{0:C}" LocaleFormatWithoutSymbol="{0:#.##,0}"/>
      </CurrencyLocales>
    </Currency>
    <Currency Code="TWD">
      <CurrencyLocales>
        <CurrencyLocale LocaleId="zh-TW" LocaleFormat="{0:C0}" LocaleFormatWithoutSymbol="{0:#,0}"/>
      </CurrencyLocales>
    </Currency>
    <Currency Code="VND">
      <CurrencyLocales>
        <CurrencyLocale LocaleId="vi-VN" LocaleFormat="{0:#,0} VND" LocaleFormatWithoutSymbol="{0:0}"/>
      </CurrencyLocales>
    </Currency>
  </Currencies>

  <!-- 基軸通貨 -->
  <!-- 
  金額計算の基準となる通貨設定となります。
  Code → 通貨コード
  LocaleId → 通貨ロケールID
  LocaleFormat → 通貨フォーマット
  PdfFormat → 納品書の通貨フォーマット
  CurrencyDecimalDigits → 補助単位 小数点以下の有効桁数 ※設定がない場合は通貨コード,通貨ロケールIDより判断します
  -->
  <!-- KeyCurrency Code="EUR" LocaleId="fr-FR" LocaleFormat="{0:C}" PdfFormat="# 0,00'€'" UnitString="€"/-->
  <!--<KeyCurrency Code="USD" LocaleId="en-US" LocaleFormat="{0:C}" PdfFormat="'$'#,0.00" UnitString="$"/>-->
  <KeyCurrency Code="JPY" LocaleId="ja-JP" LocaleFormat="{0:C}" PdfFormat="'\'#,0" UnitString="¥" />
  <!--<KeyCurrency Code="TWD" LocaleId="zh-TW" LocaleFormat="{0:C0}" PdfFormat="'NT$'#,0" CurrencyDecimalDigits="0" UnitString="NT$" />-->
  <!--<KeyCurrency Code="VND" LocaleId="vi-VN" LocaleFormat="{0:#,0} VND" PdfFormat="'\'#,0" CurrencyDecimalDigits="0" UnitString="đ" />-->
  <!-- 初回アクセス時の初期設定(Frontで利用) -->
  <!-- 
    初回アクセス時に設定されるリージョンデータとなります。
    例)
      IPアドレスが日本国内の場合はCountryIsoCode → [JP] のものが決定されます。
    
    初回アクセス時のIPアドレスが下記範囲外の場合は<NotFoundCountryIsoCode>に設定された国ISOコードが適用されます。
    
    CountryIsoCode → 国ISOコード
    LanguageCode → 言語コード
    LanguageLocaleId → 言語ロケールID
    CurrencyCode → 通貨コード
    CurrencyLocaleId → 通貨ロケールID
    
    初期設定を追加する場合は<Region/>を追加してください。
  -->
  <Regions>
    <Region CountryIsoCode="JP" LanguageCode="ja" LanguageLocaleId="ja-JP" CurrencyCode="JPY" CurrencyLocaleId="ja-JP"/>
    <Region CountryIsoCode="US" LanguageCode="en" LanguageLocaleId="en-US" CurrencyCode="USD" CurrencyLocaleId="en-US"/>
    <Region CountryIsoCode="TW" LanguageCode="zh" LanguageLocaleId="zh-TW" CurrencyCode="TWD" CurrencyLocaleId="zh-TW"/>
    <Region CountryIsoCode="VN" LanguageCode="vi" LanguageLocaleId="vi-VN" CurrencyCode="VND" CurrencyLocaleId="vi-VN"/>
  </Regions>
  <!-- IP範囲対応外のデフォルト国ISOコード -->
  <NotFoundCountryIsoCode>US</NotFoundCountryIsoCode>

  <!-- 日付切替対応 -->
  <!--
    対応日付形式を追加する場合は、<DateTimeFormat></DateTimeFormat>を追加してください。
    FormatId → 形式ID （日付を変換する時にこのIDを基にして、該当な形式を取得するためです）
    
    対応言語ロケールIDを追加する場合は、<Format></Format>を追加してください。
    LanguageLocaleId → 対応日付形式に紐づく言語ロケールID
    グローバルOPがOFFの場合でも、こちらの形式が利用されているので、
    必ず言語ロケールIDが空であるレコードを設定してください（<Format LanguageLocaleId="">〇〇〇</Format>）
    
    「FormatId、LanguageLocaleId」を基にして、日付形式を取得する時は、
    該当言語ロケールIDのレコードがない場合、該当フォーマットIDの
    1番目レコードの日付形式を取得される。
  -->
  <DateTimeFormats>
    <DateTimeFormat FormatId="f0_general">
      <Format LanguageLocaleId="">ymd</Format>
      <Format LanguageLocaleId="ja-JP">ymd</Format>
      <Format LanguageLocaleId="en-US">mdy</Format>
      <Format LanguageLocaleId="vi-VN">dmy</Format>
    </DateTimeFormat>
    <DateTimeFormat FormatId="f1_short_yyyyMMdd">
      <Format LanguageLocaleId="">yyyy/MM/dd</Format>
      <Format LanguageLocaleId="ja-JP">yyyy/MM/dd (JST)</Format>
      <Format LanguageLocaleId="en-US">MM/dd/yyyy (JST)</Format>
      <Format LanguageLocaleId="vi-VN">dd/MM/yyyy (JST)</Format>
    </DateTimeFormat>
    <DateTimeFormat FormatId="f2_short_yyyyMMdd_HHmm">
      <Format LanguageLocaleId="">yyyy/MM/dd HH:mm</Format>
      <Format LanguageLocaleId="ja-JP">yyyy/MM/dd HH:mm (JST)</Format>
      <Format LanguageLocaleId="en-US">MM/dd/yyyy HH:mm (JST)</Format>
      <Format LanguageLocaleId="vi-VN">dd/MM/yyyy HH:mm (JST)</Format>
    </DateTimeFormat>
    <DateTimeFormat FormatId="f3_short_yyyyMMdd_HHmmss">
      <Format LanguageLocaleId="">yyyy/MM/dd HH:mm:ss</Format>
      <Format LanguageLocaleId="ja-JP">yyyy/MM/dd HH:mm:ss (JST)</Format>
      <Format LanguageLocaleId="en-US">MM/dd/yyyy HH:mm:ss (JST)</Format>
      <Format LanguageLocaleId="vi-VN">dd/MM/yyyy HH:mm:ss (JST)</Format>
    </DateTimeFormat>
    <DateTimeFormat FormatId="f4_short_yyyyMMdd(d)">
      <Format LanguageLocaleId="">yyyy/MM/dd(ddd)</Format>
      <Format LanguageLocaleId="ja-JP">yyyy/MM/dd(ddd) (JST)</Format>
      <Format LanguageLocaleId="en-US">MM/dd/yyyy (ddd) (JST)</Format>
      <Format LanguageLocaleId="vi-VN">dd/MM/yyyy (ddd) (JST)</Format>
    </DateTimeFormat>
    <DateTimeFormat FormatId="f5_short_yyyyMd(d)">
      <Format LanguageLocaleId="">yyyy/M/d (ddd)</Format>
      <Format LanguageLocaleId="ja-JP">yyyy/M/d (ddd) (JST)</Format>
      <Format LanguageLocaleId="en-US">M/d/yyyy (ddd) (JST)</Format>
      <Format LanguageLocaleId="vi-VN">d/M/yyyy (ddd) (JST)</Format>
    </DateTimeFormat>
    <DateTimeFormat FormatId="f6_long_yyyyMd">
      <Format LanguageLocaleId="">yyyy年M月d日</Format>
      <Format LanguageLocaleId="ja-JP">yyyy年M月d日 (JST)</Format>
      <Format LanguageLocaleId="en-US">MMM d, yyyy (JST)</Format>
