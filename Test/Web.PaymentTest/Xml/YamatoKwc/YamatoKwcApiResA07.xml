<?xml version="1.0" encoding="utf-8" ?>
<Response>

  <!-- クレジット金額変更用 -->
  
  <success>
    <return>
      <returnCode>0</returnCode>
      <errorCode></errorCode>
      <returnDate>@@datetime@@</returnDate>
      <creditErrorCode></creditErrorCode>
    </return>
